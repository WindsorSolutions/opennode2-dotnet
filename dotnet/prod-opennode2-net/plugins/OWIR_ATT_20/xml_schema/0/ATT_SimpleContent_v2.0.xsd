        <xsd:annotation>
            <xsd:documentation>Code indicating the branch/district within the agency responsible for conducting the evaluation.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="EnforcementActionStatusDate" type="EnforcementActionStatusDateDataType">
        <xsd:annotation>
            <xsd:documentation>The calendar date of the most recent Enforcement Action Status determination.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="EnforcementActionStatusText" type="EnforcementActionStatusTextDataType">
        <xsd:annotation>
            <xsd:documentation>The current status of the enforcement action.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="EnforcementActionSupplementalInformationText" type="EnforcementActionSupplementalInformationTextDataType">
        <xsd:annotation>
            <xsd:documentation>Additional description of the evaluation.</xsd:documentation>
        </xsd:annotation>
