    <xsd:element name="GeometricTypeName" type="GeometricTypeNameDataType">
        <xsd:annotation>
            <xsd:documentation>The name that identifies the geometric entity represented by one point or a sequence of latitude and longitude points.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ICTVdBTaxonIdentifier" type="ICTVdBTaxonIdentifierDataType">
        <xsd:annotation>
            <xsd:documentation>The unique identification number assigned to a virus by the International Committee on Taxonomy of Viruses (ICTV).</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="IndividualFullName" type="IndividualFullNameDataType">
        <xsd:annotation>
            <xsd:documentation>The complete name of a person, potentially including first name, middle name or initial, and or surname.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="IndividualIdentifier" type="IndividualIdentifierDataType">
        <xsd:annotation>
            <xsd:documentation>A designator used to uniquely identify an individual within a context.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="IndividualTitleText" type="IndividualTitleTextDataType">
        <xsd:annotation>
            <xsd:documentation>The title held by a person in an organization.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="InjunctiveReliefIdentifier" type="InjunctiveReliefIdentifierDataType">
        <xsd:annotation>
            <xsd:documentation>A designator used to uniquely identify an injunctive relief within a context.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="InjunctiveReliefRequiredText" type="InjunctiveReliefRequiredTextDataType">
        <xsd:annotation>
            <xsd:documentation>Narrative description of the injunctive relief required in a Final Order.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="InjunctiveReliefSoughtText" type="InjunctiveReliefSoughtTextDataType">
        <xsd:annotation>
            <xsd:documentation>Narrative description of the injunctive relief sought through a Complaint/Proposed Order.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="InjunctiveReliefValue" type="InjunctiveReliefValueDataType">
        <xsd:annotation>
            <xsd:documentation>The estimated dollar cost to the Defendant/Respondent of carrying out all injunctive relief (not including penalties, cost recovery or Supplemental Environmental Projects) required pursuant to a Final Order.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ITISTaxonomicSerialNumber" type="ITISTaxonomicSerialNumberDataType">
        <xsd:annotation>
            <xsd:documentation>The unique identification number assigned to an Integrated Taxonomic Information System biological entity.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="LaboratoryIdentifier" type="LaboratoryIdentifierDataType">
        <xsd:annotation>
            <xsd:documentation>A designator used to uniquely identify a laboratory within a context.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="LaboratoryTypeText" type="LaboratoryTypeTextDataType">
        <xsd:annotation>
            <xsd:documentation>The classification of the laboratory.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="LastName" type="LastNameDataType">
        <xsd:annotation>
            <xsd:documentation>The surname of an individual.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="LatitudeMeasure" type="LatitudeMeasureDataType">
        <xsd:annotation>
            <xsd:documentation>The measure of the angular distance on a meridian north or south of the equator.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="LocalityName" type="LocalityNameDataType">
        <xsd:annotation>
            <xsd:documentation>The name of a city, town, village or other locality.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="LocationAddressText" type="LocationAddressTextDataType">
        <xsd:annotation>
            <xsd:documentation>The address that describes the physical (geographic) location of the front door or main entrance of a facility site, including urban-style street address or rural address.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="LocationCommentsText" type="LocationCommentsTextDataType">
        <xsd:annotation>
            <xsd:documentation>The text that provides additional information about the geographic coordinates.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="LocationDescriptionText" type="LocationDescriptionTextDataType">
        <xsd:annotation>
            <xsd:documentation>A brief explanation of a location, including navigational directions and/or more descriptive information.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="LongitudeMeasure" type="LongitudeMeasureDataType">
        <xsd:annotation>
            <xsd:documentation>The measure of the angular distance on a meridian east or west of the prime meridian.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="MailingAddressCityName" type="MailingAddressCityNameDataType">
        <xsd:annotation>
            <xsd:documentation>The name of the city, town, or village where the mail is delivered.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="MailingAddressText" type="MailingAddressTextDataType">
        <xsd:annotation>
            <xsd:documentation>The exact address where a mail piece is intended to be delivered, including urban-style street address, rural route, and PO Box.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="MeasurePrecisionText" type="MeasurePrecisionTextDataType">
        <xsd:annotation>
            <xsd:documentation>The precision of the recorded value.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="MeasureUnitCode" type="MeasureUnitCodeDataType">
        <xsd:annotation>
            <xsd:documentation>The code that represents the unit for measuring the item.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="MeasureUnitCodeListIdentifier" type="MeasureUnitCodeListIdentifierDataType">
        <xsd:annotation>
            <xsd:documentation>A designator specifying the code set used to provide a measurement unit code. Can be used to identify the URL of a source that defines the set of currently approved permitted values.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="MeasureUnitName" type="MeasureUnitNameDataType">
        <xsd:annotation>
            <xsd:documentation>A description of the unit of measure code.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="MeasureValue" type="MeasureValueDataType">
        <xsd:annotation>
            <xsd:documentation>The recorded dimension, capacity, quality, or amount of something ascertained by measuring or observing.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="MethodDescriptionText" type="MethodDescriptionTextDataType">
        <xsd:annotation>
            <xsd:documentation>A brief summary that provides general information about the method.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="MethodDeviationsText" type="MethodDeviationsTextDataType">
        <xsd:annotation>
            <xsd:documentation>Text that identifies any deviations from the published method reference.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="MethodIdentifierCode" type="MethodIdentifierCodeDataType">
        <xsd:annotation>
            <xsd:documentation>The identification number or code assigned by the method publisher.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="MethodIdentifierCodeListIdentifier" type="MethodIdentifierCodeListIdentifierDataType">
        <xsd:annotation>
            <xsd:documentation>A designator specifying the code set used to provide a reference method code. Can be used to identify the URL of a source that defines the set of currently approved permitted values.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="MethodName" type="MethodNameDataType">
        <xsd:annotation>
            <xsd:documentation>The title of the method that appears on the method from the organization that published it.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="MethodologyDescriptionText" type="MethodologyDescriptionTextDataType">
        <xsd:annotation>
            <xsd:documentation>The text that describes the process and/or tools that manage storage, disposal, treatment, and other handling protocols designed for and/or used.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="MethodologyTypeText" type="MethodologyTypeTextDataType">
        <xsd:annotation>
            <xsd:documentation>The type of process and/or tool designed or used to manage storage, disposal, treatment, and other handling protocols.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="MiddleName" type="MiddleNameDataType">
        <xsd:annotation>
            <xsd:documentation>The middle name or initial of an individual.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="MolecularFormulaCode" type="MolecularFormulaCodeDataType">
        <xsd:annotation>
            <xsd:documentation>The code that represents the number of atoms of each element in a molecule of a chemical substance.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="MonitoringFrequencyText" type="MonitoringFrequencyTextDataType">
        <xsd:annotation>
            <xsd:documentation>The required frequency with which monitoring is to be conducted at the site or location.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="MonitoringLocationDescriptionText" type="MonitoringLocationDescriptionTextDataType">
        <xsd:annotation>
            <xsd:documentation>Text description of the monitoring location.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="MonitoringLocationIdentifier" type="MonitoringLocationIdentifierDataType">
        <xsd:annotation>
            <xsd:documentation>A designator used to describe the unique name, number, or code assigned to identify the monitoring location.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="MonitoringLocationName" type="MonitoringLocationNameDataType">
        <xsd:annotation>
            <xsd:documentation>
                The designator specified by the sampling organization for the site at
                which sampling or other activities are conducted.
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="MonitoringLocationTypeName" type="MonitoringLocationTypeNameDataType">
        <xsd:annotation>
            <xsd:documentation>The descriptive name for a type of monitoring location.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="MonitoringSiteDescription" type="MonitoringSiteDescriptionDataType">
        <xsd:annotation>
            <xsd:documentation>Text that describes the monitoring site with respect to a feature.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="NAICSCode" type="NAICSCodeDataType">
        <xsd:annotation>
            <xsd:documentation>The code that represents a subdivision of an industry that accommodates user needs in the United States.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="NAICSGroupCode" type="NAICSGroupCodeDataType">
        <xsd:annotation>
            <xsd:documentation>The code that represents a group of related industries within the economy.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="NAICSIndustryCode" type="NAICSIndustryCodeDataType">
        <xsd:annotation>
            <xsd:documentation>The code that represents an industry within the economy.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="NAICSPrimaryIndicator" type="NAICSPrimaryIndicatorDataType">
        <xsd:annotation>
            <xsd:documentation>The name that indicates whether the associated NAICS code represents the primary activity occurring at the facility site.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="NAICSSectorCode" type="NAICSSectorCodeDataType">
        <xsd:annotation>
            <xsd:documentation>The code that represents an economic sector.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="NAICSSubsectorCode" type="NAICSSubsectorCodeDataType">
        <xsd:annotation>
            <xsd:documentation>The code that represents an economic subsector.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="NamePrefixText" type="NamePrefixTextDataType">
        <xsd:annotation>
            <xsd:documentation>The text that describes the title that precedes an individual's name.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="NameSuffixText" type="NameSuffixTextDataType">
        <xsd:annotation>
            <xsd:documentation>Additional title that indicates lineage or professional title.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="NumericConditionStatisticalBasisName" type="NumericConditionStatisticalBasisNameDataType">
        <xsd:annotation>
            <xsd:documentation>The name of the statistical basis specified in a limit/numeric condition.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="OrganizationFormalName" type="OrganizationFormalNameDataType">
        <xsd:annotation>
            <xsd:documentation>The legal designator (i.e. formal name) of an organization.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="OrganizationIdentifier" type="OrganizationIdentifierDataType">
        <xsd:annotation>
            <xsd:documentation>A designator used to uniquely identify a unique business establishment within a context.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="OtherPermitIdentifier" type="OtherPermitIdentifierDataType">
        <xsd:annotation>
            <xsd:documentation>An alphanumeric identifier assigned to the permit.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PenaltyAmountRequired" type="PenaltyAmountRequiredDataType">
        <xsd:annotation>
            <xsd:documentation>The dollar amount of any cash penalty, payment of which is required pursuant to a Final Order.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PenaltyAmountSought" type="PenaltyAmountSoughtDataType">
        <xsd:annotation>
            <xsd:documentation>The dollar amount of any proposed cash penalty set forth in a Complaint/Proposed Order.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PenaltyIdentifier" type="PenaltyIdentifierDataType">
        <xsd:annotation>
            <xsd:documentation>A designator that uniquely identifies each penalty.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PenaltySupplementalInformationText" type="PenaltySupplementalInformationTextDataType">
        <xsd:annotation>
            <xsd:documentation>Additional description of the penalty.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PenaltyTypeCode" type="PenaltyTypeCodeDataType">
        <xsd:annotation>
            <xsd:documentation>Code which indicates the type of penalty associated with the penalty amount.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PermitApplicationCompletionDate" type="PermitApplicationCompletionDateDataType">
        <xsd:annotation>
            <xsd:documentation>The calendar date that a permit application was deemed to be complete.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PermitEffectiveDate" type="PermitEffectiveDateDataType">
        <xsd:annotation>
            <xsd:documentation>The calendar date that a permit becomes effective.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PermitEventDate" type="PermitEventDateDataType">
        <xsd:annotation>
            <xsd:documentation>The date of an event associated with a permit or permit application process.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PermitEventTypeText" type="PermitEventTypeTextDataType">
        <xsd:annotation>
            <xsd:documentation>The text descibing an event associated with a permit or permit application process.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PermitExpirationDate" type="PermitExpirationDateDataType">
        <xsd:annotation>
            <xsd:documentation>The calendar date that a permit expires.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PermitIdentifier" type="PermitIdentifierDataType">
        <xsd:annotation>
            <xsd:documentation>The alphanumeric identifier assigned to the permit by a permit issuing/granting organization to identify a permit or permit application.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PermitIssueDate" type="PermitIssueDateDataType">
        <xsd:annotation>
            <xsd:documentation>The calendar date that a permit was issued.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PermitName" type="PermitNameDataType">
        <xsd:annotation>
            <xsd:documentation>The name assigned to the permit by a permit issuing/granting organization to identify a permit or permit application.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PermitRevocationDate" type="PermitRevocationDateDataType">
        <xsd:annotation>
            <xsd:documentation>The calendar date that a permit will be or was revoked.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PermitStatusText" type="PermitStatusTextDataType">
        <xsd:annotation>
            <xsd:documentation>The administrative or legal status of a permit.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PermittedFeatureDescriptionText" type="PermittedFeatureDescriptionTextDataType">
        <xsd:annotation>
            <xsd:documentation>The text that describes the feature that is permitted.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PermittedFeatureEndDate" type="PermittedFeatureEndDateDataType">
        <xsd:annotation>
            <xsd:documentation>The calendar date that the operating status of a permitted feature is no longer in effect.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PermittedFeatureIdentifier" type="PermittedFeatureIdentifierDataType">
        <xsd:annotation>
            <xsd:documentation>The alphanumeric identifier or name assigned by a permit issuing organization to identify a permitted unit, feature, or process.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PermittedFeatureOperatingStatusName" type="PermittedFeatureOperatingStatusNameDataType">
        <xsd:annotation>
            <xsd:documentation>The name of the category describing the operating status of a permitted unit.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PermittedFeatureStartDate" type="PermittedFeatureStartDateDataType">
        <xsd:annotation>
            <xsd:documentation>The calendar date that the operating status of a permitted feature takes effect.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PermittedFeatureTypeName" type="PermittedFeatureTypeNameDataType">
        <xsd:annotation>
            <xsd:documentation>The type of permitted unit, feature, or process represented by an identifier.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PermitTerminationDate" type="PermitTerminationDateDataType">
        <xsd:annotation>
            <xsd:documentation>The calendar date that a permit will be or was terminated or surrendered.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PermitTypeCode" type="PermitTypeCodeDataType">
        <xsd:annotation>
            <xsd:documentation>The code that represents the type of permit issued or granted to a regulated entity.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PermitTypeCodeListIdentifier" type="PermitTypeCodeListIdentifierDataType">
        <xsd:annotation>
            <xsd:documentation>A designator specifying the code set used to provide a permit type code. Can be used to identify the URL of a source that defines the set of currently approved permitted values.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PermitTypeName" type="PermitTypeNameDataType">
        <xsd:annotation>
            <xsd:documentation>A description of the permit type code.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ProgramName" type="ProgramNameDataType">
        <xsd:annotation>
            <xsd:documentation>The name of the program/jurisdictional authority under which a permit is issued or granted.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ProgramSystemAbbreviatedName" type="ProgramSystemAbbreviatedNameDataType">
        <xsd:annotation>
            <xsd:documentation>An abbreviated name that represents the name of an information management system for an environmental program.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ProgramSystemBiologicalName" type="ProgramSystemBiologicalNameDataType">
        <xsd:annotation>
            <xsd:documentation>The name applied to a biological entity in a program system.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ProgramSystemName" type="ProgramSystemNameDataType">
        <xsd:annotation>
            <xsd:documentation>The name of an information management system for an environmental program.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ReferencePointCodeListIdentifier" type="ReferencePointCodeListIdentifierDataType">
        <xsd:annotation>
            <xsd:documentation>A designator specifying the code set used to provide a geographic reference point code. Can be used to identify the URL of a source that defines the set of currently approved permitted values.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ReplacedReportIdentifier" type="ReplacedReportIdentifierDataType">
        <xsd:annotation>
            <xsd:documentation>The previous Report Identifier this report intends to replace (for revision only).</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ReportCreateDate" type="ReportCreateDateDataType">
        <xsd:annotation>
            <xsd:documentation>The date on which the report was created.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ReportCreateTime" type="ReportCreateTimeDataType">
        <xsd:annotation>
            <xsd:documentation>The time on which the report was created.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ReportDueDate" type="ReportDueDateDataType">
        <xsd:annotation>
            <xsd:documentation>The date that the report is due to the report recipient.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ReportIdentifier" type="ReportIdentifierDataType">
        <xsd:annotation>
            <xsd:documentation>The unique tracking number or name assigned by a system or program that identifies a report.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ReportingFrequencyText" type="ReportingFrequencyTextDataType">
        <xsd:annotation>
            <xsd:documentation>The frequency with which the report is required to be submitted to the report recipient.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ReportingPeriodEndDate" type="ReportingPeriodEndDateDataType">
        <xsd:annotation>
            <xsd:documentation>The end date for a series of required reports.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ReportingPeriodStartDate" type="ReportingPeriodStartDateDataType">
        <xsd:annotation>
            <xsd:documentation>The start date for a series of required reports.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ReportReceivedDate" type="ReportReceivedDateDataType">
        <xsd:annotation>
            <xsd:documentation>The actual date the report was received by the report recipient.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ReportRecipientName" type="ReportRecipientNameDataType">
        <xsd:annotation>
            <xsd:documentation>The name of the entity or entities directed by the permit or regulations to receive the report.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ReportTypeCode" type="ReportTypeCodeDataType">
        <xsd:annotation>
            <xsd:documentation>A code used to identify a type of report.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ReportTypeCodeListIdentifier" type="ReportTypeCodeListIdentifierDataType">
        <xsd:annotation>
            <xsd:documentation>A designator specifying the code set used to provide a report type code. Can be used to identify the URL of a source that defines the set of currently approved permitted values.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ReportTypeName" type="ReportTypeNameDataType">
        <xsd:annotation>
            <xsd:documentation>A description of the report type code.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ResultQualifierCode" type="ResultQualifierCodeDataType">
        <xsd:annotation>
            <xsd:documentation>A code used to identify any qualifying issues that affect the results.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ResultQualifierCodeListIdentifier" type="ResultQualifierCodeListIdentifierDataType">
        <xsd:annotation>
            <xsd:documentation>A designator specifying the code set used to provide a result qualifier code. Can be used to identify the URL of a source that defines the set of currently approved permitted values.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ResultQualifierName" type="ResultQualifierNameDataType">
        <xsd:annotation>
            <xsd:documentation>A description of the result code of any qualifying issues that affect the results.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ReturnToComplianceActualDate" type="ReturnToComplianceActualDateDataType">
        <xsd:annotation>
            <xsd:documentation>The calendar date, determined by the Responsible Authority, on which the regulated entity actually returned to compliance with respect to the legal obligation that is the subject of the Violation Determined Date.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ReturnToComplianceScheduleDate" type="ReturnToComplianceScheduleDateDataType">
        <xsd:annotation>
            <xsd:documentation>The calendar date, specified in the Compliance Schedule (if any), on which the regulated entity is scheduled to return to compliance with respect to the legal obligation that is the subject of the Violation Determined Date.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="RevisionIndicator" type="RevisionIndicatorDataType">
        <xsd:annotation>
            <xsd:documentation>An indicator to show whether the report is an original submission or a revision.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="SEPPenalityAssessmentValueAmount" type="SEPPenalityAssessmentValueAmountDataType">
        <xsd:annotation>
            <xsd:documentation>That portion of the Supplemental Environmental Projects (SEP) Value which is explicitly denominated as a civil penalty assessed in a Final Order.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="SICCode" type="SICCodeDataType">
        <xsd:annotation>
            <xsd:documentation>The code that represents the economic activity of a company.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="SICDivisionCode" type="SICDivisionCodeDataType">
        <xsd:annotation>
            <xsd:documentation>The code that represents a division in the economy that covers an economic activity.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="SICGroupCode" type="SICGroupCodeDataType">
        <xsd:annotation>
            <xsd:documentation>The code that represents a group of related industries within the economy.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="SICMajorGroupCode" type="SICMajorGroupCodeDataType">
        <xsd:annotation>
            <xsd:documentation>The code that represents a major group of industrial classifications.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="SICPrimaryIndicator" type="SICPrimaryIndicatorDataType">
        <xsd:annotation>
            <xsd:documentation>The name that indicates whether the associated SIC Code represents the primary activity occurring at the facility site.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="SICSubdivisionText" type="SICSubdivisionTextDataType">
        <xsd:annotation>
            <xsd:documentation>The text that describes a specific component of an industry.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="SourceMapScaleNumber" type="SourceMapScaleNumberDataType">
        <xsd:annotation>
            <xsd:documentation>The number that represents the proportional distance on the ground for one unit of measure on the map or photo.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="StateCode" type="StateCodeDataType">
        <xsd:annotation>
            <xsd:documentation>A code designator used to identify a principal administrative subdivision of the United States, Canada, or Mexico.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="StateCodeListIdentifier" type="StateCodeListIdentifierDataType">
        <xsd:annotation>
            <xsd:documentation>A designator specifying the code set used to provide a state code. Can be used to identify the URL of a source that defines the set of currently approved permitted values.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="StateName" type="StateNameDataType">
        <xsd:annotation>
            <xsd:documentation>A name used to identify a principal administrative subdivision of the United States, Canada, or Mexico.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="StipulatedPenaltyAmountRequired" type="StipulatedPenaltyAmountRequiredDataType">
        <xsd:annotation>
            <xsd:documentation>The dollar amount of any cash stipulated penalty, payment of which is required by the Enforcement Agency, pursuant to the terms of a pre-existing Final Order.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="SubstanceIdentifier" type="SubstanceIdentifierDataType">
        <xsd:annotation>
            <xsd:documentation>A designator used to uniquely identify a substance.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="SubstanceName" type="SubstanceNameDataType">
        <xsd:annotation>
            <xsd:documentation>The name assigned to a chemical, biological or radiological substance or feature that describes it in terms of its molecular composition, taxonomic nomenclature or other characteristic (e.g, physical measure).</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="SupplementalAddressText" type="SupplementalAddressTextDataType">
        <xsd:annotation>
            <xsd:documentation>The text that provides additional information to facilitate the delivery of a mail piece, including building name, secondary units, and mail stop or local box numbers not serviced by the U.S. Postal Service.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="SupplementalEnvironmentalProjectDescriptionText" type="SupplementalEnvironmentalProjectDescriptionTextDataType">
        <xsd:annotation>
            <xsd:documentation>The narrative text describing any Supplemental Environmental Projects required to be performed pursuant to a Final Order.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="SupplementalEnvironmentalProjectValueAmount" type="SupplementalEnvironmentalProjectValueAmountDataType">
        <xsd:annotation>
            <xsd:documentation>The dollar value of any Supplemental Environmental Projects (SEP) required to be performed pursuant to a Final Order.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="SupplementalLocationText" type="SupplementalLocationTextDataType">
        <xsd:annotation>
            <xsd:documentation>The text that provides additional information about a place, including a building name with its secondary unit and number, an industrial park name, an installation name or descriptive text where no formal address is available.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="TelephoneExtensionNumberText" type="TelephoneExtensionNumberTextDataType">
        <xsd:annotation>
            <xsd:documentation>The number assigned within an organization to an individual telephone that extends the external telephone number.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="TelephoneNumberText" type="TelephoneNumberTextDataType">
        <xsd:annotation>
            <xsd:documentation>The number that identifies a particular telephone connection.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="TelephoneNumberTypeName" type="TelephoneNumberTypeNameDataType">
        <xsd:annotation>
            <xsd:documentation>The name that describes a telephone number type.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="TotalPenaltyValueAmount" type="TotalPenaltyValueAmountDataType">
        <xsd:annotation>
            <xsd:documentation>The total value of the penalty required by a Final Order.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="TribalCode" type="TribalCodeDataType">
        <xsd:annotation>
            <xsd:documentation>The code that represents the American Indian tribe or Alaskan Native entity.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="TribalCodeListIdentifier" type="TribalCodeListIdentifierDataType">
        <xsd:annotation>
            <xsd:documentation>A designator specifying the code set used to provide a tribal code. Can be used to identify the URL of a source that defines the set of currently approved permitted values.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="TribalLandIndicator" type="TribalLandIndicatorDataType">
        <xsd:annotation>
            <xsd:documentation>An indicator denoting the location address is a tribal land</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="TribalLandName" type="TribalLandNameDataType">
        <xsd:annotation>
            <xsd:documentation>The name of an American Indian or Alaskan native area where the location address exists.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="TribalName" type="TribalNameDataType">
        <xsd:annotation>
            <xsd:documentation>The name of the American Indian tribe or Alaskan Native entity.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ViolationDeterminedDate" type="ViolationDeterminedDateDataType">
        <xsd:annotation>
            <xsd:documentation>The calendar date the Responsible Authority determines that a regulated entity is in violation of a legally enforceable obligation.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ViolationIdentifer" type="ViolationIdentiferDataType">
        <xsd:annotation>
            <xsd:documentation>Text or numeric code that uniquely identifies a violation record.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ViolationSupplementalInformationText" type="ViolationSupplementalInformationTextDataType">
        <xsd:annotation>
            <xsd:documentation>Additional description of the violation.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ViolationTypeCode" type="ViolationTypeCodeDataType">
        <xsd:annotation>
            <xsd:documentation>A code used to identify a type or class of violation.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ViolationTypeCodeListIdentifier" type="ViolationTypeCodeListIdentifierDataType">
        <xsd:annotation>
            <xsd:documentation>A designator specifying the code set used to provide a violation type code. Can be used to identify the URL of a source that defines the set of currently approved permitted values.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ViolationTypeName" type="ViolationTypeNameDataType">
        <xsd:annotation>
            <xsd:documentation>A description of the violation type code.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
</xsd:schema>
