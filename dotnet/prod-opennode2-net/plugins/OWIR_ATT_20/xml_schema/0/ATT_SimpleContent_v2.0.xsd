    <xsd:element name="BiologicalSubclassName" type="BiologicalSubclassNameDataType">
        <xsd:annotation>
            <xsd:documentation>The systematic name that represents the biological Subclass.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="BiologicalSubdivisionSubphylumName" type="BiologicalSubdivisionSubphylumNameDataType">
        <xsd:annotation>
            <xsd:documentation>The systematic name that represents the biological Subdivision/Subphylum.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="BiologicalSubfamilyName" type="BiologicalSubfamilyNameDataType">
        <xsd:annotation>
            <xsd:documentation>The systematic name that represents the biological Subfamily.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="BiologicalSuborderName" type="BiologicalSuborderNameDataType">
        <xsd:annotation>
            <xsd:documentation>The systematic name that represents the biological Suborder.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="BiologicalSubspeciesName" type="BiologicalSubspeciesNameDataType">
        <xsd:annotation>
            <xsd:documentation>The systematic name that represents the biological Subspecies.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="BiologicalSynonymName" type="BiologicalSynonymNameDataType">
        <xsd:annotation>
            <xsd:documentation>The name that represents one of two or more names applied to a single biological entity.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="BiologicalSystematicName" type="BiologicalSystematicNameDataType">
        <xsd:annotation>
            <xsd:documentation>The name assigned to a biological entity by a classification identification system.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="BiologicalVernacularName" type="BiologicalVernacularNameDataType">
        <xsd:annotation>
            <xsd:documentation>The vernacular name associated with an occurrence of a biological entity.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="CASRegistryNumber" type="CASRegistryNumberDataType">
        <xsd:annotation>
            <xsd:documentation>The unique number assigned by Chemical Abstracts Service (CAS) to a chemical substance.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ChemicalNameContextName" type="ChemicalNameContextNameDataType">
        <xsd:annotation>
            <xsd:documentation>The name that identifies the circumstances for which a chemical substance name is used.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ChemicalStructureGraphicalDiagram" type="ChemicalStructureGraphicalDiagramDataType">
        <xsd:annotation>
            <xsd:documentation>A graphical representation of a molecule of a chemical substance as a two or three dimensional diagram.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ChemicalSubstanceClassificationName" type="ChemicalSubstanceClassificationNameDataType">
        <xsd:annotation>
            <xsd:documentation>The name of a schema that classifies chemical substances according to structural similarities.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ChemicalSubstanceCommentText" type="ChemicalSubstanceCommentTextDataType">
        <xsd:annotation>
            <xsd:documentation>The text that provides additional information about a chemical substance.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ChemicalSubstanceDefinitionText" type="ChemicalSubstanceDefinitionTextDataType">
        <xsd:annotation>
            <xsd:documentation>The text that provides clarification to the identity of a chemical substance.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ChemicalSubstanceFormulaWeightQuantity" type="ChemicalSubstanceFormulaWeightQuantityDataType">
        <xsd:annotation>
            <xsd:documentation>The sum of the atomic weights of constituent atoms in a molecule of a chemical substance.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ChemicalSubstanceLinearStructureCode" type="ChemicalSubstanceLinearStructureCodeDataType">
        <xsd:annotation>
            <xsd:documentation>The code that represents the connectivity of atoms in a molecule of a chemical substance as a linear formula, such as SMILES.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ChemicalSubstanceSynonymName" type="ChemicalSubstanceSynonymNameDataType">
        <xsd:annotation>
            <xsd:documentation>A name that is used as the alternative for representing a chemical substance.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ChemicalSubstanceSystematicName" type="ChemicalSubstanceSystematicNameDataType">
        <xsd:annotation>
            <xsd:documentation>The name assigned to a chemical substance that describes it in terms of its molecular composition.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ChemicalSubstanceTypeName" type="ChemicalSubstanceTypeNameDataType">
        <xsd:annotation>
            <xsd:documentation>A descriptive name for types of chemical substances.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ChemicalSynonymSourceName" type="ChemicalSynonymSourceNameDataType">
        <xsd:annotation>
            <xsd:documentation>The name of the source of an alternate name for a chemical substance.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ChemicalSynonymStatusName" type="ChemicalSynonymStatusNameDataType">
        <xsd:annotation>
            <xsd:documentation>The name that documents the correctness of a synonym for a specific chemical substance.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="CitationReferenceCode" type="CitationReferenceCodeDataType">
        <xsd:annotation>
            <xsd:documentation>The code that represents a reference to an official printed copy of an environmental regulation.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="CollectionRequiredAmount" type="CollectionRequiredAmountDataType">
        <xsd:annotation>
            <xsd:documentation>The dollar amount of any collection payment required in a Final Order or other mechanism used to resolve the collection Complaint/Proposed Order.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="CollectionSoughtAmount" type="CollectionSoughtAmountDataType">
        <xsd:annotation>
            <xsd:documentation>The dollar amount of the prior debt sought to be collected through a Complaint/Proposed Order.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ComplianceMilestoneCompletedDate" type="ComplianceMilestoneCompletedDateDataType">
        <xsd:annotation>
            <xsd:documentation>The calendar date, determined by the Reporting Authority, on which a compliance milestone action required in an enforcement action resolution has been completed, including, if appliable, all injunctive relief, SEPs, and all penalty or cost recovery payments required.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ComplianceMilestoneExpenditureAmount" type="ComplianceMilestoneExpenditureAmountDataType">
        <xsd:annotation>
            <xsd:documentation>The actual amount (in US dollars) of the SEP or enforcement milestone expenditure.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ComplianceMilestoneIdentifier" type="ComplianceMilestoneIdentifierDataType">
        <xsd:annotation>
            <xsd:documentation>A designator used to uniquely identify a compliance milestone within a context.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ComplianceMilestoneScheduledDate" type="ComplianceMilestoneScheduledDateDataType">
        <xsd:annotation>
            <xsd:documentation>Date set by an enforcement action for the full or partial expenditure of funds for a SEP project or other compliance milestone.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ComplianceMilestoneStatusDeterminationDate" type="ComplianceMilestoneStatusDeterminationDateDataType">
        <xsd:annotation>
            <xsd:documentation>The calendar date the Responsible Authority last determined a regulated entitys status of compliance with the milestone obligations in an enforcement action resolution instrument.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ComplianceMilestoneStatusText" type="ComplianceMilestoneStatusTextDataType">
        <xsd:annotation>
            <xsd:documentation>The status of implementation by a Defendant/Respondent, on a given date (the Compliance Milestones Determination Date), of any compliance milestone actions required to have been completed by that date, including injunctive relief, Supplemental Environmental Projects, and penalty and cost recovery payments required.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ComplianceMilestoneTypeCode" type="ComplianceMilestoneTypeCodeDataType">
        <xsd:annotation>
            <xsd:documentation>A code used to identify a type of compliance milestone.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ComplianceMilestoneTypeCodeListIdentifier" type="ComplianceMilestoneTypeCodeListIdentifierDataType">
        <xsd:annotation>
            <xsd:documentation>A designator specifying the code set used to provide a compliance milestone type code. Can be used to identify the URL of a source that defines the set of currently approved permitted values.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ComplianceMilestoneTypeName" type="ComplianceMilestoneTypeNameDataType">
        <xsd:annotation>
            <xsd:documentation>A description of the compliance milestone type code.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ComplianceMilestoneViolationResponseIndicator" type="ComplianceMilestoneViolationResponseIndicatorDataType">
        <xsd:annotation>
            <xsd:documentation>The designator that indicates whether the Enforcement Agency has taken action in response to a Compliance Milestones Implementation Violation.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ComplianceScheduleDescriptionText" type="ComplianceScheduleDescriptionTextDataType">
        <xsd:annotation>
            <xsd:documentation>Description of the compliance schedule.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ComplianceScheduleIdentifier" type="ComplianceScheduleIdentifierDataType">
        <xsd:annotation>
            <xsd:documentation>A designator used to uniquely identify a compliance schedule within a context.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ComplianceScheduleIndicator" type="ComplianceScheduleIndicatorDataType">
        <xsd:annotation>
            <xsd:documentation>The designator that indicates whether the regulated entity is currently on a legally enforceable compliance schedule (i.e., pursuant to a Final Order) for return to compliance with the obligation that is the subject of a violation or series of violations.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ConditionBasisText" type="ConditionBasisTextDataType">
        <xsd:annotation>
            <xsd:documentation>The regulatory or technical framework used to define the requirement.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ConditionDescriptionText" type="ConditionDescriptionTextDataType">
        <xsd:annotation>
            <xsd:documentation>The language that explains the requirement placed on the responsible party.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ConditionEndDate" type="ConditionEndDateDataType">
        <xsd:annotation>
            <xsd:documentation>The date on which a condition ends being in effect.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ConditionIdentifier" type="ConditionIdentifierDataType">
        <xsd:annotation>
            <xsd:documentation>The reference to a section of a permit that identifies the condition within a specific permit.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ConditionStartDate" type="ConditionStartDateDataType">
        <xsd:annotation>
            <xsd:documentation>The date on which a condition begins being in effect.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ConditionStatusName" type="ConditionStatusNameDataType">
        <xsd:annotation>
            <xsd:documentation>The name of the category describing the status of the condition.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ConditionTriggerText" type="ConditionTriggerTextDataType">
        <xsd:annotation>
            <xsd:documentation>The text that describes an event or circumstance that activates a requirement.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="CoordinateDataSourceCode" type="CoordinateDataSourceCodeDataType">
        <xsd:annotation>
            <xsd:documentation>The code that represents the party responsible for providing the latitude and longitude coordinates.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="CoordinateDataSourceCodeListIdentifier" type="CoordinateDataSourceCodeListIdentifierDataType">
        <xsd:annotation>
            <xsd:documentation>A designator specifying the code set used to provide a coordinate data source type code. Can be used to identify the URL of a source that defines the set of currently approved permitted values.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="CoordinateDataSourceName" type="CoordinateDataSourceNameDataType">
        <xsd:annotation>
            <xsd:documentation>The name of the party responsible for providing the latitude and longitude coordinates.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="CostRecoveryRequiredAmount" type="CostRecoveryRequiredAmountDataType">
        <xsd:annotation>
            <xsd:documentation>The dollar amount of any cost recovery required to be paid pursuant to a Final Order.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="CostRecoverySoughtAmount" type="CostRecoverySoughtAmountDataType">
        <xsd:annotation>
            <xsd:documentation>The dollar amount of any cost recovery sought in a Complaint/Proposed Order.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="CountryCode" type="CountryCodeDataType">
        <xsd:annotation>
            <xsd:documentation>A code designator used to identify a primary geopolitical unit of the world.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="CountryCodeListIdentifier" type="CountryCodeListIdentifierDataType">
        <xsd:annotation>
            <xsd:documentation>A designator specifying the code set used to provide a country code. Can be used to identify the URL of a source that defines the set of currently approved permitted values.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="CountryName" type="CountryNameDataType">
        <xsd:annotation>
            <xsd:documentation>A name used to identify a primary geopolitical unit of the world.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="CountyCode" type="CountyCodeDataType">
        <xsd:annotation>
            <xsd:documentation>The code that represents the county.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="CountyCodeListIdentifier" type="CountyCodeListIdentifierDataType">
        <xsd:annotation>
            <xsd:documentation>A designator specifying the code set used to provide a county code. Can be used to identify the URL of a source that defines the set of currently approved permitted values.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="CountyName" type="CountyNameDataType">
        <xsd:annotation>
            <xsd:documentation>A description of the county code.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="DataCollectionDate" type="DataCollectionDateDataType">
        <xsd:annotation>
            <xsd:documentation>The calendar date when data were collected.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
