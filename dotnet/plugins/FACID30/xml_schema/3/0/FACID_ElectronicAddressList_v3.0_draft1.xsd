<?xml version="1.0" encoding="utf-8"?>
<xsd:schema targetNamespace="http://www.exchangenetwork.net/schema/facilityid/3" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:facid="http://www.exchangenetwork.net/schema/facilityid/3" elementFormDefault="qualified" attributeFormDefault="unqualified" version="3.0">
	<xsd:include schemaLocation="SC_ElectronicAddress_v2.0.xsd"/>
	<xsd:annotation>
		<xsd:documentation>
			Schema Name : 	FACID_ElectronicAddressList_v3.0
			Current Version Available At : http://www.exchangenetwork.net/
			Description : 	A container for one or more electronic addresses.
			Application : 
			Developed by : 	Environmental Protection Agency, Windsor Solutions, Inc.
			Point of Contact : Kevin Lyons
		</xsd:documentation>
	</xsd:annotation>
	<xsd:element name="ElectronicAddressList" type="facid:ElectronicAddressListDataType">
		<xsd:annotation>
			<xsd:documentation>A container for one or more electronic addresses.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="ElectronicAddressListDataType">
		<xsd:sequence>
			<xsd:element ref="facid:ElectronicAddress" minOccurs="1" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
