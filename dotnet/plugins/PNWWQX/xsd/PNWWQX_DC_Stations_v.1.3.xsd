<?xml version="1.0" encoding="utf-8"?><!-- edited with XMLSPY v2004 rel. 3 U (http://www.xmlspy.com) by Department of Environmental Quality (Department of Environmental Quality) --><xsd:schema targetNamespace="urn:us:net:exchangenetwork" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pnwwqx="urn:us:net:exchangenetwork" version="1.3">	<xsd:include schemaLocation=".\PNWWQX_LatitudeMeasureType_v.1.3.xsd"/>	<xsd:include schemaLocation=".\PNWWQX_LongitudeMeasureType_v.1.3.xsd"/>	<xsd:include schemaLocation=".\PNWWQX_StationType_v.1.3.xsd"/>	<xsd:annotation>		<xsd:documentation>		Schema Name : 					PNWWQX_DC_Stations_v.1.3.xsd 		Current Version Available At :			Description : 					This schema is used to provide a list of all stations recorded in the Pacific Northwest Water Quality Data Exchange Data Catalog (DC).		Application : 					Pacific Northwest Water Quality Data Exchange 		Developed by : 	 				Pacific Northwest Exchange States; Windsor Solutions, Inc		Point of Contact : 				Curtis Cude (cude.curtis@deq.state.or.us)										Kevin Jeffery (kevin_jeffery@windsorsolutions.com)			</xsd:documentation>	</xsd:annotation>	<xsd:element name="PNWWQX_DC_Stations" type="pnwwqx:PNWWQXDCStations"/>	<xsd:complexType name="PNWWQXDCStations">		<xsd:sequence>			<xsd:element ref="pnwwqx:PNWWQXDCStationsList" minOccurs="0" maxOccurs="unbounded"/>		</xsd:sequence>	</xsd:complexType>	<xsd:element name="PNWWQXDCStationsList" type="pnwwqx:PNWWQXDCStationsList"/>	<xsd:complexType name="PNWWQXDCStationsList">		<xsd:sequence>			<xsd:element ref="pnwwqx:ProvidingOrganizationIdentifier"/>			<xsd:element ref="pnwwqx:StationIdentifier"/>			<xsd:element ref="pnwwqx:StationName"/>			<xsd:element ref="pnwwqx:StationType"/>			<xsd:element ref="pnwwqx:LatitudeMeasure"/>			<xsd:element ref="pnwwqx:LongitudeMeasure"/>		</xsd:sequence>	</xsd:complexType>	<xsd:element name="ProvidingOrganizationIdentifier" type="xsd:string"/>	<xsd:element name="StationIdentifier" type="xsd:string"/>	<xsd:element name="StationName" type="xsd:string"/>	<xsd:element name="StationType" type="pnwwqx:StationType"/>	<xsd:element name="LatitudeMeasure" type="pnwwqx:LatitudeMeasureType"/>	<xsd:element name="LongitudeMeasure" type="pnwwqx:LongitudeMeasureType"/></xsd:schema>