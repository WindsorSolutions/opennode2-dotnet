<?xml version="1.0" encoding="utf-8"?><xsd:schema targetNamespace="urn:us:net:exchangenetwork" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pnwwqx="urn:us:net:exchangenetwork" version="1.3">	<xsd:annotation>		<xsd:documentation>		Schema Name : 					PNWWQX_BinaryLargeObjectType_v.1.3.xsd 		Current Version Available At :			Description : 					This schema defines the data elements that characterize a binary object or document that may be attached to an XML payload used to transfer water quality monitoring data across the Exchange.		Application : 					Pacific Northwest Water Quality Data Exchange 		Developed by : 	 				Pacific Northwest Exchange States; Windsor Solutions, Inc		Point of Contact : 				Curtis Cude (cude.curtis@deq.state.or.us)										Kevin Jeffery (kevin_jeffery@windsorsolutions.com)		</xsd:documentation>	</xsd:annotation>	<xsd:complexType name="BinaryLargeObjectType">		<xsd:sequence>			<xsd:element ref="pnwwqx:BinaryObject" minOccurs="0"/>			<xsd:element ref="pnwwqx:BinaryObjectURL" minOccurs="0"/>			<xsd:element ref="pnwwqx:BinaryObjectSize" minOccurs="0"/>			<xsd:element ref="pnwwqx:BinaryObjectTitleText" minOccurs="0"/>			<xsd:element ref="pnwwqx:BinaryObjectCreator" minOccurs="0"/>			<xsd:element ref="pnwwqx:BinaryObjectSubject" minOccurs="0"/>			<xsd:element ref="pnwwqx:BinaryObjectDescription" minOccurs="0"/>			<xsd:element ref="pnwwqx:BinaryObjectPublisher" minOccurs="0"/>			<xsd:element ref="pnwwqx:BinaryObjectContributor" minOccurs="0"/>			<xsd:element ref="pnwwqx:BinaryObjectDate" minOccurs="0"/>			<xsd:element ref="pnwwqx:BinaryObjectType" minOccurs="0"/>			<xsd:element ref="pnwwqx:BinaryObjectContentTypeText" minOccurs="0"/>			<xsd:element ref="pnwwqx:BinaryObjectIdentifierText" minOccurs="0"/>			<xsd:element ref="pnwwqx:BinaryObjectSource" minOccurs="0"/>			<xsd:element ref="pnwwqx:BinaryObjectLanguage" minOccurs="0"/>			<xsd:element ref="pnwwqx:BinaryObjectRelation" minOccurs="0"/>			<xsd:element ref="pnwwqx:BinaryObjectCoverage" minOccurs="0"/>			<xsd:element ref="pnwwqx:BinaryObjectRights" minOccurs="0"/>		</xsd:sequence>	</xsd:complexType>	<xsd:element name="BinaryObject" type="pnwwqx:NodeDocument"/>	<xsd:element name="BinaryObjectURL" type="xsd:string"/>	<xsd:element name="BinaryObjectSize" type="xsd:nonNegativeInteger"/>	<xsd:element name="BinaryObjectTitleText" type="xsd:string"/>	<xsd:element name="BinaryObjectCreator" type="xsd:string"/>	<xsd:element name="BinaryObjectSubject" type="xsd:string"/>	<xsd:element name="BinaryObjectDescription" type="xsd:string"/>	<xsd:element name="BinaryObjectPublisher" type="xsd:string"/>	<xsd:element name="BinaryObjectContributor" type="xsd:string"/>	<xsd:element name="BinaryObjectDate" type="xsd:date"/>	<xsd:element name="BinaryObjectType" type="xsd:string"/>	<xsd:element name="BinaryObjectContentTypeText" type="xsd:string"/>	<xsd:element name="BinaryObjectIdentifierText" type="xsd:string"/>	<xsd:element name="BinaryObjectSource" type="xsd:string"/>	<xsd:element name="BinaryObjectLanguage" type="xsd:string"/>	<xsd:element name="BinaryObjectRelation" type="xsd:string"/>	<xsd:element name="BinaryObjectCoverage" type="xsd:string"/>	<xsd:element name="BinaryObjectRights" type="xsd:string"/>	<xsd:complexType name="NodeDocument">		<xsd:sequence>			<xsd:element ref="pnwwqx:Name"/>			<xsd:element ref="pnwwqx:Type"/>			<xsd:element ref="pnwwqx:Content"/>		</xsd:sequence>	</xsd:complexType>	<xsd:element name="Name" type="xsd:string"/>	<xsd:element name="Type" type="xsd:string"/>	<xsd:element name="Content" type="xsd:base64Binary"/></xsd:schema>