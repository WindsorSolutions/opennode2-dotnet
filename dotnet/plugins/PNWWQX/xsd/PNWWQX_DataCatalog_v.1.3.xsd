<?xml version="1.0" encoding="utf-8"?><!-- edited with XMLSPY v2004 rel. 3 U (http://www.xmlspy.com) by Department of Environmental Quality (Department of Environmental Quality) --><xsd:schema targetNamespace="urn:us:net:exchangenetwork" xmlns:pnwwqx="urn:us:net:exchangenetwork" xmlns:xsd="http://www.w3.org/2001/XMLSchema" version="1.3">	<xsd:include schemaLocation=".\PNWWQX_ProvidingOrganizationDetailsType_v.1.3.xsd"/>	<xsd:include schemaLocation=".\PNWWQX_DataAccessType_v.1.3.xsd"/>	<xsd:include schemaLocation=".\PNWWQX_LocationDescriptorContextType_v.1.3.xsd"/>	<xsd:include schemaLocation=".\PNWWQX_LatitudeMeasureType_v.1.3.xsd"/>	<xsd:include schemaLocation=".\PNWWQX_LongitudeMeasureType_v.1.3.xsd"/>	<xsd:include schemaLocation=".\PNWWQX_MediaType_v.1.3.xsd"/>	<xsd:include schemaLocation=".\PNWWQX_StationType_v.1.3.xsd"/>	<xsd:annotation>		<xsd:documentation>		Schema Name : 					PNWWQX_DataCatalog_v.1.3.xsd 		Current Version Available At :			Description : 					This schema defines the overall, high-level descriptive information about the data contained in relevant data sources.		Application : 					Pacific Northwest Water Quality Data Exchange 		Developed by : 	 				Pacific Northwest Exchange States; Windsor Solutions, Inc		Point of Contact : 				Curtis Cude (cude.curtis@deq.state.or.us)										Kevin Jeffery (kevin_jeffery@windsorsolutions.com)			</xsd:documentation>	</xsd:annotation>	<xsd:element name="PNWWQXDataCatalog" type="pnwwqx:PNWWQXDataCatalog"/>	<xsd:complexType name="PNWWQXDataCatalog">		<xsd:sequence>			<xsd:element ref="pnwwqx:PNWWQXDataCatalogList" minOccurs="0" maxOccurs="unbounded"/>		</xsd:sequence>	</xsd:complexType>	<xsd:element name="PNWWQXDataCatalogList" type="pnwwqx:PNWWQXDataCatalogList"/>	<xsd:complexType name="PNWWQXDataCatalogList">		<xsd:sequence>			<xsd:element ref="pnwwqx:ProvidingOrganizationDetails"/>			<xsd:element ref="pnwwqx:DataAccess"/>			<xsd:element ref="pnwwqx:ProjectSummary" maxOccurs="unbounded"/>			<xsd:element ref="pnwwqx:StationSummary" maxOccurs="unbounded"/>			<xsd:element ref="pnwwqx:FieldEventSummary" maxOccurs="unbounded"/>		</xsd:sequence>	</xsd:complexType>	<xsd:element name="ProvidingOrganizationDetails" type="pnwwqx:ProvidingOrganizationDetailsType"/>	<xsd:element name="DataAccess" type="pnwwqx:DataAccessType"/>	<xsd:element name="ProjectSummary" type="pnwwqx:ProjectSummaryType">		<xsd:key name="ProjectKey">			<xsd:selector xpath="pnwwqx:ProjectSummaryType"/>			<xsd:field xpath="pnwwqx:ProjectIdentifier"/>		</xsd:key>	</xsd:element>	<xsd:complexType name="ProjectSummaryType">		<xsd:sequence>			<xsd:element ref="pnwwqx:ProjectIdentifier"/>			<xsd:element ref="pnwwqx:ProjectName"/>			<xsd:element ref="pnwwqx:ProjectStartDate"/>			<xsd:element ref="pnwwqx:ProjectEndDate" minOccurs="0"/>			<xsd:element ref="pnwwqx:ProjectContactOrganizationName"/>		</xsd:sequence>	</xsd:complexType>	<xsd:element name="ProjectIdentifier" type="xsd:string"/>	<xsd:element name="ProjectName" type="xsd:string"/>	<xsd:element name="ProjectStartDate" type="xsd:date"/>	<xsd:element name="ProjectEndDate" type="xsd:date"/>	<xsd:element name="ProjectContactOrganizationName" type="xsd:string"/>	<xsd:element name="StationSummary" type="pnwwqx:StationSummaryType">		<xsd:key name="StationKey">			<xsd:selector xpath="pnwwqx:StationSummaryType"/>			<xsd:field xpath="pnwwqx:StationIdentifier"/>		</xsd:key>	</xsd:element>	<xsd:complexType name="StationSummaryType">		<xsd:sequence>			<xsd:element ref="pnwwqx:StationIdentifier"/>			<xsd:element ref="pnwwqx:StationName"/>			<xsd:element ref="pnwwqx:StationType"/>			<xsd:element ref="pnwwqx:LocationDescriptor" minOccurs="0" maxOccurs="unbounded"/>			<xsd:element ref="pnwwqx:LatitudeMeasure"/>			<xsd:element ref="pnwwqx:LongitudeMeasure"/>			<xsd:element ref="pnwwqx:StationContactOrganizationName" minOccurs="0"/>		</xsd:sequence>	</xsd:complexType>	<xsd:element name="StationIdentifier" type="xsd:string"/>	<xsd:element name="StationName" type="xsd:string"/>	<xsd:element name="StationType" type="pnwwqx:StationType"/>	<xsd:element name="LocationDescriptor" type="pnwwqx:StationLocationDescriptorType"/>	<xsd:element name="LatitudeMeasure" type="pnwwqx:LatitudeMeasureType"/>	<xsd:element name="LongitudeMeasure" type="pnwwqx:LongitudeMeasureType"/>	<xsd:complexType name="StationLocationDescriptorType">		<xsd:sequence>			<xsd:element ref="pnwwqx:LocationDescriptorName"/>			<xsd:element ref="pnwwqx:LocationDescriptorContext"/>		</xsd:sequence>	</xsd:complexType>	<xsd:element name="LocationDescriptorName" type="xsd:string"/>	<xsd:element name="LocationDescriptorContext" type="pnwwqx:LocationDescriptorContextType"/>	<xsd:element name="StationContactOrganizationName" type="xsd:string"/>	<xsd:element name="FieldEventSummary" type="pnwwqx:FieldEventSummaryType">		<xsd:keyref name="FieldEventProjectKey" refer="pnwwqx:ProjectKey">			<xsd:selector xpath="pnwwqx:FieldEventDetails"/>			<xsd:field xpath="pnwwqx:FieldEventProjectIdentifier"/>		</xsd:keyref>		<xsd:keyref name="FieldEventStationKey" refer="pnwwqx:StationKey">			<xsd:selector xpath="pnwwqx:FieldEventDetails"/>			<xsd:field xpath="pnwwqx:FieldEventStationIdentifier"/>		</xsd:keyref>	</xsd:element>	<xsd:complexType name="FieldEventSummaryType">		<xsd:sequence>			<xsd:element ref="pnwwqx:MinFieldEventStartDate"/>			<xsd:element ref="pnwwqx:MaxFieldEventEndDate"/>			<xsd:element ref="pnwwqx:MediaText"/>			<xsd:element ref="pnwwqx:BiologicalSystematicName" minOccurs="0"/>			<xsd:element ref="pnwwqx:AnalyteName"/>			<xsd:element ref="pnwwqx:NumberResults"/>			<xsd:element ref="pnwwqx:FieldEventProjectIdentifier"/>			<xsd:element ref="pnwwqx:FieldEventStationIdentifier"/>			<xsd:element ref="pnwwqx:FieldEventContactOrganizationName" minOccurs="0"/>		</xsd:sequence>	</xsd:complexType>	<xsd:element name="MinFieldEventStartDate" type="xsd:date"/>	<xsd:element name="MaxFieldEventEndDate" type="xsd:date"/>	<xsd:element name="MediaText" type="pnwwqx:MediaType"/>	<xsd:element name="BiologicalSystematicName" type="xsd:string"/>	<xsd:element name="AnalyteName" type="xsd:string"/>	<xsd:element name="NumberResults" type="xsd:nonNegativeInteger"/>	<xsd:element name="FieldEventProjectIdentifier" type="xsd:string"/>	<xsd:element name="FieldEventStationIdentifier" type="xsd:string"/>	<xsd:element name="FieldEventContactOrganizationName" type="xsd:string"/></xsd:schema>