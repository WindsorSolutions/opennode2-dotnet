<?xml version="1.0" encoding="utf-8"?><xsd:schema targetNamespace="urn:us:net:exchangenetwork" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pnwwqx="urn:us:net:exchangenetwork" version="1.3">	<xsd:annotation>		<xsd:documentation>		Schema Name : 					PNWWQX_WellDetailType_v.1.3.xsd 		Current Version Available At :			Description : 					This schema defines the data elements to be shared through the Pacific Northwest Water Quality Data Exchange related to a well used to monitor or measure water quality.		Application : 					Pacific Northwest Water Quality Data Exchange 		Developed by : 	 				Pacific Northwest Exchange States; Windsor Solutions, Inc		Point of Contact : 				Curtis Cude (cude.curtis@deq.state.or.us)										Kevin Jeffery (kevin_jeffery@windsorsolutions.com)		</xsd:documentation>	</xsd:annotation>	<xsd:complexType name="WellDetailType">		<xsd:sequence>			<xsd:element ref="pnwwqx:WellIdentifier"/>			<xsd:element ref="pnwwqx:WellIdentifierContext"/>			<xsd:element ref="pnwwqx:WellDepthCompletionMeasure" minOccurs="0"/>			<xsd:element ref="pnwwqx:WellOpenIntervalType" minOccurs="0"/>			<xsd:element ref="pnwwqx:WellDepthTopOpenIntervalMeasure" minOccurs="0"/>			<xsd:element ref="pnwwqx:WellDepthBottomOpenIntervalMeasure" minOccurs="0"/>			<xsd:element ref="pnwwqx:WellDepthUnitMeasureName" minOccurs="0"/>			<xsd:element ref="pnwwqx:WellDiameterMeasure" minOccurs="0"/>			<xsd:element ref="pnwwqx:WellDiameterUnitMeasureName" minOccurs="0"/>			<xsd:element ref="pnwwqx:WellStatus"/>			<xsd:element ref="pnwwqx:WellUse"/>		</xsd:sequence>	</xsd:complexType>	<xsd:element name="WellIdentifier" type="xsd:string"/>	<xsd:element name="WellIdentifierContext" type="xsd:string"/>	<xsd:element name="WellDepthCompletionMeasure" type="xsd:decimal"/>	<xsd:element name="WellOpenIntervalType" type="xsd:string"/>	<xsd:element name="WellDepthTopOpenIntervalMeasure" type="xsd:decimal"/>	<xsd:element name="WellDepthBottomOpenIntervalMeasure" type="xsd:decimal"/>	<xsd:element name="WellDepthUnitMeasureName" type="xsd:string"/>	<xsd:element name="WellDiameterMeasure" type="xsd:decimal"/>	<xsd:element name="WellDiameterUnitMeasureName" type="xsd:string"/>	<xsd:element name="WellStatus" type="pnwwqx:WellStatusType"/>	<xsd:element name="WellUse" type="pnwwqx:WellUseType"/>	<xsd:simpleType name="WellStatusType">		<xsd:restriction base="xsd:string">			<xsd:enumeration value="Active"/>			<xsd:enumeration value="Abandoned"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="WellUseType">		<xsd:restriction base="xsd:string">			<xsd:enumeration value="Withdrawal of Water"/>			<xsd:enumeration value="Waste Injection - Non Specific"/>			<xsd:enumeration value="EPA Waste Injection: Class I"/>			<xsd:enumeration value="EPA Waste Injection: Class II"/>			<xsd:enumeration value="EPA Waste Injection: Class III"/>			<xsd:enumeration value="EPA Waste Injection: Class IV"/>			<xsd:enumeration value="EPA Waste Injection: Class V"/>			<xsd:enumeration value="Standby Emergency Supply"/>			<xsd:enumeration value="Test"/>			<xsd:enumeration value="Observation"/>			<xsd:enumeration value="Monitoring"/>			<xsd:enumeration value="Geothermal"/>			<xsd:enumeration value="Heat Reservoir"/>			<xsd:enumeration value="Drainage"/>			<xsd:enumeration value="Seismic"/>			<xsd:enumeration value="Oil or Gas"/>			<xsd:enumeration value="Mine"/>			<xsd:enumeration value="Recharge"/>			<xsd:enumeration value="Repressurization"/>			<xsd:enumeration value="Anode"/>			<xsd:enumeration value="Unused"/>			<xsd:enumeration value="Destroyed"/>			<xsd:enumeration value="Other"/>			<xsd:enumeration value="Unknown"/>		</xsd:restriction>	</xsd:simpleType></xsd:schema>