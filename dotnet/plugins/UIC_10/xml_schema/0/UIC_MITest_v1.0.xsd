<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:uic="http://www.exchangenetwork.net/schema/uic/1" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.exchangenetwork.net/schema/uic/1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<xsd:include schemaLocation="UIC_SimpleContent_v1.0.xsd"/>
	<xsd:annotation>
		<xsd:documentation>
        Schema Name  : UIC_MITest_v1.0.xsd
        Description   : Information about mechanical integrity (MI) test of wells.
        Developed by  : 	 Lockheed Martin
        Point of Contact :   Lockheed Martin
</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="MITestDetailType">
		<xsd:annotation>
			<xsd:documentation>Defines the MI Test information structure.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="uic:MechanicalIntegrityTestIdentifier"/>
			<xsd:element ref="uic:MechanicalIntegrityTestCompletedDate"/>
			<xsd:element ref="uic:MechanicalIntegrityTestResultCode"/>
			<xsd:element ref="uic:MechanicalIntegrityTestTypeCode"/>
			<xsd:element ref="uic:MechanicalIntegrityTestRemedialActionDate" minOccurs="0"/>
			<xsd:element ref="uic:MechanicalIntegrityTestRemedialActionTypeCode" minOccurs="0"/>
			<xsd:element ref="uic:MechanicalIntegrityTestWellIdentifier"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="MITestDetail" type="uic:MITestDetailType"/>
</xsd:schema>
