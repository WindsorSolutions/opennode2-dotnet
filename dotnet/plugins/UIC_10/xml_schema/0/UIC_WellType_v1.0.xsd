	<xsd:annotation>
		<xsd:documentation>
        Schema Name  : UIC_WellType_v1.0.xsd
        Description   : indicates the current and/or historical well type.
        Developed by  : 	 Lockheed Martin
        Point of Contact :   Lockheed Martin
</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="WellTypeDetailDataType">
