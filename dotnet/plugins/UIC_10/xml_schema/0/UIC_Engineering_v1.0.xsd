<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:uic="http://www.exchangenetwork.net/schema/uic/1" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.exchangenetwork.net/schema/uic/1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<xsd:include schemaLocation="UIC_SimpleContent_v1.0.xsd"/>
	<xsd:annotation>
		<xsd:documentation>
        Schema Name  : UIC_Engineering_v1.0.xsd
        Description   : Information on Class I and deep well injection operational parameters that measure volumes of fluid going down a well. 
        Developed by  : 	 Lockheed Martin
        Point of Contact :   Lockheed Martin
</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="EngineeringDetailType">
		<xsd:annotation>
			<xsd:documentation>Defines the Engineering information structure.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="uic:EngineeringIdentifier"/>
			<xsd:element ref="uic:EngineeringMaximumFlowRateNumeric" minOccurs="0"/>
			<xsd:element ref="uic:EngineeringPermittedOnsiteInjectionVolumeNumeric" minOccurs="0"/>
			<xsd:element ref="uic:EngineeringPermittedOffsiteInjectionVolumeNumeric" minOccurs="0"/>
			<xsd:element ref="uic:EngineeringWellIdentifier"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="EngineeringDetail" type="uic:EngineeringDetailType"/>
</xsd:schema>
