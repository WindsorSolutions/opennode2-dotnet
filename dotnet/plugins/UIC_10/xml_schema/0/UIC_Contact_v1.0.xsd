<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:uic="http://www.exchangenetwork.net/schema/uic/1" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.exchangenetwork.net/schema/uic/1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<xsd:include schemaLocation="UIC_SimpleContent_v1.0.xsd"/>
	<xsd:annotation>
		<xsd:documentation>
        Schema Name  : UIC_Contact_v1.0.xsd
        Description   : the contact information for Injection wells.
        Developed by  : 	 Lockheed Martin
        Point of Contact :   Lockheed Martin
</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="ContactDetailType">
		<xsd:annotation>
			<xsd:documentation>Defines the Owner Operator information structure.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="uic:ContactIdentifier"/>
			<xsd:element ref="uic:TelephoneNumberText" minOccurs="0"/>
			<xsd:element ref="uic:IndividualFullName"/>
			<xsd:element ref="uic:ContactCityName" minOccurs="0"/>
			<xsd:element ref="uic:ContactAddressStateCode" minOccurs="0"/>
			<xsd:element ref="uic:ContactAddressText"/>
			<xsd:element ref="uic:ContactAddressPostalCode"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="ContactDetail" type="uic:ContactDetailType"/>
</xsd:schema>
