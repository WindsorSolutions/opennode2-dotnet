<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:facid="http://www.exchangenetwork.net/schema/facilityid/3" targetNamespace="http://www.exchangenetwork.net/schema/facilityid/3" elementFormDefault="qualified" attributeFormDefault="unqualified" version="3.0">
	<xsd:include schemaLocation="FACID_DataSource_v3.0_draft1.xsd"/>
	<xsd:include schemaLocation="FACID_SimpleContent_v3.0_draft1.xsd"/>
	<xsd:annotation>
		<xsd:documentation>
			Schema Name : 	FACID_EnvironmentalInterestSummaryDataType_v3.0 
			Current Version Available At : http://www.exchangenetwork.net/
			Description : 	The details of an environmental permit or regulatory program that applies to a facility site.
			Application : 
			Developed by : 	Environmental Protection Agency, Windsor Solutions, Inc.
			Point of Contact : Kevin Lyons
		</xsd:documentation>
	</xsd:annotation>
	<xsd:element name="EnvironmentalInterestSummary" type="facid:EnvironmentalInterestSummaryDataType">
		<xsd:annotation>
			<xsd:documentation>A summary of an environmental permit or regulatory program that applies to a facility site.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="EnvironmentalInterestSummaryDataType">
		<xsd:sequence>
			<xsd:element ref="facid:DataSource"/>
			<xsd:element ref="facid:EnvironmentalInterestIdentifier"/>
			<xsd:element ref="facid:EnvironmentalInterestTypeText"/>
			<xsd:element ref="facid:EnvironmentalInterestURLText" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
