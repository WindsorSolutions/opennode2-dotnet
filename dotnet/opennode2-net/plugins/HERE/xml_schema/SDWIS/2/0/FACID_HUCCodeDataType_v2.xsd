<?xml version="1.0" encoding="utf-8"?> 
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
<xsd:annotation>
	<xsd:appinfo>HUCCodeDataType_v2.xsd v2.0 2001-02 </xsd:appinfo>
	<xsd:documentation>
		Data Type for HUC Code.
	</xsd:documentation>
</xsd:annotation>
<xsd:simpleType name="HUCCodeDataType">
	<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9]{8}"/>
			<xsd:length value="8"/>
	</xsd:restriction>
</xsd:simpleType>
</xsd:schema>