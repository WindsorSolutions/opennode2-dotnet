<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:icis="http://www.exchangenetwork.net/schema/icis/3" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.exchangenetwork.net/schema/icis/3" elementFormDefault="qualified" attributeFormDefault="unqualified" version="3">
	<xsd:include schemaLocation="ICIS_KeyElements_v3.1.xsd"/>
	<xsd:include schemaLocation="ICIS_EnforcementActionViolationLinkage_v3.1.xsd"/>
	<xsd:annotation>
		<xsd:documentation>Schema Name: ICIS_FinalOrderViolationLinkage_v3.1.xsd</xsd:documentation>
		<xsd:documentation>Current Version Available: http://www.exchangenetwork.net/schema/icis/3</xsd:documentation>
		<xsd:documentation>Description:enforcement action violation linkage data input format</xsd:documentation>
		<xsd:documentation>Application: ICIS-NPDES</xsd:documentation>
		<xsd:documentation>Developed By: U.S. Environmental Protection Agency</xsd:documentation>
		<xsd:documentation>Point of Contact: EPA/Alison Kittle</xsd:documentation>
		<xsd:documentation>Data Written: July 2006</xsd:documentation>
		<xsd:documentation>Last Date Modified: 2012-02-16</xsd:documentation>
	</xsd:annotation>
	<xsd:element name="FinalOrderViolationLinkage" type="icis:FinalOrderViolationLinkage"/>
	<xsd:complexType name="FinalOrderViolationLinkageData">
		<xsd:sequence>
			<xsd:element ref="icis:TransactionHeader"/>
			<xsd:element ref="icis:FinalOrderViolationLinkage"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="FinalOrderViolationLinkage">
		<xsd:complexContent>
			<xsd:extension base="icis:EnforcementActionKeyElements">
				<xsd:sequence>
					<xsd:element ref="icis:FinalOrderIdentifier"/>
					<xsd:choice>
						<xsd:element ref="icis:PermitScheduleViolation"/>
						<xsd:element ref="icis:ComplianceScheduleViolation"/>
						<xsd:element ref="icis:DischargeMonitoringReportViolation"/>
						<xsd:element ref="icis:DischargeMonitoringReportParameterViolation"/>
						<xsd:element ref="icis:SingleEventsViolation"/>
					</xsd:choice>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
</xsd:schema>
