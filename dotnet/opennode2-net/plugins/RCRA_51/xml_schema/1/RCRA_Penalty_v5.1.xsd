<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:RC="http://www.exchangenetwork.net/schema/RCRA/5" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.exchangenetwork.net/schema/RCRA/5" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="5.1">
	<xsd:include schemaLocation="./RCRA_Shared_v5.1.xsd"/>
	<xsd:include schemaLocation="./RCRA_Payment_v5.1.xsd"/>
	<xsd:annotation>
		<xsd:documentation>
		Schema Name : 	RCRA_Penalty_v5.1
		Description : 	
		Developed by : 	U.S. EPA
	</xsd:documentation>
	</xsd:annotation>
	<xsd:element name="Penalty" type="RC:PenaltyDataType">
		<xsd:annotation>
			<xsd:documentation>Compliance Monitoring and Enforcement Penalty Data</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="PenaltyDataType">
		<xsd:sequence>
			<xsd:element ref="RC:TransactionCode" minOccurs="0"/>
			<xsd:element ref="RC:PenaltyTypeOwner"/>
			<xsd:element ref="RC:PenaltyType"/>
			<xsd:element ref="RC:CashCivilPenaltySoughtAmount" minOccurs="0"/>
			<xsd:element ref="RC:Notes" minOccurs="0"/>
			<xsd:element ref="RC:Payment" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
