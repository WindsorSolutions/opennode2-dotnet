<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:RC="http://www.exchangenetwork.net/schema/RCRA/5" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.exchangenetwork.net/schema/RCRA/5" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="5.1">
	<xsd:include schemaLocation="./RCRA_Shared_v5.1.xsd"/>
	<xsd:include schemaLocation="./RCRA_PermitRelatedEvent_v5.1.xsd"/>
	<xsd:include schemaLocation="./RCRA_HandlerWasteCodeDetails_v5.1.xsd"/>
	<xsd:annotation>
		<xsd:documentation>
		Schema Name : 	RCRA_DetailPermitEvent_v5.1
		Description : 	Permit Unit Detail Data
		Developed by : 	U.S. EPA
	</xsd:documentation>
	</xsd:annotation>
	<xsd:element name="PermitUnitDetail" type="RC:PermitUnitDetailDataType">
		<xsd:annotation>
			<xsd:documentation>Permit Unit Detail Data</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="PermitUnitDetailDataType">
		<xsd:annotation>
			<xsd:documentation>Characteristics of process unit group details.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RC:TransactionCode" minOccurs="0"/>
			<xsd:element ref="RC:PermitUnitDetailSequenceNumber"/>
			<xsd:element ref="RC:ProcessUnitDataOwnerCode" minOccurs="0"/>
			<xsd:element ref="RC:ProcessUnitCode" minOccurs="0"/>
			<xsd:element ref="RC:PermitStatusEffectiveDate" minOccurs="0"/>
			<xsd:element ref="RC:PermitUnitCapacityQuantity" minOccurs="0"/>
			<xsd:element ref="RC:CapacityTypeCode" minOccurs="0"/>
			<xsd:element ref="RC:CommercialStatusCode" minOccurs="0"/>
			<xsd:element ref="RC:LegalOperatingStatusDataOwnerCode" minOccurs="0"/>
			<xsd:element ref="RC:LegalOperatingStatusCode" minOccurs="0"/>
			<xsd:element ref="RC:MeasurementUnitDataOwnerCode" minOccurs="0"/>
			<xsd:element ref="RC:MeasurementUnitCode" minOccurs="0"/>
			<xsd:element ref="RC:NumberOfUnitsCount" minOccurs="0"/>
			<xsd:element ref="RC:StandardPermitIndicator" minOccurs="0"/>
			<xsd:element ref="RC:SupplementalInformationText" minOccurs="0"/>
			<xsd:element ref="RC:PermitRelatedEvent" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="RC:HandlerWasteCodeDetails" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
