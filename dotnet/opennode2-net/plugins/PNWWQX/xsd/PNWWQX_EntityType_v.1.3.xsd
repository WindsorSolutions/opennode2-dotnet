<?xml version="1.0" encoding="utf-8"?><xsd:schema targetNamespace="urn:us:net:exchangenetwork" xmlns:pnwwqx="urn:us:net:exchangenetwork" xmlns:xsd="http://www.w3.org/2001/XMLSchema" version="1.3">	<xsd:annotation>		<xsd:documentation>		Schema Name : 					PNWWQX_EntityType_v.1.3.xsd 		Current Version Available At :			Description : 					This schema defines the values for the entity type element used to characterize the type of an organization or individual.		Application : 					Pacific Northwest Water Quality Data Exchange 		Developed by : 	 				Pacific Northwest Exchange States; Windsor Solutions, Inc		Point of Contact : 				Curtis Cude (cude.curtis@deq.state.or.us)										Kevin Jeffery (kevin_jeffery@windsorsolutions.com)		</xsd:documentation>	</xsd:annotation>	<xsd:simpleType name="EntityType">		<xsd:restriction base="xsd:string">			<xsd:enumeration value="Federal"/>			<xsd:enumeration value="Tribal"/>			<xsd:enumeration value="State or Provincial"/>			<xsd:enumeration value="Local"/>			<xsd:enumeration value="Public or Volunteer"/>			<xsd:enumeration value="Non-profit"/>			<xsd:enumeration value="Educational"/>			<xsd:enumeration value="Commercial"/>			<xsd:enumeration value="Other"/>		</xsd:restriction>	</xsd:simpleType></xsd:schema>