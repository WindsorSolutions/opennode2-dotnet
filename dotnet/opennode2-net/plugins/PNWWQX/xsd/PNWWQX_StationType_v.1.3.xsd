<?xml version="1.0" encoding="utf-8"?><xsd:schema targetNamespace="urn:us:net:exchangenetwork" xmlns:pnwwqx="urn:us:net:exchangenetwork" xmlns:xsd="http://www.w3.org/2001/XMLSchema" version="1.3">	<xsd:annotation>		<xsd:documentation>		Schema Name : 					PNWWQX_StationType_v.1.3.xsd 		Current Version Available At :			Description : 					This schema defines the values for the station type element used to characterize a monitoring station.		Application : 					Pacific Northwest Water Quality Data Exchange 		Developed by : 	 				Pacific Northwest Exchange States; Windsor Solutions, Inc		Point of Contact : 				Curtis Cude (cude.curtis@deq.state.or.us)										Kevin Jeffery (kevin_jeffery@windsorsolutions.com)		</xsd:documentation>	</xsd:annotation>	<xsd:simpleType name="StationType">		<xsd:restriction base="xsd:string">			<xsd:enumeration value="Estuary"/>			<xsd:enumeration value="Groundwater"/>			<xsd:enumeration value="Intertidal"/>			<xsd:enumeration value="Lake or Pond"/>			<xsd:enumeration value="Reservoir or Quarry"/>			<xsd:enumeration value="Land"/>			<xsd:enumeration value="Ocean"/>			<xsd:enumeration value="Riparian Zone"/>			<xsd:enumeration value="Source"/>			<xsd:enumeration value="Spring"/>			<xsd:enumeration value="Stream or River"/>			<xsd:enumeration value="Wetland"/>			<xsd:enumeration value="Other"/>		</xsd:restriction>	</xsd:simpleType></xsd:schema>