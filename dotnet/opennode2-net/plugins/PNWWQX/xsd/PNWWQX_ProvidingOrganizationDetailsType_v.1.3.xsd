<?xml version="1.0" encoding="utf-8"?><xsd:schema targetNamespace="urn:us:net:exchangenetwork" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pnwwqx="urn:us:net:exchangenetwork" version="1.3">	<xsd:include schemaLocation=".\PNWWQX_MailingAddressType_v.1.3.xsd"/>	<xsd:include schemaLocation=".\PNWWQX_PhoneEmailType_v.1.3.xsd"/>	<xsd:include schemaLocation=".\PNWWQX_EntityType_v.1.3.xsd"/>	<xsd:annotation>		<xsd:documentation>		Schema Name : 					PNWWQX_ProvidingOrganizationDetailsType_v.1.3.xsd 		Current Version Available At :			Description : 					This schema defines the data elements to be shared through the Pacific Northwest Water Quality Data Exchange related to the organization providing a given set of water quality monitoring information.		Application : 					Pacific Northwest Water Quality Data Exchange 		Developed by : 	 				Pacific Northwest Exchange States; Windsor Solutions, Inc		Point of Contact : 				Curtis Cude (cude.curtis@deq.state.or.us)										Kevin Jeffery (kevin_jeffery@windsorsolutions.com)			</xsd:documentation>	</xsd:annotation>	<xsd:complexType name="ProvidingOrganizationDetailsType">		<xsd:sequence>			<xsd:element ref="pnwwqx:ProvidingOrganizationIdentifier"/>			<xsd:element ref="pnwwqx:ProvidingOrganizationName"/>			<xsd:element ref="pnwwqx:ProvidingOrganizationType"/>			<xsd:element ref="pnwwqx:ProvidingOrganizationContactName"/>			<xsd:element ref="pnwwqx:ProvidingOrganizationMailingAddress" minOccurs="0"/>			<xsd:element ref="pnwwqx:ProvidingOrganizationPhoneEmail"/>			<xsd:element ref="pnwwqx:ProvidingOrganizationURL" minOccurs="0"/>		</xsd:sequence>	</xsd:complexType>	<xsd:element name="ProvidingOrganizationIdentifier" type="xsd:string"/>	<xsd:element name="ProvidingOrganizationName" type="xsd:string"/>	<xsd:element name="ProvidingOrganizationType" type="pnwwqx:EntityType"/>	<xsd:element name="ProvidingOrganizationContactName" type="xsd:string"/>	<xsd:element name="ProvidingOrganizationMailingAddress" type="pnwwqx:MailingAddressType"/>	<xsd:element name="ProvidingOrganizationPhoneEmail" type="pnwwqx:PhoneEmailType"/>	<xsd:element name="ProvidingOrganizationURL" type="xsd:string"/></xsd:schema>