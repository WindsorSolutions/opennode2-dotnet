<?xml version="1.0" encoding="utf-8"?><xsd:schema targetNamespace="urn:us:net:exchangenetwork" xmlns:pnwwqx="urn:us:net:exchangenetwork" xmlns:xsd="http://www.w3.org/2001/XMLSchema" version="1.3">	<xsd:include schemaLocation=".\PNWWQX_MethodType_v.1.3.xsd"/>	<xsd:include schemaLocation=".\PNWWQX_MediaType_v.1.3.xsd"/>	<xsd:include schemaLocation=".\PNWWQX_TaxonType_v.1.3.xsd"/>	<xsd:annotation>		<xsd:documentation>		Schema Name : 					PNWWQX_FieldEventDetailsType_v.1.3.xsd 		Current Version Available At :			Description : 					This schema defines the data elements to be shared through the Pacific Northwest Water Quality Data Exchange related to a field activity used to monitor or measure water quality.		Application : 					Pacific Northwest Water Quality Data Exchange 		Developed by : 	 				Pacific Northwest Exchange States; Windsor Solutions, Inc		Point of Contact : 				Curtis Cude (cude.curtis@deq.state.or.us)										Kevin Jeffery (kevin_jeffery@windsorsolutions.com)			</xsd:documentation>	</xsd:annotation>	<xsd:complexType name="FieldEventDetailsType">		<xsd:sequence>			<xsd:element ref="pnwwqx:FieldEventStartDate"/>			<xsd:element ref="pnwwqx:FieldEventStartTimeZone"/>			<xsd:element ref="pnwwqx:FieldEventEndDate"/>			<xsd:element ref="pnwwqx:FieldEventEndTimeZone"/>			<xsd:element ref="pnwwqx:FieldEventTypeText"/>			<xsd:element ref="pnwwqx:SampleIdentificationText" minOccurs="0"/>			<xsd:element ref="pnwwqx:MediaText"/>			<xsd:element ref="pnwwqx:Taxon" minOccurs="0"/>			<xsd:element ref="pnwwqx:FieldEventUpperDepthMeasure" minOccurs="0"/>			<xsd:element ref="pnwwqx:FieldEventLowerDepthMeasure" minOccurs="0"/>			<xsd:element ref="pnwwqx:FieldEventDepthUnitMeasureName" minOccurs="0"/>			<xsd:element ref="pnwwqx:FieldEventDepthComment" minOccurs="0"/>			<xsd:element ref="pnwwqx:FieldEventNotes" minOccurs="0"/>			<xsd:element ref="pnwwqx:SampleCollectionMethod"/>			<xsd:element ref="pnwwqx:SamplePreparationMethod" minOccurs="0" maxOccurs="unbounded"/>			<xsd:element ref="pnwwqx:SamplePreservationMethod" maxOccurs="unbounded"/>		</xsd:sequence>	</xsd:complexType>	<xsd:element name="FieldEventStartDate" type="xsd:dateTime"/>	<xsd:element name="FieldEventStartTimeZone" type="pnwwqx:TimeZoneCodeType"/>	<xsd:element name="FieldEventEndDate" type="xsd:dateTime"/>	<xsd:element name="FieldEventEndTimeZone" type="pnwwqx:TimeZoneCodeType"/>	<xsd:element name="FieldEventTypeText" type="pnwwqx:ActivityType"/>	<xsd:element name="SampleIdentificationText" type="xsd:string"/>	<xsd:element name="MediaText" type="pnwwqx:MediaType"/>	<xsd:element name="Taxon" type="pnwwqx:TaxonType"/>	<xsd:element name="FieldEventUpperDepthMeasure" type="xsd:decimal"/>	<xsd:element name="FieldEventLowerDepthMeasure" type="xsd:decimal"/>	<xsd:element name="FieldEventDepthUnitMeasureName" type="xsd:string"/>	<xsd:element name="FieldEventDepthComment" type="xsd:string"/>	<xsd:element name="FieldEventNotes" type="xsd:string"/>	<xsd:element name="SampleCollectionMethod" type="pnwwqx:MethodType"/>	<xsd:element name="SamplePreparationMethod" type="pnwwqx:MethodType"/>	<xsd:element name="SamplePreservationMethod" type="pnwwqx:MethodType"/>	<xsd:simpleType name="TimeZoneCodeType">		<xsd:restriction base="xsd:string">			<xsd:enumeration value="AST"/>			<xsd:enumeration value="ADT"/>			<xsd:enumeration value="EST"/>			<xsd:enumeration value="EDT"/>			<xsd:enumeration value="CST"/>			<xsd:enumeration value="CDT"/>			<xsd:enumeration value="MST"/>			<xsd:enumeration value="MDT"/>			<xsd:enumeration value="PST"/>			<xsd:enumeration value="PDT"/>			<xsd:enumeration value="KST"/>			<xsd:enumeration value="KDT"/>			<xsd:enumeration value="HST"/>			<xsd:enumeration value="HDT"/>			<xsd:enumeration value="SST"/>			<xsd:enumeration value="SDT"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="ActivityType">		<xsd:restriction base="xsd:string">			<xsd:enumeration value="Routine In Situ Measurement/Observation"/>			<xsd:enumeration value="Replicate In Situ Measurement/Observation"/>			<xsd:enumeration value="Sample -- Routine Sample"/>			<xsd:enumeration value="Sample -- Field Blank"/>			<xsd:enumeration value="Sample -- Field Replicate"/>			<xsd:enumeration value="Sample -- Depletion Replicate"/>			<xsd:enumeration value="Sample -- Integrated Time Series"/>			<xsd:enumeration value="Sample -- Integrate Flow Proportioned"/>			<xsd:enumeration value="Sample -- Integrate Horizontal Profile"/>			<xsd:enumeration value="Sample -- Integrated Vertical Profile"/>			<xsd:enumeration value="Sample -- Composite Without Parents"/>			<xsd:enumeration value="Sample -- Positive Control"/>			<xsd:enumeration value="Sample -- Negative Control"/>			<xsd:enumeration value="Sample -- Grab"/>			<xsd:enumeration value="Sample -- Confirmation"/>			<xsd:enumeration value="Sample -- Repeat"/>			<xsd:enumeration value="Sample -- Noncompliance"/>			<xsd:enumeration value="Sample -- Other"/>			<xsd:enumeration value="Sample -- Continuous Monitoring Routine Sample"/>			<xsd:enumeration value="Sample -- Continuous Monitoring Field Replicate"/>			<xsd:enumeration value="Sample Created from Sample"/>			<xsd:enumeration value="Composite Sample with Parents"/>			<xsd:enumeration value="QC Sample -- Trip blank"/>			<xsd:enumeration value="QC Sample -- Reagent Blank"/>			<xsd:enumeration value="QC Sample -- Equipment Blank"/>			<xsd:enumeration value="QC Sample -- Pre-preservative Blank"/>			<xsd:enumeration value="QC Sample -- Post-preservative Blank"/>			<xsd:enumeration value="QC Sample -- Field Spike"/>			<xsd:enumeration value="QC Sample -- Field Blank"/>			<xsd:enumeration value="QC Sample -- Reference Sample"/>			<xsd:enumeration value="QC Sample -- Measurement Precision Sample"/>			<xsd:enumeration value="QC Sample -- Blind Duplicate"/>			<xsd:enumeration value="QC Sample -- Inter-lab split"/>			<xsd:enumeration value="QC Sample -- Other"/>		</xsd:restriction>	</xsd:simpleType></xsd:schema>