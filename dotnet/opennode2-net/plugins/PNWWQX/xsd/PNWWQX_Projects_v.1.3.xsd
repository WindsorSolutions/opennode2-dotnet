<?xml version="1.0" encoding="utf-8"?><!-- edited with XMLSPY v2004 rel. 3 U (http://www.xmlspy.com) by Department of Environmental Quality (Department of Environmental Quality) --><xsd:schema targetNamespace="urn:us:net:exchangenetwork" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pnwwqx="urn:us:net:exchangenetwork" version="1.3">	<xsd:include schemaLocation=".\PNWWQX_ProvidingOrganizationDetailsType_v.1.3.xsd"/>	<xsd:include schemaLocation=".\PNWWQX_ProjectDetailsType_v.1.3.xsd"/>	<xsd:include schemaLocation=".\PNWWQX_BinaryLargeObjectType_v.1.3.xsd"/>	<xsd:include schemaLocation=".\PNWWQX_AnalyteType_v.1.3.xsd"/>	<xsd:include schemaLocation=".\PNWWQX_ContactEntityDetailsType_v.1.3.xsd"/>	<xsd:annotation>		<xsd:documentation>		Schema Name : 					PNWWQX_Projects_v.1.3.xsd 		Current Version Available At :			Description : 					This schema defines the data elements to be shared through the Pacific Northwest Water Quality Data Exchange related to a monitoring project designed to measure and assess water quality.		Application : 					Pacific Northwest Water Quality Data Exchange 		Developed by : 	 				Pacific Northwest Exchange States; Windsor Solutions, Inc		Point of Contact : 				Curtis Cude (cude.curtis@deq.state.or.us)										Kevin Jeffery (kevin_jeffery@windsorsolutions.com)			</xsd:documentation>	</xsd:annotation>	<xsd:element name="PNWWQXProjects" type="pnwwqx:PNWWQXProjects"/>	<xsd:complexType name="PNWWQXProjects">		<xsd:sequence>			<xsd:element ref="pnwwqx:PNWWQXProjectsList" minOccurs="0" maxOccurs="unbounded"/>		</xsd:sequence>	</xsd:complexType>	<xsd:element name="PNWWQXProjectsList" type="pnwwqx:PNWWQXProjectsList"/>	<xsd:complexType name="PNWWQXProjectsList">		<xsd:sequence>			<xsd:element ref="pnwwqx:ProvidingOrganizationDetails"/>			<xsd:element ref="pnwwqx:ProjectDetails" maxOccurs="unbounded"/>			<xsd:element ref="pnwwqx:ContactEntityDetails" maxOccurs="unbounded"/>		</xsd:sequence>	</xsd:complexType>	<xsd:element name="ProvidingOrganizationDetails" type="pnwwqx:ProvidingOrganizationDetailsType"/>	<xsd:element name="ProjectDetails" type="pnwwqx:ProjectDetails">		<xsd:keyref name="ProjectContactKey" refer="pnwwqx:ContactEntityKey">			<xsd:selector xpath="pnwwqx:ProjectDetails"/>			<xsd:field xpath="pnwwqx:ProjectContactIdentifier"/>		</xsd:keyref>	</xsd:element>	<xsd:complexType name="ProjectDetails">		<xsd:sequence>			<xsd:element ref="pnwwqx:ProjectDetailsType"/>			<xsd:element ref="pnwwqx:ProjectBinaryLargeObject" minOccurs="0" maxOccurs="unbounded"/>			<xsd:element ref="pnwwqx:ProjectContact" maxOccurs="unbounded"/>			<xsd:element ref="pnwwqx:Analyte" minOccurs="0" maxOccurs="unbounded"/>		</xsd:sequence>	</xsd:complexType>	<xsd:element name="ProjectDetailsType" type="pnwwqx:ProjectDetailsType"/>	<xsd:element name="ProjectBinaryLargeObject" type="pnwwqx:BinaryLargeObjectType"/>	<xsd:element name="ProjectContact" type="pnwwqx:ProjectContactType"/>	<xsd:complexType name="ProjectContactType">		<xsd:sequence>			<xsd:element ref="pnwwqx:ProjectContactIdentifier"/>			<xsd:element ref="pnwwqx:PrimaryDataSourceIndicator"/>		</xsd:sequence>	</xsd:complexType>	<xsd:element name="ProjectContactIdentifier" type="xsd:string"/>	<xsd:element name="PrimaryDataSourceIndicator" type="xsd:boolean"/>	<xsd:element name="Analyte" type="pnwwqx:AnalyteType"/>	<xsd:element name="ContactEntityDetails" type="pnwwqx:ContactEntityDetailsType">		<xsd:key name="ContactEntityKey">			<xsd:selector xpath="pnwwqx:ContactEntityDetailsType"/>			<xsd:field xpath="pnwwqx:ContactEntityIdentifier"/>		</xsd:key>	</xsd:element></xsd:schema>