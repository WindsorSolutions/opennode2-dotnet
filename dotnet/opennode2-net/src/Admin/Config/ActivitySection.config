<?xml version="1.0" encoding="utf-8"?>
<objects xmlns="http://www.springframework.net">

  <!-- ActivitySearchCriteria -->
  <object type="~/Secure/ActivitySearchCriteria.aspx">
    <property name="SectionTitle" value="Activity Log Viewer"/>
    <property name="SectionHeadline" value="Activity Search Criteria"/>
    <property name="IntroParagraphs">
      <list>
        <value>This screen allows you to search for activity on the Node log using a variety of criteria.</value>
      </list>
    </property>
    <property name="DataService" ref="activityManagerService"/>
  </object>

  <!-- Transaction -->
  <object type="~/Secure/Transaction.aspx">
    <property name="SectionTitle" value="Node Transaction"/>
    <property name="SectionHeadline" value="View Transaction Details"/>
    <property name="IntroParagraphs">
      <list>
        <value>This screen allows you to view the details of a node transaction.</value>
      </list>
    </property>
    <property name="DataService" ref="transactionManagerService"/>
    <property name="AccountService" ref="accountManagerService"/>
  </object>

  <!-- TaskDetails -->
  <object type="~/Secure/TaskDetails.aspx">
    <property name="SectionTitle" value="Task Details"/>
    <property name="SectionHeadline" value="View Task Details"/>
    <property name="IntroParagraphs">
      <list>
        <value>This screen allows you to view the details of a node task.</value>
      </list>
    </property>
    <property name="DataService" ref="transactionManagerService"/>
    <property name="AccountService" ref="accountManagerService"/>
  </object>

</objects>



