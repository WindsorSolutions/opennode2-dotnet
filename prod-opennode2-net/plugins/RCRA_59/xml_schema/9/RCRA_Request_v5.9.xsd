<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:RC="http://www.exchangenetwork.net/schema/RCRA/5" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.exchangenetwork.net/schema/RCRA/5" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="5.9">
	<xsd:include schemaLocation="./RCRA_Shared_v5.9.xsd"/>
	<xsd:include schemaLocation="./RCRA_SharedPublishing_v5.9.xsd"/>
	<xsd:annotation>
		<xsd:documentation>
		Schema Name : 	RCRA_Request_v5.8
		Description : 	
		Developed by : 	 Windsor Solutions, Inc.
	</xsd:documentation>
	</xsd:annotation>
	<xsd:element name="Request" type="RC:RequestDataType">
		<xsd:annotation>
			<xsd:documentation>Compliance Monitoring and Enforcement Request Data</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="RequestDataType">
		<xsd:sequence>
			<xsd:element ref="RC:TransactionCode" minOccurs="0"/>
			<xsd:element ref="RC:RequestSequenceNumber"/>
			<xsd:element ref="RC:DateOfRequest" minOccurs="0"/>
			<xsd:element ref="RC:DateResponseReceived" minOccurs="0"/>
			<xsd:element ref="RC:RequestAgency" minOccurs="0"/>
			<xsd:element ref="RC:AgencyText" minOccurs="0"/>
			<xsd:element ref="RC:Notes" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
