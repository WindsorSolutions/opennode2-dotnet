<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:RC="http://www.exchangenetwork.net/schema/RCRA/5" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.exchangenetwork.net/schema/RCRA/5" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="5.9">
	<xsd:include schemaLocation="./RCRA_Shared_v5.9.xsd"/>
	<xsd:include schemaLocation="./RCRA_SharedPublishing_v5.9.xsd"/>
	<xsd:annotation>
		<xsd:documentation>
		Schema Name : 	RCRA_CorrectiveActionRelatedEvent_v5.9
		Description : 	Linking Data for Corrective Action Areas and Events or Authorities and Events
		Developed by : 	U.S. EPA
	</xsd:documentation>
	</xsd:annotation>
	<xsd:element name="CorrectiveActionRelatedEvent" type="RC:CorrectiveActionRelatedEventDataType">
		<xsd:annotation>
			<xsd:documentation>Linking Data for Corrective Action Areas and Events or Authorities and Events</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="CorrectiveActionRelatedEventDataType">
		<xsd:sequence>
			<xsd:element ref="RC:TransactionCode" minOccurs="0"/>
			<xsd:element ref="RC:ActivityLocationCode"/>
			<xsd:element ref="RC:CorrectiveActionEventDataOwnerCode"/>
			<xsd:element ref="RC:CorrectiveActionEventCode"/>
			<xsd:element ref="RC:CorrectiveActionEventText" minOccurs="0"/>
			<xsd:element ref="RC:EventAgencyCode"/>
			<xsd:element ref="RC:AgencyText" minOccurs="0"/>
			<xsd:element ref="RC:EventSequenceNumber"/>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
