	</xsd:simpleType>
	<xsd:simpleType name="ImportCityDataType">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="100"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PostalCodeDataType">
		<xsd:restriction base="xsd:string">
