<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:RC="http://www.exchangenetwork.net/schema/RCRA/5" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.exchangenetwork.net/schema/RCRA/5" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="5.6">
	<xsd:include schemaLocation="./RCRA_Shared_v5.7.xsd"/>
	<xsd:include schemaLocation="./RCRA_SharedPublishing_v5.7.xsd"/>
	<xsd:annotation>
		<xsd:documentation>
		Schema Name : 	RCRA_AreaAcreage_v5.7
		Description : 	Used to define the acreage of a handler or area.
		Developed by : 	U.S. EPA
	</xsd:documentation>
	</xsd:annotation>
	<xsd:element name="AreaAcreage" type="RC:AreaAcreageDataType">
		<xsd:annotation>
			<xsd:documentation>Used to define the acreage of a handler or area.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="AreaAcreageDataType">
		<xsd:annotation>
			<xsd:documentation>Used to define the acreage of a handler or area.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RC:AreaAcreageMeasure"/>
			<xsd:element ref="RC:AreaMeasureSourceDataOwnerCode"/>
			<xsd:element ref="RC:AreaMeasureSourceCode"/>
			<xsd:element ref="RC:AreaMeasureSourceText" minOccurs="0"/>
			<xsd:element ref="RC:AreaMeasureDate"/>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
