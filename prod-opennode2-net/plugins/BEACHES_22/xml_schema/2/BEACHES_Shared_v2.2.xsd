<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:bch="http://www.exchangenetwork.net/schema/BEACHES/2" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.exchangenetwork.net/schema/BEACHES/2" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.2">
	<xsd:annotation>
		<xsd:documentation>
			Schema Name: Shared v2.2
			Current Version Available At: http://www.exchangenetwork.net/exchanges/water/beach_notif.htm 
			Description: Schema used to declare all elements, simple data types and elementary complex data types.
			Application: PRAWN
			Developed by: EPA ORD/OW  and CGI Federal
			Point of Contact: eBeaches@cgifederal.com
			Change History: 
					20051208 - Initial Creation of Notification Schema v1.2
					20061108 - Revised to follow EPA/Exchange Network Guidelines
					20070821 - Revised to upgrade to version 2.0 of the schema
					20080825 - Revised to upgrade to version 2.1 of the schema
					20111003 - Revised to upgrade to version 2.2 of the schema
		</xsd:documentation>
	</xsd:annotation>
	<xsd:simpleType name="Char2">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="2"/>
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Char5">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="5"/>
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Char12">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="12"/>
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Char30">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="30"/>
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Char24">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="24"/>
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Char50">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="50"/>
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Char60">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="60"/>
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Char65">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="65"/>
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Char100">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="100"/>
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Char255">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="255"/>
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="StateCodeDataType">
		<xsd:restriction base="xsd:string">
			<xsd:length value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="MailingAddressZipCodeDataType">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="5"/>
			<xsd:maxLength value="12"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="StatusDataType" final="list">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ACTIVE"/>
			<xsd:enumeration value="INACTIVE"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TelephoneNumberDataType">
		<xsd:restriction base="xsd:string">
			<xsd:length value="12"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="BeachIdentifierDataType">
		<xsd:restriction base="xsd:string">
			<xsd:length value="8"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ProcedureBeachIdentifierDataType">
		<xsd:restriction base="xsd:string">
			<xsd:length value="8"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Number4">
		<xsd:restriction base="xsd:decimal">
			<xsd:totalDigits value="4"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Number14">
		<xsd:restriction base="xsd:decimal">
			<xsd:totalDigits value="14"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="DistanceUnitOfMeasureCodeDataType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="FT"/>
			<xsd:enumeration value="YDS"/>
			<xsd:enumeration value="MI"/>
			<xsd:enumeration value="M"/>
			<xsd:enumeration value="KM"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="BeachSwimSeasonLengthDetailDataType">
		<xsd:choice>
			<xsd:sequence>
				<xsd:element ref="bch:SwimSeasonLengthMeasure"/>
				<xsd:element ref="bch:SwimSeasonUnitOfMeasureCode"/>
			</xsd:sequence>
			<xsd:sequence>
				<xsd:element ref="bch:SwimSeasonStartDate"/>
				<xsd:element ref="bch:SwimSeasonEndDate"/>
			</xsd:sequence>
		</xsd:choice>
	</xsd:complexType>
	<xsd:simpleType name="SwimSeasonUnitOfMeasureCodeDataType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="DAYS"/>
			<xsd:enumeration value="WEEKS"/>
			<xsd:enumeration value="MONTHS"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="ReportingFrequencyDetailDataType">
		<xsd:sequence>
			<xsd:element ref="bch:ReportingFrequencyMeasure"/>
			<xsd:element ref="bch:ReportingFrequencyUnitOfMeasureCode"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="ReportingFrequencyUnitOfMeasureCodeDataType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="PER_DAY"/>
			<xsd:enumeration value="PER_WEEK"/>
			<xsd:enumeration value="PER_MONTH"/>
			<xsd:enumeration value="PER_YEAR"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="MonitoringFrequencyDetailDataType">
		<xsd:sequence>
			<xsd:element ref="bch:SwimSeasonFrequencyMeasure"/>
			<xsd:element ref="bch:OffSeasonFrequencyMeasure"/>
			<xsd:element ref="bch:MonitoringFrequencyUnitOfMeasureCode"/>
			<xsd:element ref="bch:MonitoredIrregularly"/>
			<xsd:element ref="bch:MonitoredIrregularlyComment" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="MonitoringFrequencyUnitOfMeasureCodeDataType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="PER_DAY"/>
			<xsd:enumeration value="PER_WEEK"/>
			<xsd:enumeration value="PER_MONTH"/>
			<xsd:enumeration value="PER_YEAR"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="BeachPollutionSourceDetailDataType">
		<xsd:choice>
			<xsd:element ref="bch:BeachPollutionSource" maxOccurs="unbounded"/>
			<xsd:element ref="bch:NoPollutionSourcesIndicator"/>
			<xsd:element ref="bch:PollutionSourcesUninvestigatedIndicator"/>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="BeachPollutionSourceDataType">
		<xsd:sequence>
			<xsd:element ref="bch:BeachPollutionSourceCode"/>
			<xsd:element ref="bch:BeachPollutionSourceDescription" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="BeachPollutionSourceCodeDataType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="AGRICULTURAL"/>
			<xsd:enumeration value="ALGAE"/>
			<xsd:enumeration value="BOAT"/>
			<xsd:enumeration value="CAFO"/>
			<xsd:enumeration value="CSO"/>
			<xsd:enumeration value="POTW"/>
			<xsd:enumeration value="RUNOFF"/>
			<xsd:enumeration value="SEPTIC"/>
			<xsd:enumeration value="SEWER_LINE"/>
			<xsd:enumeration value="SSO"/>
			<xsd:enumeration value="STORM"/>
			<xsd:enumeration value="WILDLIFE"/>
			<xsd:enumeration value="UNKNOWN"/>
			<xsd:enumeration value="OTHER"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NoPollutionSourcesIndicatorDataType">
		<xsd:restriction base="xsd:boolean">
			<xsd:pattern value="(true|1)"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PollutionSourcesUninvestigatedIndicatorDataType">
		<xsd:restriction base="xsd:boolean">
			<xsd:pattern value="(true|1)"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="BeachCriterionDetailDataType">
		<xsd:sequence>
			<xsd:element ref="bch:IndicatorName"/>
			<xsd:element ref="bch:WaterTypeName"/>
			<xsd:element ref="bch:CriterionMeasure"/>
			<xsd:element ref="bch:CriterionComment" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="IndicatorNameDataType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ENTEROCOCCI"/>
			<xsd:enumeration value="FECAL"/>
			<xsd:enumeration value="TOTAL"/>
			<xsd:enumeration value="ECOLI"/>
      <xsd:enumeration value="OTHER"/>
    </xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="WaterTypeNameDataType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="MARINE"/>
			<xsd:enumeration value="FRESH"/>
			<xsd:enumeration value="BOTH"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="CriterionMeasureDataType">
		<xsd:sequence>
			<xsd:element ref="bch:MeasureTypeName"/>
			<xsd:element ref="bch:MeasureValue"/>
			<xsd:element ref="bch:MeasureUnitCode"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="MeasureTypeNameDataType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="WQS"/>
			<xsd:enumeration value="GM"/>
			<xsd:enumeration value="SSM"/>
			<xsd:enumeration value="CFU"/>
			<xsd:enumeration value="STV"/>
			<xsd:enumeration value="qPCR"/>
			<xsd:enumeration value="CCE"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="IntNumber1">
		<xsd:restriction base="xsd:integer">
			<xsd:totalDigits value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="ActivityReasonDetailDataType">
		<xsd:sequence>
			<xsd:element ref="bch:ActivityReasonType"/>
			<xsd:element ref="bch:ActivityReasonDescriptionText" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="ActivityReasonDataType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ELEV_BACT"/>
			<xsd:enumeration value="RAINFALL"/>
			<xsd:enumeration value="SEWAGE"/>
			<xsd:enumeration value="CHEM_OIL"/>
			<xsd:enumeration value="MODEL"/>
			<xsd:enumeration value="POLICY"/>
			<xsd:enumeration value="OTHER"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="ActivitySourceDetailDataType">
		<xsd:sequence>
			<xsd:element ref="bch:ActivitySourceType"/>
			<xsd:element ref="bch:ActivitySourceDescriptionText" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="ActivitySourceDataType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="AGRICULTURAL"/>
			<xsd:enumeration value="ALGAE"/>
			<xsd:enumeration value="BOAT"/>
			<xsd:enumeration value="CAFO"/>
			<xsd:enumeration value="CSO"/>
			<xsd:enumeration value="POTW"/>
			<xsd:enumeration value="RUNOFF"/>
			<xsd:enumeration value="SEPTIC"/>
			<xsd:enumeration value="SEWER_LINE"/>
			<xsd:enumeration value="SSO"/>
			<xsd:enumeration value="STORM"/>
			<xsd:enumeration value="WILDLIFE"/>
			<xsd:enumeration value="UNKNOWN"/>
			<xsd:enumeration value="OTHER"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="ActivityIndicatorDetailDataType">
		<xsd:sequence>
			<xsd:element ref="bch:ActivityIndicatorType"/>
			<xsd:element ref="bch:ActivityIndicatorDescriptionText" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="ActivityIndicatorDataType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="PREEMPT"/>
			<xsd:enumeration value="ENTERO"/>
			<xsd:enumeration value="TOTAL_COL"/>
			<xsd:enumeration value="FECAL_COL"/>
			<xsd:enumeration value="ECOLI"/>
			<xsd:enumeration value="RATIO"/>
			<xsd:enumeration value="OTHER"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="ActivityExtentDetailDataType">
		<xsd:sequence>
			<xsd:element ref="bch:ActivityExtentStartMeasure" minOccurs="0"/>
			<xsd:element ref="bch:ActivityExtentLengthMeasure"/>
			<xsd:element ref="bch:ActivityExtentUnitOfMeasureCode"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="ActivityExtentUnitOfMeasureCodeDataType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="FT"/>
			<xsd:enumeration value="YDS"/>
			<xsd:enumeration value="MI"/>
			<xsd:enumeration value="M"/>
			<xsd:enumeration value="KM"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="FIPSCountyCodeDataType">
		<xsd:restriction base="xsd:string">
			<xsd:length value="5"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="WaterBodyNameCodeDataType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ATLANTIC"/>
			<xsd:enumeration value="PACIFIC"/>
			<xsd:enumeration value="GULF_MEXICO"/>
			<xsd:enumeration value="LAKE_SUPR"/>
			<xsd:enumeration value="LAKE_MCHGN"/>
			<xsd:enumeration value="LAKE_HURON"/>
			<xsd:enumeration value="LAKE_ERIE"/>
			<xsd:enumeration value="LAKE_ONTR"/>
			<xsd:enumeration value="INLAND"/>
			<xsd:enumeration value="LNG_ISL_SND"/>
			<xsd:enumeration value="CHSPK_BAY"/>
			<xsd:enumeration value=""/>
			<xsd:enumeration value=""/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="WaterBodyTypeCodeDataType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="OPEN_COAST"/>
			<xsd:enumeration value="SND_BY_INLT"/>
			<xsd:enumeration value="STILL_WATER"/>
			<xsd:enumeration value="FLOW_WATER"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="BeachAccessibilityDetailDataType">
		<xsd:sequence>
			<xsd:element ref="bch:BeachAccessibilityType"/>
			<xsd:element ref="bch:BeachAccessibilityComment" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="BeachAccessibilityDataType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="PUB_PUB_ACC"/>
			<xsd:enumeration value="PRV_PRV_ACC"/>
			<xsd:enumeration value="PUB_PRV_ACC"/>
			<xsd:enumeration value="PRV_PUB_ACC"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="BeachCoordinatePointDetailDataType">
		<xsd:sequence>
			<xsd:element ref="bch:LatitudeMeasure"/>
			<xsd:element ref="bch:LongitudeMeasure"/>
			<xsd:element ref="bch:SourceMapScaleNumeric" minOccurs="0"/>
			<xsd:element ref="bch:HorizontalCollectionMethodName"/>
			<xsd:element ref="bch:HorizontalCoordinateReferenceSystemDatumName"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="LatitudeMeasureDataType">
		<xsd:restriction base="xsd:decimal">
			<xsd:totalDigits value="8"/>
			<xsd:fractionDigits value="6"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LongitudeMeasureDataType">
		<xsd:restriction base="xsd:decimal">
			<xsd:totalDigits value="9"/>
			<xsd:fractionDigits value="6"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SourceMapScaleNumericDataType">
		<xsd:restriction base="xsd:nonNegativeInteger"/>
	</xsd:simpleType>
	<xsd:simpleType name="HorizontalCollectionMethodNameDataType">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="150"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="HorizontalCoordinateReferenceSystemDatumNameDataType">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="6"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Element Declarations  ================================================================-->
	<xsd:element name="BeachCoordinateStartPointDetail" type="bch:BeachCoordinatePointDetailDataType">
		<xsd:annotation>
			<xsd:documentation>Geospatial description of the starting point of a beach.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="BeachCoordinateEndPointDetail" type="bch:BeachCoordinatePointDetailDataType">
		<xsd:annotation>
			<xsd:documentation>Geospatial description of the ending point of a beach.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="LatitudeMeasure" type="bch:LatitudeMeasureDataType">
		<xsd:annotation>
			<xsd:documentation>The measure of the angular distance on a meridian north or south of the equator.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="LongitudeMeasure" type="bch:LongitudeMeasureDataType">
		<xsd:annotation>
			<xsd:documentation>The measure of the angular distance on a meridian east or west of the prime meridian.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SourceMapScaleNumeric" type="bch:SourceMapScaleNumericDataType">
		<xsd:annotation>
			<xsd:documentation>The number that represents the proportional distance on the ground for one unit of measure on the map or photo.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="HorizontalCollectionMethodName" type="bch:HorizontalCollectionMethodNameDataType">
		<xsd:annotation>
			<xsd:documentation>The name that identifies the method used to determine the latitude and longitude coordinates for a point on the earth.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="HorizontalCoordinateReferenceSystemDatumName" type="bch:HorizontalCoordinateReferenceSystemDatumNameDataType">
		<xsd:annotation>
			<xsd:documentation>The name that describes the reference datum used in determining latitude and longitude coordinates.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="BeachAccessibilityComment" type="bch:Char255">
		<xsd:annotation>
			<xsd:documentation>A short comment about the accessibility of the beach</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="BeachAccessibilityType" type="bch:BeachAccessibilityDataType">
		<xsd:annotation>
			<xsd:documentation>The accessibility of the beach (ex: PUB_PUB_ACC, PRV_PUB_ACC)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="BeachAccessibilityDetail" type="bch:BeachAccessibilityDetailDataType">
		<xsd:annotation>
			<xsd:documentation>Information about the accessibility of the beach</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="WaterBodyNameCode" type="bch:WaterBodyNameCodeDataType">
		<xsd:annotation>
			<xsd:documentation>The water body name code for the beach.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="WaterBodyTypeCode" type="bch:WaterBodyTypeCodeDataType">
		<xsd:annotation>
			<xsd:documentation>The water body type code for the beach.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ProgramInterestFIPSCountyCode" type="bch:FIPSCountyCodeDataType">
		<xsd:annotation>
			<xsd:documentation>The FIPS county code for the program interest</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ProgramInterestStateCode" type="bch:StateCodeDataType">
		<xsd:annotation>
			<xsd:documentation>The two letter code fot the state the program interest is located in</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ProgramInterestCommentText" type="bch:Char255">
		<xsd:annotation>
			<xsd:documentation>A comment about the program interest</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ProgramInterestDescriptionText" type="bch:Char255">
		<xsd:annotation>
			<xsd:documentation>A short description of the program interest</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ProgramInterestName" type="bch:Char60">
		<xsd:annotation>
			<xsd:documentation>The name of the program interest</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="NamePrefixText" type="bch:Char60">
		<xsd:annotation>
			<xsd:documentation>The person's title</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="NameSuffixText" type="bch:Char5">
		<xsd:annotation>
			<xsd:documentation>The person's suffix</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PersonMiddleInitial" type="bch:Char2">
		<xsd:annotation>
			<xsd:documentation>The middle initial of the person</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="LastName" type="bch:Char50">
		<xsd:annotation>
			<xsd:documentation>The last name of the person</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="FirstName" type="bch:Char50">
		<xsd:annotation>
			<xsd:documentation>The first name of the person</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PersonStatusIndicator" type="bch:StatusDataType">
		<xsd:annotation>
			<xsd:documentation>Indicates whether the person is ACTIVE or INACTIVE</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PersonIdentifier" type="bch:Char12">
		<xsd:annotation>
			<xsd:documentation>The unique code that identifies each a person</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ContactFirstName" type="bch:Char50">
		<xsd:annotation>
			<xsd:documentation>The first name of the state contact</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ContactLastName" type="bch:Char50">
		<xsd:annotation>
			<xsd:documentation>The last name of the state contact</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ContactAgencyName" type="bch:Char60">
		<xsd:annotation>
			<xsd:documentation>Details the name of the state contact's agency being described</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ContactTelephoneNumberText" type="bch:TelephoneNumberDataType">
		<xsd:annotation>
			<xsd:documentation>The state contact's telephone number being described (Must include "-".  ex: xxx-xxx-xxxx)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ContactElectronicAddressText" type="bch:Char255">
		<xsd:annotation>
			<xsd:documentation>The state contact's electronic address being described</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="BeachRoleStatusIndicator" type="bch:StatusDataType">
		<xsd:annotation>
			<xsd:documentation>Indicates whether the role is ACTIVE or INACTIVE</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
