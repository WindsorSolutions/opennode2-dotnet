<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:RC="http://www.exchangenetwork.net/schema/RCRA/5" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.exchangenetwork.net/schema/RCRA/5" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="5.11">
	<xsd:include schemaLocation="./RCRA_Shared_v5.11.xsd"/>
	<xsd:include schemaLocation="./RCRA_SharedPublishing_v5.11.xsd"/>
	<xsd:annotation>
		<xsd:documentation>
		Schema Name : 	RCRA_HandlerWasteCodeDetails_v5.11
		Description : 	Hazardous waste codes describing the handler's hazardous waste streams.
		Developed by : 	U.S. EPA
	</xsd:documentation>
	</xsd:annotation>
	<xsd:element name="HandlerWasteCodeDetails" type="RC:HandlerWasteCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Hazardous waste codes describing the handler's hazardous waste streams.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="HandlerWasteCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Hazardous waste codes describing the handler's hazardous waste streams.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RC:TransactionCode" minOccurs="0"/>
			<xsd:element ref="RC:WasteCodeOwnerName" minOccurs="0"/>
			<xsd:element ref="RC:WasteCode" minOccurs="0"/>
			<xsd:element ref="RC:WasteCodeText" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
