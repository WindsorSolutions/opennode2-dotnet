<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:emts="http://www.exchangenetwork.net/schema/emts/3" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.exchangenetwork.net/schema/emts/3" elementFormDefault="qualified" attributeFormDefault="unqualified" version="3.0">
	<xsd:annotation>
		<xsd:documentation>
		Schema Name: EMTS_RetireTransactionDetail_v3.0.xsd
        Description: Information on the RINs being Retired.
        Developed by: SRA International, Inc.
        Point of Contact: Jeff Heatwole
        </xsd:documentation>
	</xsd:annotation>
	<xsd:include schemaLocation="EMTS_SimpleContent_v3.0.xsd"/>
	<xsd:include schemaLocation="EMTS_SupportingDocumentDetail_v3.0.xsd"/>
	<xsd:include schemaLocation="EMTS_OriginatingSourceDetail_v3.0.xsd"/>
	<xsd:complexType name="RetireTransactionDetailDataType">
		<xsd:sequence>
			<xsd:element ref="emts:RINQuantity"/>
			<xsd:element ref="emts:BatchVolume" minOccurs="0"/>
			<xsd:element ref="emts:FuelCode"/>
			<xsd:element ref="emts:AssignmentCode"/>
			<xsd:element ref="emts:RINYear"/>
            <xsd:element ref="emts:QAPServiceTypeCode" minOccurs="0"/>
			<xsd:element ref="emts:RetireReasonCode"/>
            <xsd:element ref="emts:TransactionDate"/>
			<xsd:element ref="emts:ComplianceYear" minOccurs="0"/>
			<xsd:element ref="emts:ComplianceLevelCode" minOccurs="0"/>
			<xsd:element ref="emts:ComplianceFacilityIdentifier" minOccurs="0"/>
			<xsd:element ref="emts:TransactionDetailCommentText" minOccurs="0"/>
			<xsd:element ref="emts:RetireSupportingDocumentDetail" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="emts:RetireOriginatingSourceDetail" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="RetireTransactionDetail" type="emts:RetireTransactionDetailDataType"/>
</xsd:schema>
