<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:IC="http://www.exchangenetwork.net/schema/IC/1" targetNamespace="http://www.exchangenetwork.net/schema/IC/1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<xsd:include schemaLocation="IC_ICLocation_v1.0.xsd"/>
	<xsd:annotation>
		<xsd:documentation>
			Schema Name:	IC_ICLocationList_v1.0.xsd
			Current Version Available At: http://www.exchangenetwork.net/
			Description: A container for one or more geographic locations.
			Developed by: Windsor Solutions, Inc.
	        Point of Contact: ECOS
	        Last Modified by: Windsor Solutions, Inc.
		</xsd:documentation>
	</xsd:annotation>
	<xsd:element name="ICLocationList" type="IC:ICLocationListDataType">
		<xsd:annotation>
			<xsd:documentation>A container for one or more locations.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="ICLocationListDataType">
		<xsd:sequence>
			<xsd:element ref="IC:ICLocation" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
