<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:wqx="http://www.exchangenetwork.net/schema/wqx/3" targetNamespace="http://www.exchangenetwork.net/schema/wqx/3" elementFormDefault="qualified" attributeFormDefault="unqualified" version="3.0">
	<xsd:include schemaLocation="WQX_OrganizationDescription_v3.0.xsd"/>
	<xsd:include schemaLocation="WQX_ElectronicAddress_v3.0.xsd"/>
	<xsd:include schemaLocation="WQX_Telephonic_v3.0.xsd"/>
	<xsd:include schemaLocation="WQX_OrganizationAddress_v3.0.xsd"/>
  
	<xsd:include schemaLocation="WQX_Project_v3.0.xsd"/>
	<xsd:include schemaLocation="WQX_MonitoringLocation_v3.0.xsd"/>
	<xsd:include schemaLocation="WQX_Activity_v3.0.xsd"/>
	<xsd:include schemaLocation="WQX_ActivityGroup_v3.0.xsd"/>
	<xsd:include schemaLocation="WQX_BiologicalHabitatIndex_v3.0.xsd"/>
	<xsd:annotation>
		<xsd:documentation>
  	        Schema Name  : WQX_Organization_v3.0.xsd
  	        Description   : Schema used to transfer organization information.
  	        Developed by  : EPA Office of Water
  	        Point of Contact :   Kevin Christian
		</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="OrganizationDataType">
		<xsd:sequence>
			<xsd:element ref="wqx:OrganizationDescription"/>
			<xsd:element ref="wqx:ElectronicAddress" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="wqx:Telephonic" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="wqx:OrganizationAddress" minOccurs="0" maxOccurs="3"/>
 
			<xsd:element ref="wqx:Project" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="wqx:MonitoringLocation" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="wqx:BiologicalHabitatIndex" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="wqx:Activity" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="wqx:ActivityGroup" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="Organization" type="wqx:OrganizationDataType">
		<xsd:annotation>
			<xsd:documentation>Schema used to transfer organization information.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
</xsd:schema>
