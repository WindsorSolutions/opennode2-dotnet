<?xml version="1.0" encoding="utf-8"?><xsd:schema targetNamespace="urn:us:net:exchangenetwork" xmlns:pnwwqx="urn:us:net:exchangenetwork" xmlns:xsd="http://www.w3.org/2001/XMLSchema" version="1.3">	<xsd:annotation>		<xsd:documentation>		Schema Name : 					PNWWQX_MediaType_v.1.3.xsd 		Current Version Available At :			Description : 					This schema defines the values for the media type element used to characterize the environmental material from which a sample was taken.		Application : 					Pacific Northwest Water Quality Data Exchange 		Developed by : 	 				Pacific Northwest Exchange States; Windsor Solutions, Inc		Point of Contact : 				Curtis Cude (cude.curtis@deq.state.or.us)										Kevin Jeffery (kevin_jeffery@windsorsolutions.com)		</xsd:documentation>	</xsd:annotation>	<xsd:simpleType name="MediaType">		<xsd:restriction base="xsd:string">			<xsd:enumeration value="Water"/>			<xsd:enumeration value="Sediment"/>			<xsd:enumeration value="Soil"/>			<xsd:enumeration value="Biological (tissue)"/>			<xsd:enumeration value="Biological (population)"/>		</xsd:restriction>	</xsd:simpleType></xsd:schema>