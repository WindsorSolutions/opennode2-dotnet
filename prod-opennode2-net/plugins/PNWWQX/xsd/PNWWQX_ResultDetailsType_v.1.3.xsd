<?xml version="1.0" encoding="utf-8"?><xsd:schema targetNamespace="urn:us:net:exchangenetwork" xmlns:pnwwqx="urn:us:net:exchangenetwork" xmlns:xsd="http://www.w3.org/2001/XMLSchema" version="1.3">	<xsd:include schemaLocation=".\PNWWQX_TaxonType_v.1.3.xsd"/>	<xsd:include schemaLocation=".\PNWWQX_AnalyteType_v.1.3.xsd"/>	<xsd:include schemaLocation=".\PNWWQX_MethodType_v.1.3.xsd"/>	<xsd:annotation>		<xsd:documentation>		Schema Name : 					PNWWQX_ResultDetailsType_v.1.3.xsd 		Current Version Available At :			Description : 					This schema defines the data elements to be shared through the Pacific Northwest Water Quality Data Exchange related to a water quality monitoring result.		Application : 					Pacific Northwest Water Quality Data Exchange 		Developed by : 	 				Pacific Northwest Exchange States; Windsor Solutions, Inc		Point of Contact : 				Curtis Cude (cude.curtis@deq.state.or.us)										Kevin Jeffery (kevin_jeffery@windsorsolutions.com)			</xsd:documentation>	</xsd:annotation>	<xsd:complexType name="ResultDetailsType">		<xsd:sequence>			<xsd:element ref="pnwwqx:ResultValueMeasure"/>			<xsd:element ref="pnwwqx:ResultValueSignificantDigitsNumber" minOccurs="0"/>			<xsd:element ref="pnwwqx:ResultUnitMeasureName"/>			<xsd:element ref="pnwwqx:ResultQualifier" minOccurs="0"/>			<xsd:element ref="pnwwqx:ResultAnalyte"/>			<xsd:element ref="pnwwqx:ResultTaxon" minOccurs="0"/>			<xsd:element ref="pnwwqx:ResultQualityAssessment"/>			<xsd:element ref="pnwwqx:ResultStatus"/>			<xsd:element ref="pnwwqx:ResultAvailabilityDescription"/>			<xsd:element ref="pnwwqx:ResultAnalyticalMethod"/>			<xsd:element ref="pnwwqx:ResultDetectionLevels" minOccurs="0" maxOccurs="unbounded"/>			<xsd:element ref="pnwwqx:QAQCExceptionIndicator"/>			<xsd:element ref="pnwwqx:QAQCExceptionCommentText" minOccurs="0"/>		</xsd:sequence>	</xsd:complexType>	<xsd:element name="ResultValueMeasure" type="xsd:string"/>	<xsd:element name="ResultValueSignificantDigitsNumber" type="xsd:integer"/>	<xsd:element name="ResultUnitMeasureName" type="xsd:string"/>	<xsd:element name="ResultQualifier" type="xsd:string"/>	<xsd:element name="ResultAnalyte" type="pnwwqx:AnalyteType"/>	<xsd:element name="ResultTaxon" type="pnwwqx:TaxonType"/>	<xsd:element name="ResultQualityAssessment" type="pnwwqx:ResultQualityAssessmentType"/>	<xsd:element name="ResultStatus" type="pnwwqx:ResultStatusType"/>	<xsd:element name="ResultAvailabilityDescription" type="pnwwqx:ResultAvailabilityType"/>	<xsd:element name="ResultAnalyticalMethod" type="pnwwqx:MethodType"/>	<xsd:element name="ResultDetectionLevels" type="pnwwqx:DetectionQuantitationLevelType"/>	<xsd:element name="QAQCExceptionIndicator" type="xsd:boolean"/>	<xsd:element name="QAQCExceptionCommentText" type="xsd:string"/>	<xsd:complexType name="DetectionQuantitationLevelType">		<xsd:sequence>			<xsd:element ref="pnwwqx:DetectionQuantitationLevelMeasure"/>			<xsd:element ref="pnwwqx:DetectionQuantitationLevelUnitMeasureName"/>			<xsd:element ref="pnwwqx:DetectionQuantitationLevelTypeText"/>		</xsd:sequence>	</xsd:complexType>	<xsd:element name="DetectionQuantitationLevelMeasure" type="xsd:decimal"/>	<xsd:element name="DetectionQuantitationLevelUnitMeasureName" type="xsd:string"/>	<xsd:element name="DetectionQuantitationLevelTypeText" type="xsd:string"/>	<xsd:simpleType name="ResultQualityAssessmentType">		<xsd:restriction base="xsd:string">			<xsd:enumeration value="A - Known good quality"/>			<xsd:enumeration value="B - Known suspect quality"/>			<xsd:enumeration value="C - Known unacceptable quality"/>			<xsd:enumeration value="D - Sample collection failure"/>			<xsd:enumeration value="E - Unknown quality"/>			<xsd:enumeration value="F - Good quality non-representative sampling"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="ResultStatusType">		<xsd:restriction base="xsd:string">			<xsd:enumeration value="Preliminary - data has not been quality assured"/>			<xsd:enumeration value="Final - data has been quality assured"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="ResultAvailabilityType">		<xsd:restriction base="xsd:string">			<xsd:enumeration value="Routine - No restrictions"/>			<xsd:enumeration value="Agreement - Restricted by agreement between partners"/>			<xsd:enumeration value="Civil - Restricted due to ongoing civil enforcement action"/>			<xsd:enumeration value="Criminal - Restricted due to ongoing criminal enforcement action"/>		</xsd:restriction>	</xsd:simpleType></xsd:schema>