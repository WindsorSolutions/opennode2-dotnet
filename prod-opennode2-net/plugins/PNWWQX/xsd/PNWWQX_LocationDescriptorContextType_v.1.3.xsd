<?xml version="1.0" encoding="utf-8"?><xsd:schema targetNamespace="urn:us:net:exchangenetwork" xmlns:pnwwqx="urn:us:net:exchangenetwork" xmlns:xsd="http://www.w3.org/2001/XMLSchema" version="1.3">	<xsd:annotation>		<xsd:documentation>		Schema Name : 					PNWWQX_LocationDescriptorContextType_v.1.3.xsd 		Current Version Available At :			Description : 					This schema defines the keywords used to describe areas within which stations are located or for alternative designations for station locations.		Application : 					Pacific Northwest Water Quality Data Exchange 		Developed by : 	 				Pacific Northwest Exchange States; Windsor Solutions, Inc		Point of Contact : 				Curtis Cude (cude.curtis@deq.state.or.us)										Kevin Jeffery (kevin_jeffery@windsorsolutions.com)		</xsd:documentation>	</xsd:annotation>	<xsd:simpleType name="LocationDescriptorContextType">		<xsd:restriction base="xsd:string">			<xsd:enumeration value="HUC"/>			<xsd:enumeration value="Waterbody"/>			<xsd:enumeration value="WRIA"/>			<xsd:enumeration value="Aquifer"/>			<xsd:enumeration value="NRCS ID"/>			<xsd:enumeration value="Ecoregion"/>			<xsd:enumeration value="LLID"/>			<xsd:enumeration value="County"/>			<xsd:enumeration value="State"/>			<xsd:enumeration value="Congressional District"/>			<xsd:enumeration value="ZIP Code"/>			<xsd:enumeration value="Other"/>		</xsd:restriction>	</xsd:simpleType></xsd:schema>