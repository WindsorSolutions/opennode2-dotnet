<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:RC="http://www.exchangenetwork.net/schema/RCRA/5" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.exchangenetwork.net/schema/RCRA/5" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="5.10">
	<xsd:include schemaLocation="./RCRA_Shared_v5.10.xsd"/>
	<xsd:include schemaLocation="./RCRA_SharedPublishing_v5.10.xsd"/>
	<xsd:annotation>
		<xsd:documentation>
		Schema Name : 	RCRA_GeographicMetadata_v5.10
		Description : 	Metadata for the geographic coordinates of the Handler.
		Developed by : 	U.S. EPA
	</xsd:documentation>
	</xsd:annotation>
	<xsd:element name="GeographicMetadata" type="RC:GeographicMetadataDataType">
		<xsd:annotation>
			<xsd:documentation>Metadata for the geographic coordinates of the Handler.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="GeographicMetadataDataType">
		<xsd:annotation>
			<xsd:documentation>Metadata for the geographic coordinates of the Handler.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RC:DataCollectionDate"/>
			<xsd:element ref="RC:HorizontalAccuracyMeasure" minOccurs="0"/>
			<xsd:element ref="RC:SourceMapScaleNumeric" minOccurs="0"/>
			<xsd:element ref="RC:CoordinateDataSourceDataOwnerCode" minOccurs="0"/>
			<xsd:element ref="RC:CoordinateDataSourceCode" minOccurs="0"/>
			<xsd:element ref="RC:CoordinateDataSourceName" minOccurs="0"/>
			<xsd:element ref="RC:GeographicReferencePointDataOwnerCode" minOccurs="0"/>
			<xsd:element ref="RC:GeographicReferencePointCode" minOccurs="0"/>
			<xsd:element ref="RC:GeographicReferencePointName" minOccurs="0"/>
			<xsd:element ref="RC:GeometricTypeDataOwnerCode" minOccurs="0"/>
			<xsd:element ref="RC:GeometricTypeCode" minOccurs="0"/>
			<xsd:element ref="RC:GeometricTypeName" minOccurs="0"/>
			<xsd:element ref="RC:HorizontalCollectionMethodDataOwnerCode" minOccurs="0"/>
			<xsd:element ref="RC:HorizontalCollectionMethodCode" minOccurs="0"/>
			<xsd:element ref="RC:HorizontalCollectionMethodName" minOccurs="0"/>
			<xsd:element ref="RC:HorizontalCoordinateReferenceSystemDatumDataOwnerCode" minOccurs="0"/>
			<xsd:element ref="RC:HorizontalCoordinateReferenceSystemDatumCode" minOccurs="0"/>
			<xsd:element ref="RC:HorizontalCoordinateReferenceSystemDatumName" minOccurs="0"/>
			<xsd:element ref="RC:VerificationMethodDataOwnerCode" minOccurs="0"/>
			<xsd:element ref="RC:VerificationMethodCode" minOccurs="0"/>
			<xsd:element ref="RC:VerificationMethodName" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
