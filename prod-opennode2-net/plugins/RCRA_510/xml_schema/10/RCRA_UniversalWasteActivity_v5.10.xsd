<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:RC="http://www.exchangenetwork.net/schema/RCRA/5" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.exchangenetwork.net/schema/RCRA/5" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="5.10">
	<xsd:include schemaLocation="./RCRA_Shared_v5.10.xsd"/>
	<xsd:include schemaLocation="./RCRA_SharedPublishing_v5.10.xsd"/>
	<xsd:annotation>
		<xsd:documentation>
		Schema Name : 	RCRA_UniversalWasteActivity_v5.10
		Description : 	Information about universal waste generated by the handler.
		Developed by : 	U.S. EPA
	</xsd:documentation>
	</xsd:annotation>
	<xsd:element name="HandlerUniversalWaste" type="RC:UniversalWasteActivityDataType">
		<xsd:annotation>
			<xsd:documentation>Information about universal waste generated by the handler.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="UniversalWasteActivityDataType">
		<xsd:sequence>
			<xsd:element ref="RC:TransactionCode" minOccurs="0"/>
			<xsd:element ref="RC:UniversalWasteOwnerName" minOccurs="0"/>
			<xsd:element ref="RC:UniversalWasteTypeCode" minOccurs="0"/>
			<xsd:element ref="RC:UniversalWasteTypeText" minOccurs="0"/>
			<xsd:element ref="RC:AccumulatedWasteCode" minOccurs="0"/>
			<xsd:element ref="RC:GeneratedHandlerCode" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
