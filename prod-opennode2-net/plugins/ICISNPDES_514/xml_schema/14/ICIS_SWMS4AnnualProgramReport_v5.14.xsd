<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:icis="http://www.exchangenetwork.net/schema/icis/5" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.exchangenetwork.net/schema/icis/5" elementFormDefault="qualified" attributeFormDefault="unqualified" version="5">

	<xsd:include schemaLocation="ICIS_KeyElements_v5.14.xsd"/>
	<xsd:include schemaLocation="ICIS_SW_v5.14.xsd"/>

	<xsd:annotation>
		<xsd:documentation>Schema Name: ICIS_SWMS4ProgramReport_v5.14.xsd</xsd:documentation>
		<xsd:documentation>Current Version Available: http://www.exchangenetwork.net/schema/icis/5</xsd:documentation>
		<xsd:documentation>Description: ICIS-NPDES stormwater ms4 program report data input format</xsd:documentation>
		<xsd:documentation>Application: ICIS-NPDES</xsd:documentation>
		<xsd:documentation>Developed By: U.S. Environmental Protection Agency</xsd:documentation>
		<xsd:documentation>Point of Contact: Jay Stubbs, U.S. EPA</xsd:documentation>
		<xsd:documentation>Data Written: July 2024</xsd:documentation>
		<xsd:documentation>Last Date Modified: 2024-07-18</xsd:documentation>
	</xsd:annotation>

	<xsd:element name="SWMS4AnnualProgramReport" type="icis:SWMS4AnnualProgramReport"/>

	<xsd:complexType name="SWMS4AnnualProgramReportData">
		<xsd:sequence>
			<xsd:element ref="icis:TransactionHeader"/>
			<xsd:element ref="icis:SWMS4AnnualProgramReport"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="SWMS4AnnualProgramReport">
		<xsd:complexContent>
			<xsd:extension base="icis:SWMS4AnnualProgramReportKeyElements">
				<xsd:sequence>
					<xsd:element ref="icis:ProgramReportFormID" minOccurs="0"/>
					<xsd:element ref="icis:ProgramReportReceivedDate" minOccurs="0"/>
					<xsd:element ref="icis:ProgramReportStartDate" minOccurs="0"/>
					<xsd:element ref="icis:ProgramReportEndDate" minOccurs="0"/>
					<xsd:element ref="icis:ElectronicSubmissionTypeCode" minOccurs="0"/>
					<xsd:element ref="icis:ProgramReportNPDESDataGroupNumberCode" minOccurs="0"/>
					<xsd:element ref="icis:MS4IllicitDetectionRegulatedEntityInformationProgramReport" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="icis:MS4ProgramReportRequirementsRegulatedEntity" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="icis:MS4SWMPChanges" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="icis:MS4RegulatedEntityEnforcement" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="icis:MS4ProgramReportAnalysis" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

</xsd:schema>
