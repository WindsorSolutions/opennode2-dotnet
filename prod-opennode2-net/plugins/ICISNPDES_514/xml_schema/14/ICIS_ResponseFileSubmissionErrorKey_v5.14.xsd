<?xml version="1.0"?>
<xsd:schema xmlns:icis="http://www.exchangenetwork.net/schema/icis/5" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.exchangenetwork.net/schema/icis/5" elementFormDefault="qualified" attributeFormDefault="unqualified" version="5">
	<xsd:annotation>
		<xsd:documentation>Schema Name : ICIS_ResponseFileSubmissionErrorKey_v5.14.xsd</xsd:documentation>
		<xsd:documentation>Current Version Available: http://www.exchangenetwork.net/schema/icis/5</xsd:documentation>
		<xsd:documentation>Description : Component SubmissionErrorKey provides the options for uniquely identifying the part of the submission that caused the error.</xsd:documentation> 
		<xsd:documentation>Developed by : ICIS Full Batch IPT</xsd:documentation>
		<xsd:documentation>Point of Contact: Jay Stubbs, U.S. EPA</xsd:documentation>
		<xsd:documentation>Last Date Modified: 2024-06-27</xsd:documentation>
	</xsd:annotation>
	<xsd:include schemaLocation="ICIS_ResponseFileShared_v5.14.xsd"/>
	<xsd:element name="SubmissionErrorKey" type="icis:SubmissionErrorKeyDataType">
		<xsd:annotation>
			<xsd:documentation>Provides the keys that uniquely identify the part of a submission that caused an error.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="SubmissionErrorKeyDataType">
		<xsd:sequence>
			<xsd:group ref="icis:SubmissionResponseIdentifierChoicesGroup"/>
			<xsd:element ref="icis:SubmissionTransactionTypeCode"/>
		</xsd:sequence>
	</xsd:complexType>
