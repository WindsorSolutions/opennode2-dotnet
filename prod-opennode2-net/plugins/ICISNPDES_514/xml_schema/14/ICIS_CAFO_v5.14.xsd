			<xsd:element ref="icis:CAFOMLPWStorageWithinDesignCapacity" minOccurs="0"/>
			<xsd:element ref="icis:CAFOMLPWStorageWithinDesignCapacityText" minOccurs="0"/>
		</xsd:sequence>
