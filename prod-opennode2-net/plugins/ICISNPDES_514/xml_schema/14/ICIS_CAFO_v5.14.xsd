<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:icis="http://www.exchangenetwork.net/schema/icis/5" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.exchangenetwork.net/schema/icis/5" elementFormDefault="qualified" attributeFormDefault="unqualified" version="5">
	<xsd:include schemaLocation="ICIS_Common_v5.14.xsd"/>
	<xsd:annotation>
		<xsd:documentation>Schema Name: ICIS_CAFO_v5.14.xsd</xsd:documentation>
		<xsd:documentation>Current Version Available: http://www.exchangenetwork.net/schema/icis/5</xsd:documentation>
		<xsd:documentation>Description: ICIS-NPDES CAFO shared data input format</xsd:documentation>
		<xsd:documentation>Application: ICIS-NPDES</xsd:documentation>
		<xsd:documentation>Developed By: U.S. Environmental Protection Agency</xsd:documentation>
		<xsd:documentation>Point of Contact: Jay Stubbs, U.S. EPA</xsd:documentation>
		<xsd:documentation>Data Written: July 2006</xsd:documentation>
		<xsd:documentation>Last Date Modified: 2024-12-09</xsd:documentation>
	</xsd:annotation>
	<xsd:element name="AnimalType" type="icis:AnimalType"/>
	<xsd:element name="AnimalTypeCode" type="icis:StringMin1Max3Type"/>
	<xsd:element name="ApplicationMeasureAvailableLandNumber" type="icis:NonNegativeIntegerMin1Max6Type"/>
	<xsd:element name="AuthorizedLivestockCapacityNumber" type="icis:NonNegativeIntegerMin1Max9Type"/>
	<xsd:element name="CAFOClassificationCode" type="icis:StringMin1Max3Type"/>
	<xsd:element name="CAFODesignationDate" type="icis:DateType"/>
	<xsd:element name="CAFODesignationReasonText" type="icis:StringMin1Max500Type"/>
	<xsd:element name="CAFOLandApplicationFieldInformation" type="icis:CAFOLandApplicationFieldInformation"/>
	<xsd:element name="CAFOLandApplicationFieldCropInformation" type="icis:CAFOLandApplicationFieldCropInformation"/>
	<xsd:element name="CAFOLandApplicationFieldCropIdentifier" type="icis:StringMin1Max20Type"/>
	<xsd:element name="CAFOLandApplicationFieldCropCode" type="icis:StringMin1Max4Type"/>
	<xsd:element name="CAFOLandApplicationFieldCropYield" type="icis:DecimalRange0To999999.99Type"/>
	<xsd:element name="CAFOLandApplicationFieldCropYieldUnit" type="icis:StringMin1Max3Type"/>
	<xsd:element name="CAFOLandApplicationFieldCropSeeded" type="icis:YesNoIndicatorType"/>
	<xsd:element name="CAFOLandApplicationFieldIdentifier" type="icis:StringMin1Max20Type"/>
	<xsd:element name="CAFOLandApplicationFieldAcreage" type="icis:DecimalRange0To999999.99Type"/>
	<xsd:element name="CAFOMLPWCode" type="icis:StringMin1Max3Type"/>
	<xsd:element name="CAFOMLPWAmountGenerated" type="icis:DecimalRange0To999999999999.99Type"/>
	<xsd:element name="CAFOMLPWAmountTransferred" type="icis:DecimalRange0To999999999999.99Type"/>
	<xsd:element name="CAFOMLPWFieldAmounts" type="icis:CAFOMLPWFieldAmounts"/>
	<xsd:element name="CAFOMLPWFieldMaxAllowableAmount" type="icis:DecimalRange0To999999999.99Type"/>
	<xsd:element name="CAFOMLPWFieldActualAmount" type="icis:DecimalRange0To999999999.99Type"/>
	<xsd:element name="CAFOMLPWLandApplicationUnit" type="icis:StringMin1Max3Type"/>
	<xsd:element name="CAFOMLPWNutrientForm" type="icis:StringMin1Max3Type"/>
	<xsd:element name="CAFOMLPWNutrientValue" type="icis:DecimalRange0To999.99Type"/>
	<xsd:element name="CAFOMLPWNutrientValueUnit" type="icis:StringMin1Max3Type"/>
	<xsd:element name="CAFOMLPWUnit" type="icis:StringMin1Max3Type"/>
	<xsd:element name="CAFOMLPWTotalAmounts" type="icis:CAFOMLPWTotalAmounts"/>
	<xsd:element name="CAFOMLPWMaximumAmountMethod" type="icis:StringMin1Max3Type"/>
	<xsd:element name="CAFOMLPWNutrientMonitoring" type="icis:CAFOMLPWNutrientMonitoring"/>
	<xsd:element name="CAFOMLPWStorageWithinDesignCapacity" type="icis:StringMin1Max3Type"/>
	<xsd:element name="CAFOMLPWStorageWithinDesignCapacityText" type="icis:StringMin1Max500Type"/>
	<xsd:element name="CAFONarrativeRateApproachSoilMonitoring" type="icis:CAFONarrativeRateApproachSoilMonitoring"/>
	<xsd:element name="CAFONarrativeRateApproachSupplementalFertilizer" type="icis:CAFONarrativeRateApproachSupplementalFertilizer"/>
	<xsd:element name="CAFOProductionAreaDischarge" type="icis:CAFOProductionAreaDischarge"/>
	<xsd:element name="CAFOProductionAreaDischargeDiscoveryDate" type="icis:DateType"/>
	<xsd:element name="CAFOProductionAreaDischarge24hrRainEventIndicator" type="icis:YesNoIndicatorType"/>
	<xsd:element name="CAFOProductionAreaDischargeDurationHours" type="icis:DecimalRange0To999.99Type"/>
	<xsd:element name="CAFOProductionAreaDischargeVolumeGallons" type="icis:NonNegativeIntegerMin1Max9Type"/>
	<xsd:element name="CAFOSoilMonitoringMeasurementForm" type="icis:StringMin1Max3Type"/>
	<xsd:element name="CAFOSoilMonitoringMeasurementValue" type="icis:DecimalRange0To999.99Type"/>
	<xsd:element name="CAFOSoilMonitoringMeasurementValueUnit" type="icis:StringMin1Max3Type"/>
	<xsd:element name="CAFOSoilMonitoringAnalyticalMethod" type="icis:StringMin1Max3Type"/>
	<xsd:element name="CAFOSoilMonitoringSampleDepthInches" type="icis:PercentRange0To99.9Type"/>
	<xsd:element name="CAFOSoilMonitoringSampleDate" type="icis:DateType"/>
	<xsd:element name="CAFOSupplementalFertilizerMeasurementForm" type="icis:StringMin1Max3Type"/>
	<xsd:element name="CAFOSupplementalFertilizerMeasurementValue" type="icis:DecimalRange0To999.99Type"/>
	<xsd:element name="CAFOSupplementalFertilizerMeasurementValueUnit" type="icis:StringMin1Max3Type"/>
	<xsd:element name="Containment" type="icis:Containment"/>
	<xsd:element name="ContainmentCapacityNumber" type="icis:NonNegativeIntegerMin1Max9Type"/>
	<xsd:element name="ContainmentTypeCode" type="icis:StringMin1Max3Type"/>
	<xsd:element name="DaysOfStorage" type="icis:IntegerRange0To1000Type"/>
	<xsd:element name="DischargesDuringYearProductionAreaIndicator" type="icis:StringMin1Max3Type"/>
	<xsd:element name="DischargesDuringYearLandApplicationAreaIndicator" type="icis:StringMin1Max3Type"/>
