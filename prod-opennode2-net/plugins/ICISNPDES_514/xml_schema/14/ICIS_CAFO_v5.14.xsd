	<xsd:element name="OpenConfinementCount" type="icis:NonNegativeIntegerMin1Max9Type"/>
	<xsd:element name="OtherAnimalTypeName" type="icis:StringMin1Max50Type"/>
	<xsd:element name="OtherContainmentTypeName" type="icis:StringMin1Max50Type"/>
	<xsd:element name="OtherLandApplicationBMPTypeName" type="icis:StringMin1Max50Type"/>
	<xsd:element name="OtherStorageTypeName" type="icis:StringMin1Max50Type"/>
	<xsd:element name="ReportedAnimalType" type="icis:ReportedAnimalType"/>
	<xsd:element name="SolidManureLitterGeneratedAmount" type="icis:NonNegativeIntegerMin1Max9Type"/>
	<xsd:element name="SolidManureLitterTransferAmount" type="icis:NonNegativeIntegerMin1Max9Type"/>
	<xsd:element name="StorageTotalCapacityMeasure" type="icis:NonNegativeIntegerMin1Max9Type"/>
	<xsd:element name="TotalNumbersEachLivestock" type="icis:NonNegativeIntegerMin1Max9Type"/>
	<xsd:group name="AnimalNMPLitterGroup">
		<xsd:sequence>
			<xsd:element ref="icis:SolidManureLitterGeneratedAmount" minOccurs="0"/>
			<xsd:element ref="icis:LiquidManureWastewaterGeneratedAmount" minOccurs="0"/>
			<xsd:element ref="icis:SolidManureLitterTransferAmount" minOccurs="0"/>
			<xsd:element ref="icis:LiquidManureWastewaterTransferAmount" minOccurs="0"/>
			<xsd:element ref="icis:NMPDevelopedCertifiedPlannerApprovedIndicator" minOccurs="0"/>
		</xsd:sequence>
	</xsd:group>
	<xsd:complexType name="ReportedAnimalType">
		<xsd:sequence>
			<xsd:element ref="icis:AnimalTypeCode"/>
			<xsd:element ref="icis:OtherAnimalTypeName" minOccurs="0"/>
			<xsd:element ref="icis:TotalNumbersEachLivestock" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AnimalType">
		<xsd:complexContent>
			<xsd:extension base="icis:ReportedAnimalType">
				<xsd:sequence>
					<xsd:element ref="icis:OpenConfinementCount" minOccurs="0"/>
					<xsd:element ref="icis:HousedUnderRoofConfinementCount" minOccurs="0"/>
					<xsd:element ref="icis:LiquidManureHandlingSystem" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CAFOMLPWTotalAmounts">
		<xsd:sequence>
			<xsd:element ref="icis:CAFOMLPWCode"/>
			<xsd:element ref="icis:CAFOMLPWAmountGenerated" minOccurs="0"/>
			<xsd:element ref="icis:CAFOMLPWAmountTransferred" minOccurs="0"/>
			<xsd:element ref="icis:CAFOMLPWUnit" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CAFOMLPWNutrientMonitoring">
		<xsd:sequence>
			<xsd:element ref="icis:CAFOMLPWCode"/>
			<xsd:element ref="icis:CAFOMLPWNutrientForm" minOccurs="0"/>
			<xsd:element ref="icis:CAFOMLPWNutrientValue" minOccurs="0"/>
			<xsd:element ref="icis:CAFOMLPWNutrientValueUnit" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CAFOProductionAreaDischarge">
		<xsd:sequence>
			<xsd:element ref="icis:CAFOProductionAreaDischargeDiscoveryDate"/>
			<xsd:element ref="icis:CAFOProductionAreaDischarge24hrRainEventIndicator" minOccurs="0"/>
			<xsd:element ref="icis:CAFOProductionAreaDischargeDurationHours" minOccurs="0"/>
			<xsd:element ref="icis:CAFOProductionAreaDischargeVolumeGallons" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CAFOLandApplicationFieldInformation">
		<xsd:sequence>
			<xsd:element ref="icis:CAFOLandApplicationFieldIdentifier"/>
			<xsd:element ref="icis:CAFOLandApplicationFieldAcreage" minOccurs="0"/>
			<xsd:element ref="icis:CAFOMLPWMaximumAmountMethod" minOccurs="0"/>
			<xsd:element ref="icis:CAFOLandApplicationFieldCropInformation" minOccurs="0"/>
			<xsd:element ref="icis:CAFOMLPWFieldAmounts" minOccurs="0"/>
			<xsd:element ref="icis:CAFONarrativeRateApproachSoilMonitoring" minOccurs="0"/>
			<xsd:element ref="icis:CAFONarrativeRateApproachSupplementalFertilizer" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CAFOLandApplicationFieldCropInformation">
		<xsd:sequence>
			<xsd:element ref="icis:CAFOLandApplicationFieldCropIdentifier"/>
			<xsd:element ref="icis:CAFOLandApplicationFieldCropCode" minOccurs="0"/>
			<xsd:element ref="icis:CAFOLandApplicationFieldCropYield" minOccurs="0"/>
			<xsd:element ref="icis:CAFOLandApplicationFieldCropYieldUnit" minOccurs="0"/>
			<xsd:element ref="icis:CAFOLandApplicationFieldCropSeeded" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CAFOMLPWFieldAmounts">
		<xsd:sequence>
			<xsd:element ref="icis:CAFOMLPWCode"/>
			<xsd:element ref="icis:CAFOMLPWFieldMaxAllowableAmount" minOccurs="0"/>
			<xsd:element ref="icis:CAFOMLPWFieldActualAmount" minOccurs="0"/>
			<xsd:element ref="icis:CAFOMLPWLandApplicationUnit" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CAFONarrativeRateApproachSoilMonitoring">
		<xsd:sequence>
			<xsd:element ref="icis:CAFOSoilMonitoringMeasurementForm"/>
			<xsd:element ref="icis:CAFOSoilMonitoringMeasurementValue" minOccurs="0"/>
			<xsd:element ref="icis:CAFOSoilMonitoringMeasurementValueUnit" minOccurs="0"/>
			<xsd:element ref="icis:CAFOSoilMonitoringAnalyticalMethod" minOccurs="0"/>
			<xsd:element ref="icis:CAFOSoilMonitoringSampleDepthInches" minOccurs="0"/>
			<xsd:element ref="icis:CAFOSoilMonitoringSampleDate" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CAFONarrativeRateApproachSupplementalFertilizer">
		<xsd:sequence>
			<xsd:element ref="icis:CAFOSupplementalFertilizerMeasurementForm"/>
			<xsd:element ref="icis:CAFOSupplementalFertilizerMeasurementValue" minOccurs="0"/>
			<xsd:element ref="icis:CAFOSupplementalFertilizerMeasurementValueUnit" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:group name="CAFOClassificationGroup">
		<xsd:sequence>
			<xsd:element ref="icis:CAFOClassificationCode" minOccurs="0"/>
			<xsd:element ref="icis:CAFOContact" minOccurs="0"/>
			<xsd:element ref="icis:CAFOAddress" minOccurs="0"/>
			<xsd:element ref="icis:IsAnimalFacilityTypeCAFOIndicator" minOccurs="0"/>
			<xsd:element ref="icis:CAFODesignationDate" minOccurs="0"/>
			<xsd:element ref="icis:CAFODesignationReasonText" minOccurs="0"/>
		</xsd:sequence>
	</xsd:group>
	<xsd:group name="CAFOInspectionClassificationGroup">
		<xsd:sequence>
			<xsd:element ref="icis:CAFOClassificationCode" minOccurs="0"/>
			<xsd:element ref="icis:IsAnimalFacilityTypeCAFOIndicator" minOccurs="0"/>
			<xsd:element ref="icis:CAFODesignationDate" minOccurs="0"/>
			<xsd:element ref="icis:CAFODesignationReasonText" minOccurs="0"/>
		</xsd:sequence>
	</xsd:group>
	<xsd:group name="ConfinementContainmentGroup">
		<xsd:sequence>
			<xsd:element ref="icis:ManureLitterProcessedWastewaterStorage" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="icis:Containment" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:group>
	<xsd:complexType name="Containment">
		<xsd:sequence>
			<xsd:element ref="icis:ContainmentTypeCode"/>
			<xsd:element ref="icis:OtherContainmentTypeName" minOccurs="0"/>
			<xsd:element ref="icis:ContainmentCapacityNumber" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:group name="DrainageApplicationGroup">
		<xsd:sequence>
			<xsd:element ref="icis:NumberAcresContributingDrainage" minOccurs="0"/>
			<xsd:element ref="icis:ApplicationMeasureAvailableLandNumber" minOccurs="0"/>
		</xsd:sequence>
	</xsd:group>
	<xsd:group name="NMPDetailsGroup">
		<xsd:sequence>
			<xsd:element ref="icis:NMPDevelopedDate" minOccurs="0"/>
			<xsd:element ref="icis:NMPLastUpdatedDate" minOccurs="0"/>
		</xsd:sequence>
	</xsd:group>
	<xsd:group name="EMSGroup">
		<xsd:sequence>
			<xsd:element ref="icis:EnvironmentalManagementSystemIndicator" minOccurs="0"/>
			<xsd:element ref="icis:EMSDevelopedDate" minOccurs="0"/>
			<xsd:element ref="icis:EMSLastUpdatedDate" minOccurs="0"/>
		</xsd:sequence>
	</xsd:group>
	<xsd:group name="CapacityGroup">
		<xsd:sequence>
			<xsd:element ref="icis:LivestockMaximumCapacityNumber" minOccurs="0"/>
			<xsd:element ref="icis:LivestockCapacityDeterminationBasedUponNumber" minOccurs="0"/>
			<xsd:element ref="icis:AuthorizedLivestockCapacityNumber" minOccurs="0"/>
		</xsd:sequence>
	</xsd:group>
