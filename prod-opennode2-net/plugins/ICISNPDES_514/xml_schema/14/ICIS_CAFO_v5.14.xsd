	<xsd:complexType name="LandApplicationBMP">
		<xsd:sequence>
			<xsd:element ref="icis:LandApplicationBMPTypeCode"/>
			<xsd:element ref="icis:OtherLandApplicationBMPTypeName" minOccurs="0"/>
		</xsd:sequence>
