<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:IR="http://www.exchangenetwork.net/schema/IR/0" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.exchangenetwork.net/schema/IR/0" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="0.4">
	<xsd:annotation>
		<xsd:documentation>
		Schema Name:	IR_Shared_v0.4.xsd
		Description:	Shared data types for ATTAINS submissions
		Developed By:	Dwane Young, U.S. EPA
		</xsd:documentation>
	</xsd:annotation>

	<xsd:element name="ActionComment" type="IR:ActionCommentDataType">
		<xsd:annotation>
			<xsd:documentation>Free text providing additional comments on the  action</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="ActionIdentifier" type="IR:ActionIdentifierDataType">
		<xsd:annotation>
			<xsd:documentation>Unique code identifying the action</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="ActionStatusCode" type="IR:ActionStatusCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Status of the Action (i.e. Final, Draft, Public Comment, etc.)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="ActionTypeCode" type="IR:ActionTypeCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code identifying the type of action being taken</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="ActualCompletionDate" type="IR:ActualCompletionDateDataType">
		<xsd:annotation>
			<xsd:documentation>Completion date for the action</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="AgencyCode" type="IR:AgencyCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code identifying the agency responsible for the action (S=State, E=EPA, T=Tribal)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="AlternateListingIdentifier" type="IR:AlternateListingIdentifierDataType">
		<xsd:annotation>
			<xsd:documentation>Unique identifier for the listed water.  Use this identifier ONLY if the listing identifier is different from the Assessment Unit Identifier</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="AssessmentBasisCode" type="IR:AssessmentBasisCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code representing the basis for the assessment; is it based on monitored data, extrapolated data, or both.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="AssessmentCommentsText" type="IR:AssessmentCommentsTextDataType">
		<xsd:annotation>
			<xsd:documentation>Free text for providing additional comments on the assessment.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="AssessmentConfidenceCode" type="IR:AssessmentConfidenceCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating the confidence in the AssessmentType</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="AssessmentDate" type="IR:AssessmentDateDataType">
		<xsd:annotation>
			<xsd:documentation>Day on which the assessment was completed</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="AssessmentTypeCode" type="IR:AssessmentTypeCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code representing the type of assessment that was performed.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="AssessmentUnitCommentText" type="IR:AssessmentUnitCommentTextDataType">
		<xsd:annotation>
			<xsd:documentation>Text to provide a comment on a specific Assessment Unit</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="AssessmentUnitIdentifier" type="IR:AssessmentUnitIdentifierDataType">
		<xsd:annotation>
			<xsd:documentation>A unique identifier assigned to the Assessment Unit by the reporting organization</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="AssessmentUnitName" type="IR:AssessmentUnitNameDataType">
		<xsd:annotation>
			<xsd:documentation>Name of Assessment Unit (i.e. GNIS Name)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="AssessorName" type="IR:AssessorNameDataType">
		<xsd:annotation>
			<xsd:documentation>Name of the individual performing the assessment</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="AssociatedActionIdentifier" type="IR:AssociatedActionIdentifierDataType">
		<xsd:annotation>
			<xsd:documentation>Unique code identifying the Action that corresponds to this cause.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="AssociatedUseName" type="IR:AssociatedUseNameDataType">
		<xsd:annotation>
			<xsd:documentation>Name of the designated use.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="CauseName" type="IR:CauseNameDataType">
		<xsd:annotation>
			<xsd:documentation>Name of the cause</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="ChangeCycleText" type="IR:ChangeCycleTextDataType">
		<xsd:annotation>
			<xsd:documentation>First Assessment Cycle when then change occurred</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="ChangeDescriptionText" type="IR:ChangeDescriptionTextDataType">
		<xsd:annotation>
			<xsd:documentation>Text describing the change made to the Assessment Unit</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="ChangeTypeText" type="IR:ChangeTypeTextDataType">
		<xsd:annotation>
			<xsd:documentation>Free text describing the relationship between the TMDLs</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="ConsentDecreeCycleText" type="IR:ConsentDecreeCycleTextDataType">
		<xsd:annotation>
			<xsd:documentation>Cycle for which Consent Decree actions are due</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="ContactType" type="IR:ContactTypeDataType">
		<xsd:annotation>
			<xsd:documentation>Area of responsibility for this contact (TMDL Program, 303(d) Program, Monitoring, Assessment, NPS)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="CWA303dPriorityRankingText" type="IR:CWA303dPriorityRankingTextDataType">
		<xsd:annotation>
			<xsd:documentation>CWA 303(d) priority for developing a TMDL (i.e. High, Medium, Low)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="CycleBaseLine" type="IR:CycleBaseLineDataType">
		<xsd:annotation>
			<xsd:documentation>Cycle that is the baseline for the priority (starting cycle)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="CycleExpectedToAttainText" type="IR:CycleExpectedToAttainTextDataType">
		<xsd:annotation>
			<xsd:documentation>Cycle by which the Assessment Unit is expected to attain its standards (used to indicate whether or not this cause should be considered towards category 4B)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="CycleFirstListedText" type="IR:CycleFirstListedTextDataType">
		<xsd:annotation>
			<xsd:documentation>Cycle the Assessment Unit was first listed for this cause</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="CycleGoal" type="IR:CycleGoalDataType">
		<xsd:annotation>
			<xsd:documentation>Cycle that is the goal for the priority (ending cycle)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="CycleLastAssessedText" type="IR:CycleLastAssessedTextDataType">
		<xsd:annotation>
			<xsd:documentation>Cycle this Assessment Unit was last assessed, which can include any conclusions related to this Assessment Unit and can include delisting decisions.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="CycleScheduledForTMDLText" type="IR:CycleScheduledForTMDLTextDataType">
		<xsd:annotation>
			<xsd:documentation>Cycle when the jurisdiction anticipates submitting the TMDL for EPA approval</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="DelistingCommentText" type="IR:DelistingCommentTextDataType">
		<xsd:annotation>
			<xsd:documentation>Free text for providing additional comments on the delisting</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="DelistingReasonCode" type="IR:DelistingReasonCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Reason the waterbody/cause is being delisted</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="DocumentComments" type="IR:DocumentCommentsDataType">
		<xsd:annotation>
			<xsd:documentation>Free text for providing additional comments on the document.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="DocumentDescription" type="IR:DocumentDescriptionDataType">
		<xsd:annotation>
			<xsd:documentation>Description of the document</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="DocumentFileName" type="IR:DocumentFileNameDataType">
		<xsd:annotation>
			<xsd:documentation>File of the document.  Should be the file name of the document, including the extension (.doc, .pdf, etc.).  The document must be included in a .zip file with this name and included in the submission along with the XML file.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="DocumentFileType" type="IR:DocumentFileTypeDataType">
		<xsd:annotation>
			<xsd:documentation>The file extension of the document</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="DocumentIdentifier" type="IR:DocumentIdentifierDataType">
		<xsd:annotation>
			<xsd:documentation>A unique identifier for the Document</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="DocumentName" type="IR:DocumentNameDataType">
		<xsd:annotation>
			<xsd:documentation>Name of the document</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="DocumentTypeCode" type="IR:DocumentTypeCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Type of document being provided</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="EmailAddressText" type="IR:EmailAddressTextDataType">
		<xsd:annotation>
			<xsd:documentation>Email address for requesting additional information</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="EPAActionCode" type="IR:EPAActionCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating actions that EPA has taken on this Action, including the approval of TMDLs</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="ExplicitMarginofSafetyText" type="IR:ExplicitMarginofSafetyTextDataType">
		<xsd:annotation>
			<xsd:documentation>Explicit margin of safety for the load allocation</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="FaxNumberText" type="IR:FaxNumberTextDataType">
		<xsd:annotation>
			<xsd:documentation>Fax Number</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="FirstName" type="IR:FirstNameDataType">
		<xsd:annotation>
			<xsd:documentation>First name of a person.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="ImplicitMarginofSafetyText" type="IR:ImplicitMarginofSafetyTextDataType">
		<xsd:annotation>
			<xsd:documentation>Implicit margin of safety for the load allocation</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="IndianCountryIndicator" type="IR:IndianCountryIndicatorDataType">
		<xsd:annotation>
			<xsd:documentation>Indicates if the water is either wholly or partially in Indian country.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="IndividualTitleText" type="IR:IndividualTitleTextDataType">
		<xsd:annotation>
			<xsd:documentation>Title of the contact person.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="JustificationURLText" type="IR:JustificationURLTextDataType">
		<xsd:annotation>
			<xsd:documentation>URL providing the link to find more information about the 4B activity.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="LastName" type="IR:LastNameDataType">
		<xsd:annotation>
			<xsd:documentation>Last name of a person.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="LocationDescriptionText" type="IR:LocationDescriptionTextDataType">
		<xsd:annotation>
			<xsd:documentation>Text description of the Assessment Unit location (describes the extent of the Assessment Unit)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="LocationText" type="IR:LocationTextDataType">
		<xsd:annotation>
			<xsd:documentation>Value for the location</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="LocationTypeCode" type="IR:LocationTypeCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Description of the type of location (i.e. 8-digit HUC, County, etc.)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="LocationTypeContext" type="IR:LocationTypeContextDataType">
		<xsd:annotation>
			<xsd:documentation>Context for the LocationTypeCode (typically OrganizationIdentifier)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="MailingAddressCityName" type="IR:MailingAddressCityNameDataType">
		<xsd:annotation>
			<xsd:documentation>City or Locality Name</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="MailingAddressCountryCode" type="IR:MailingAddressCountryCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Country Name</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="MailingAddressStateUSPSCode" type="IR:MailingAddressStateUSPSCodeDataType">
		<xsd:annotation>
			<xsd:documentation>State USPS Code (i.e. KS)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="MailingAddressText" type="IR:MailingAddressTextDataType">
		<xsd:annotation>
			<xsd:documentation>Mailing Address </xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="MailingAddressZIPCode" type="IR:MailingAddressZIPCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Zip Code</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="MethodTypeCode" type="IR:MethodTypeCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code for the Assesment Method</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="MethodTypeContext" type="IR:MethodTypeContextDataType">
		<xsd:annotation>
			<xsd:documentation>Context for the MethodTypeName</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="MethodTypeName" type="IR:MethodTypeNameDataType">
		<xsd:annotation>
			<xsd:documentation>Name of the method used</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="MiddleInitial" type="IR:MiddleInitialDataType">
		<xsd:annotation>
			<xsd:documentation>Middle Initial of a person.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="ModificationTypeCode" type="IR:ModificationTypeCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code representing the type of modification that was made.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="MonitoringDataLinkText" type="IR:MonitoringDataLinkTextDataType">
		<xsd:annotation>
			<xsd:documentation>URL providing the link to the monitoring data for this station.  For Water Quality Portal data, this link should be the Restful URL that would provide access to the data.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="MonitoringEndDate" type="IR:MonitoringEndDateDataType">
		<xsd:annotation>
			<xsd:documentation>Date on which monitoring ended</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="MonitoringLocationIdentifier" type="IR:MonitoringLocationIdentifierDataType">
		<xsd:annotation>
			<xsd:documentation>Unique identifier for the monitoring location</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="MonitoringOrganizationIdentifier" type="IR:MonitoringOrganizationIdentifierDataType">
		<xsd:annotation>
			<xsd:documentation>Organization that conducted the monitoring.  For data that is available via STORET/WQX or the Water Quality portal, this Organization ID is a unique ID assigned by EPA, or other agency responsible for assigning organizations.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="MonitoringStartDate" type="IR:MonitoringStartDateDataType">
		<xsd:annotation>
			<xsd:documentation>Date on which monitoring began</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="NPDESIdentifier" type="IR:NPDESIdentifierDataType">
		<xsd:annotation>
			<xsd:documentation>Unique identifier for a permit as assigned by the NPDES program</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="OrganizationIdentifier" type="IR:OrganizationIdentifierDataType">
		<xsd:annotation>
			<xsd:documentation>A unique identifier assigned to the organization.  Identifiers would be managed centrally by EPA</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="OrganizationName" type="IR:OrganizationNameDataType">
		<xsd:annotation>
			<xsd:documentation>Name corresponding to unique organization ID (i.e. Utah Division of Water Quality); org name will be ignored on submission, for outbound services only</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="OrganizationType" type="IR:OrganizationTypeDataType">
		<xsd:annotation>
			<xsd:documentation>State/tribe/territory; org name will be ignored on submission, for outbound services only</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="OtherIdentifier" type="IR:OtherIdentifierDataType">
		<xsd:annotation>
			<xsd:documentation>Other state identifier for a permit</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="ParameterAttainmentCode" type="IR:ParameterAttainmentCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating the attainment status for this parameter for this specific use.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="ParameterCommentText" type="IR:ParameterCommentTextDataType">
		<xsd:annotation>
			<xsd:documentation>Free text providing additional comments for the parameter</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="ParameterName" type="IR:ParameterNameDataType">
		<xsd:annotation>
			<xsd:documentation>Name of the parameter</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="ParameterStatusName" type="IR:ParameterStatusNameDataType">
		<xsd:annotation>
			<xsd:documentation>Status for the parameter indicating whether this parameter is a cause, observed effect, or provided for informational purposes as a monitored parameter.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="PhoneExtensionText" type="IR:PhoneExtensionTextDataType">
		<xsd:annotation>
			<xsd:documentation>Telephone number extension</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="PlannedCompletionDate" type="IR:PlannedCompletionDateDataType">
		<xsd:annotation>
			<xsd:documentation>Date the action is planned to be completed</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="PollutantIndicator" type="IR:PollutantIndicatorDataType">
		<xsd:annotation>
			<xsd:documentation>Flag indicating whether or not the cause is a pollutant</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="PollutantName" type="IR:PollutantNameDataType">
		<xsd:annotation>
			<xsd:documentation>Name of the pollutant</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="PollutantSourceTypeCode" type="IR:PollutantSourceTypeCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Are the sources of the pollutant point source, nonpoint source or both.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="PriorCauseCycleText" type="IR:PriorCauseCycleTextDataType">
		<xsd:annotation>
			<xsd:documentation>Cycle for the prior cause that is being replaced with or related to the cause.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="PriorCauseName" type="IR:PriorCauseNameDataType">
		<xsd:annotation>
			<xsd:documentation>Prior name for the cause for this Assessment Unit</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="PriorityCommentText" type="IR:PriorityCommentTextDataType">
		<xsd:annotation>
			<xsd:documentation>Free text providing additional comments on the priority</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="PriorityDescription" type="IR:PriorityDescriptionDataType">
		<xsd:annotation>
			<xsd:documentation>Description of the priority</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="PriorityIdentifier" type="IR:PriorityIdentifierDataType">
		<xsd:annotation>
			<xsd:documentation>Unique identifier for a priority</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="PriorityName" type="IR:PriorityNameDataType">
		<xsd:annotation>
			<xsd:documentation>Name for the priority</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="PriorityStatus" type="IR:PriorityStatusDataType">
		<xsd:annotation>
			<xsd:documentation>Status of the Prority</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="PriorityStatusDate" type="IR:PriorityStatusDateDataType">
		<xsd:annotation>
			<xsd:documentation>Date the status was changed</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="PriorityTypeCode" type="IR:PriorityTypeCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code identifying the type of priority that is being set</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="RationaleText" type="IR:RationaleTextDataType">
		<xsd:annotation>
			<xsd:documentation>Rationale for the assessment conclusion.  This text will be avaialble for the public.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="ReportingCycleText" type="IR:ReportingCycleTextDataType">
		<xsd:annotation>
			<xsd:documentation>Reporting cycle for the Assessments</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="ReportStatusCode" type="IR:ReportStatusCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Status of the report or 303(d) list (i.e. Final, Draft, Public Comment, etc.)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="ReviewCommentDate" type="IR:ReviewCommentDateDataType">
		<xsd:annotation>
			<xsd:documentation>The date the comment was made</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="ReviewCommentText" type="IR:ReviewCommentTextDataType">
		<xsd:annotation>
			<xsd:documentation>The actual comment</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="ReviewCommentUserName" type="IR:ReviewCommentUserNameDataType">
		<xsd:annotation>
			<xsd:documentation>The userName of the commenter</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="SeasonEndText" type="IR:SeasonEndTextDataType">
		<xsd:annotation>
			<xsd:documentation>The month and day for the end of when the parameter was evaluated or when the Action applies (DD/MM)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="SeasonStartText" type="IR:SeasonStartTextDataType">
		<xsd:annotation>
			<xsd:documentation>The month and day for the start of when the parameter was evaluated or when the Action applies (DD/MM)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="SizeEstimationMethodCode" type="IR:SizeEstimationMethodCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code representing the method used for determining the water size</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="SizeSourceScaleText" type="IR:SizeSourceScaleTextDataType">
		<xsd:annotation>
			<xsd:documentation>Text describing the scale of the source material used for estimating the water size (i.e. 1:24000)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="SizeSourceText" type="IR:SizeSourceTextDataType">
		<xsd:annotation>
			<xsd:documentation>Text describing the source used for estimating the water size.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="SourceCommentText" type="IR:SourceCommentTextDataType">
		<xsd:annotation>
			<xsd:documentation>Free text for providing additional comments on the source</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:element name="SourceConfirmedIndicator" type="IR:SourceConfirmedIndicatorDataType">
		<xsd:annotation>
			<xsd:documentation>Indicator of whether the source has been confirmed</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

