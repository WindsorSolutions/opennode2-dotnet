<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:IR="http://www.exchangenetwork.net/schema/IR/1" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.exchangenetwork.net/schema/IR/1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<xsd:include schemaLocation="IR_MailingAddress_v1.0.xsd"/>
	<xsd:include schemaLocation="IR_Shared_v1.0.xsd"/>
	<xsd:annotation>
		<xsd:documentation>
		Schema Name:	IR_OrganizationContact_v1.0.xsd
		Description:	Individual Organization Contact Information: who should be contacted for more information. 
		Developed By:	Dwane Young, U.S. EPA
		</xsd:documentation>
	</xsd:annotation>
	<xsd:element name="OrganizationContact" type="IR:OrganizationContactDataType">
		<xsd:annotation>
			<xsd:documentation>Individual Organization Contact Information: who should be contacted for more information. </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="OrganizationContactDataType">
		<xsd:annotation>
			<xsd:documentation>Individual Organization Contact Information: who should be contacted for more information. </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="IR:ContactTypeText"/>
			<xsd:element ref="IR:WebURLText"/>
			<xsd:element ref="IR:SubOrganizationName" minOccurs="0"/>
			<xsd:element ref="IR:EmailAddressText" minOccurs="0"/>
			<xsd:element ref="IR:FirstName" minOccurs="0"/>
			<xsd:element ref="IR:MiddleInitial" minOccurs="0"/>
			<xsd:element ref="IR:LastName" minOccurs="0"/>
			<xsd:element ref="IR:IndividualTitleText" minOccurs="0"/>
			<xsd:element ref="IR:TelephoneNumberText" minOccurs="0"/>
			<xsd:element ref="IR:PhoneExtensionText" minOccurs="0"/>
			<xsd:element ref="IR:FaxNumberText" minOccurs="0"/>
			<xsd:element ref="IR:MailingAddress" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
