            <xsd:element minOccurs="0" maxOccurs="1" ref="sc:PermitName" />
            <xsd:element minOccurs="0" maxOccurs="1" ref="sc:OtherPermitIdentifier" />
            <xsd:element minOccurs="0" maxOccurs="1" ref="sc:ProgramName" />
            <xsd:element minOccurs="0" maxOccurs="1" ref="sc:PermitType" />
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="PermitIdentity" type="sc:PermitIdentityDataType">
        <xsd:annotation>
            <xsd:documentation>Identification information about the permit and the organization responsible for issuing or granting the permit.</xsd:documentation>
        </xsd:annotation>
