    <!--Data type include file-->
    <xsd:include schemaLocation="AQS_TYPES/EN_AQS_DecimalTenPointFiveDataType_v2.2.xsd"/>
    <!--Statistical Percentile data elements-->
    <xsd:element name="TenthPercentileValue" type="aqs:TenPointFiveDecimalType">
        <xsd:annotation>
            <xsd:documentation>The sample value occurring in the tenth percentile of the yearly data set when sorted from lowest to highest.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
