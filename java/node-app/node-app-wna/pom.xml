        <version>2.14</version>
    </parent>
    <artifactId>node-app-wna</artifactId>
    <packaging>war</packaging>
    <name>Node Application - Web Node Administration</name>
    <properties>
        <wna.extra.resources.location>${project.basedir}/src/main/env/${environment}</wna.extra.resources.location>
    </properties>
    <build>
        <finalName>wna</finalName>
        <resources>
            <resource>
                <directory>${wna.extra.resources.location}</directory>
            </resource>
            <resource>
