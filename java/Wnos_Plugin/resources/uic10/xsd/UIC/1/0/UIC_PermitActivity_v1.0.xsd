<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:uic="http://www.exchangenetwork.net/schema/uic/1" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.exchangenetwork.net/schema/uic/1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<xsd:include schemaLocation="UIC_SimpleContent_v1.0.xsd"/>
	<xsd:annotation>
		<xsd:documentation>
        Schema Name  : UIC_PermitActivity_v1.0.xsd
        Description   : information on activities related to permit or rule authorized (e.g. permit issuance, authorized file review, etc.)	.
        Developed by  : 	 Lockheed Martin
        Point of Contact :   Lockheed Martin
</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="PermitActivityDetailType">
		<xsd:annotation>
			<xsd:documentation>Defines the Permit Activity information structure.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="uic:PermitActivityIdentifier"/>
			<xsd:element ref="uic:PermitActivityActionTypeCode"/>
			<xsd:element ref="uic:PermitActivityDate"/>
			<xsd:element ref="uic:PermitActivityPermitIdentifier"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="PermitActivityDetail" type="uic:PermitActivityDetailType"/>
</xsd:schema>
