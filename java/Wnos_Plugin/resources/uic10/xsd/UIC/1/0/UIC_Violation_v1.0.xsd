<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:uic="http://www.exchangenetwork.net/schema/uic/1" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.exchangenetwork.net/schema/uic/1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<xsd:include schemaLocation="UIC_SimpleContent_v1.0.xsd"/>
	<xsd:annotation>
		<xsd:documentation>
        Schema Name  : UIC_Violation_v1.0.xsd
        Description   : data identifying non-compliance with UIC regulation reported for regulated wells.
        Developed by  : 	 Lockheed Martin
        Point of Contact :   Lockheed Martin
</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="ViolationDetailType">
		<xsd:annotation>
			<xsd:documentation>Defines the Violation information structure.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="uic:ViolationIdentifier"/>
			<xsd:element ref="uic:ViolationContaminationCode" minOccurs="0"/>
			<xsd:element ref="uic:ViolationEndangeringCode" minOccurs="0"/>
			<xsd:element ref="uic:ViolationReturnComplianceDate" minOccurs="0"/>
			<xsd:element ref="uic:ViolationSignificantCode" minOccurs="0"/>
			<xsd:element ref="uic:ViolationDeterminedDate"/>
			<xsd:element ref="uic:ViolationTypeCode"/>
			<xsd:element ref="uic:ViolationWellIdentifier" minOccurs="0"/>
			<xsd:element ref="uic:ViolationFacilityIdentifier" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="ViolationDetail" type="uic:ViolationDetailType"/>
</xsd:schema>
