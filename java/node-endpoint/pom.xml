<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>com.windsor.node</groupId>
        <artifactId>node-parent</artifactId>
        <version>2.01</version>
    </parent>

    <artifactId>node-endpoint</artifactId>
    <packaging>war</packaging>
    <name>Node Endpoint v1.1</name>
    <build>
        <finalName>wne</finalName>
        <resources>
            <resource>
                <directory>src/main/resources</directory>
            </resource>
            <resource>
                <directory>${wne.extra.resources.location}</directory>
            </resource>
        </resources>
    </build>
    <properties>
        <extra.resources.location>src/main/env/ecos-generic</extra.resources.location>
        <wne.extra.resources.location>${extra.resources.location}</wne.extra.resources.location>
    </properties>
    <profiles>
        <profile>
            <id>ecos-generic</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <extra.resources.location>src/main/env/ecos-generic</extra.resources.location>
                <wne.extra.resources.location>${extra.resources.location}</wne.extra.resources.location>
                <build.profile>ecos-generic</build.profile>
            </properties>
        </profile>
    </profiles>
