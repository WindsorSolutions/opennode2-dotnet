<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:icis="http://www.exchangenetwork.net/schema/icis/4" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.exchangenetwork.net/schema/icis/4" elementFormDefault="qualified" attributeFormDefault="unqualified" version="4">
	
	<xsd:include schemaLocation="ICIS_KeyElements_v4.0e.xsd"/>
	<xsd:include schemaLocation="ICIS_ScheduleEvent_v4.0e.xsd"/>
	
	<xsd:annotation>
		<xsd:documentation>Schema Name: ICIS_HistoricalPermitScheduleEvents_v4.0e.xsd</xsd:documentation>
		<xsd:documentation>Current Version Available: http://www.exchangenetwork.net/schema/icis/4</xsd:documentation>
		<xsd:documentation>Description: ICIS-NPDES historical permit schedule events data input format</xsd:documentation>
		<xsd:documentation>Application: ICIS-NPDES</xsd:documentation>
		<xsd:documentation>Developed By: U.S. Environmental Protection Agency</xsd:documentation>
		<xsd:documentation>Point of Contact: EPA/Alison Kittle</xsd:documentation>
		<xsd:documentation>Data Written: Nov 2010</xsd:documentation>
		<xsd:documentation>Last Date Modified: 2011-11-07</xsd:documentation>
	</xsd:annotation>
	
	<xsd:element name="HistoricalPermitScheduleEvents" type="icis:HistoricalPermitScheduleEvents"/>
	
	<xsd:complexType name="HistoricalPermitScheduleEventsData">
		<xsd:sequence>
			<xsd:element ref="icis:TransactionHeader"/>
			<xsd:element ref="icis:HistoricalPermitScheduleEvents"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="HistoricalPermitScheduleEvents">
		<xsd:complexContent>
			<xsd:extension base="icis:HistoricalPermitScheduleKeyElements">
				<xsd:sequence>
					<xsd:group ref="icis:ScheduleEventGroup"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
</xsd:schema>
