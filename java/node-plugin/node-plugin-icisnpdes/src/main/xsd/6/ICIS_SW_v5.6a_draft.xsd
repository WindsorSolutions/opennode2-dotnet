		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TreatmentChemicalsList">
		<xsd:sequence>
			<xsd:element ref="icis:TreatmentChemicalName" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
