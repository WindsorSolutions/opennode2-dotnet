			<inheritance:implements>com.windsor.node.plugin.icisnpdes.domain.IContactList</inheritance:implements>
		</jaxb:bindings>
	</jaxb:bindings>

	<!-- storm water contact -->

	<jaxb:bindings>
		<jaxb:bindings node="//xsd:complexType[@name='StormWaterContact']">
			<hj:embeddable />
			<inheritance:implements>com.windsor.node.plugin.icisnpdes.domain.IContactList</inheritance:implements>
		</jaxb:bindings>
	</jaxb:bindings>

</jaxb:bindings>