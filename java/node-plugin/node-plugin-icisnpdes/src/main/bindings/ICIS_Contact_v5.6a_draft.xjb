	</jaxb:bindings>
	
	<!-- inspection government contact -->

	<jaxb:bindings>
		<jaxb:bindings node="//xsd:complexType[@name='InspectionGovernmentContact']">
			<hj:ignored />
		</jaxb:bindings>
	</jaxb:bindings>

	<!-- permit contact -->

	<jaxb:bindings>
		<jaxb:bindings node="//xsd:complexType[@name='PermitContact']">
			<hj:embeddable />
