<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<jaxb:bindings version="2.1" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:hj="http://hyperjaxb3.jvnet.org/ejb/schemas/customizations"
	xmlns:annox="http://annox.dev.java.net" xmlns:orm="http://java.sun.com/xml/ns/persistence/orm"
	jaxb:extensionBindingPrefixes="hj orm annox">

	<jaxb:bindings schemaLocation="../xsd/0/ICIS_DMRProgramReportLinkage_v4.0e.xsd">

		<!-- DMR program report linkage -->

		<jaxb:bindings>

			<jaxb:bindings node="//xsd:complexType[@name='DMRProgramReportLinkage']">
				<hj:embeddable />
			</jaxb:bindings>

			<jaxb:bindings node="//xsd:element[@ref='icis:LinkageBiosolidsReport']">
				<hj:many-to-one>
					<orm:join-column name="ICS_DMR_PROG_REP_LNK_ID" insertable="false" updatable="false" />
				</hj:many-to-one>
			</jaxb:bindings>

			<jaxb:bindings node="//xsd:element[@ref='icis:LinkageSWEventReport']">
				<hj:many-to-one>
					<orm:join-column name="ICS_DMR_PROG_REP_LNK_ID" insertable="false" updatable="false" />
				</hj:many-to-one>
			</jaxb:bindings>

		</jaxb:bindings>

	</jaxb:bindings>

</jaxb:bindings>