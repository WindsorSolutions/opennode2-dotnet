<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:RC="http://www.exchangenetwork.net/schema/RCRA/4" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.exchangenetwork.net/schema/RCRA/4" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="4.0">
	<xsd:include schemaLocation="./RCRA_Shared_v4.0.xsd"/>
	<xsd:include schemaLocation="./RCRA_Payment_v4.0.xsd"/>
	<xsd:annotation>
		<xsd:documentation>
		Schema Name : 	RCRA_Penalty_v4.0
		Description : 	
		Developed by : 	 Windsor Solutions, Inc.
	</xsd:documentation>
	</xsd:annotation>
	<xsd:element name="Penalty" type="RC:PenaltyDataType">
		<xsd:annotation>
			<xsd:documentation>Compliance Monitoring and Enforcement Penalty Data</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="PenaltyDataType">
		<xsd:sequence>
			<xsd:element ref="RC:TransactionCode" minOccurs="0"/>
			<xsd:element ref="RC:PenaltyTypeOwner"/>
			<xsd:element ref="RC:PenaltyType"/>
			<xsd:element ref="RC:CashCivilPenaltySoughtAmount" minOccurs="0"/>
			<xsd:element ref="RC:Notes" minOccurs="0"/>
			<xsd:element ref="RC:Payment" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
