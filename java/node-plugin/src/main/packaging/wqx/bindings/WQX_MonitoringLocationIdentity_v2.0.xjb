<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<jaxb:bindings version="2.1" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:hj="http://hyperjaxb3.jvnet.org/ejb/schemas/customizations" xmlns:orm="http://java.sun.com/xml/ns/persistence/orm"
	jaxb:extensionBindingPrefixes="hj orm" schemaLocation="../xsd/WQX_MonitoringLocationIdentity_v2.0.xsd">

	<!-- monitoring location identity -->

	<jaxb:bindings>
		
		<jaxb:bindings node="//xsd:complexType[@name='MonitoringLocationIdentityDataType']">
			<hj:embeddable />
		</jaxb:bindings>
		
		<jaxb:bindings node="//xsd:element[@ref='wqx:MonitoringLocationIdentifier']">
			<hj:basic>
				<orm:column name="MONITORINGLOCATIONID"/>
			</hj:basic>
		</jaxb:bindings>
		
		<jaxb:bindings node="//xsd:element[@ref='wqx:MonitoringLocationTypeName']">
			<hj:basic>
				<orm:column name="MONLOCTYPE"/>
			</hj:basic>
		</jaxb:bindings>
		
	</jaxb:bindings>

</jaxb:bindings>

