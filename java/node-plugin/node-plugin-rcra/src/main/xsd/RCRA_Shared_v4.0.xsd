<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:RC="http://www.exchangenetwork.net/schema/RCRA/4" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.exchangenetwork.net/schema/RCRA/4" elementFormDefault="qualified" attributeFormDefault="unqualified" version="4.0">
	<xsd:annotation>
		<xsd:documentation>
		Schema Name : 	RCRA_Shared_v4.0.xsd
		Description : 	Shared data types for RCRA submissions
		Developed by : 	 Windsor Solutions, Inc.
		</xsd:documentation>
	</xsd:annotation>
	<!-- Handler elements -->
	<xsd:element name="PhoneExtensionText" type="RC:PhoneExtensionTextDataType">
		<xsd:annotation>
			<xsd:documentation>Telephone number extension</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AccumulatedWasteCode" type="RC:AccumulatedWasteCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating that the handler is engaged in accumulating waste on site.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>	
	<xsd:element name="ActivityLocationCode" type="RC:ActivityLocationCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Indicates the location of the agency regulating the handler.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CertificationSequenceNumber" type="RC:CertificationSequenceNumberDataType">
		<xsd:annotation>
			<xsd:documentation>Sequence number for each certification for the handler.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CurrentEndDate" type="RC:CurrentEndDateDataType">
		<xsd:annotation>
			<xsd:documentation>Date indicating when the owner/operator changed to a different owner/operator.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CurrentStartDate" type="RC:CurrentStartDateDataType">
		<xsd:annotation>
			<xsd:documentation>Date indicating when the owner/operator became current.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DUNSID" type="RC:DUNSIdentifierDataType">
		<xsd:annotation>
			<xsd:documentation>Dunn and Bradstreet number used as an identifier</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ElectronicAddress" type="RC:EmailAddressDataType">
		<xsd:annotation>
			<xsd:documentation>Email address associated with the permit contact.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EmailAddressText" type="RC:EmailAddressTextDataType">
		<xsd:annotation>
			<xsd:documentation>Email address data</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EstimatedQuantity" type="RC:EstimatedQuantityDataType">
		<xsd:annotation>
			<xsd:documentation>The quantity of waste that is handled by each process code. This element pertains only to Part A submissions.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="FacilitySiteName" type="RC:FacilitySiteNameDataType">
		<xsd:annotation>
			<xsd:documentation>The legal name of the site.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="FirstName" type="RC:FirstNameDataType">
		<xsd:annotation>
			<xsd:documentation>First name of a person.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="FuelBurnerCode" type="RC:FuelBurnerCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating that the handler is engaged in the burning of used oil fuel.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="FurnaceExemptionCode" type="RC:FurnaceExemptionCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating that the handler qualifies for the Smelting, Melting, and Refining Furnace Exemption.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="GeneratedHandlerCode" type="RC:GeneratedHandlerCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating that the handler is engaged in generating waste on site.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="HandlerID" type="RC:HandlerIDDataType">
		<xsd:annotation>
			<xsd:documentation>Number that uniquely identifies the handler.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="IndividualTitleText" type="RC:IndividualTitleTextDataType">
		<xsd:annotation>
			<xsd:documentation>Title of the person who certified the handler information reported to the authorizing agency.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ImporterActivityCode" type="RC:ImporterActivityCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating that the handler is engaged in importing hazardous waste into the United States.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="LandTypeCode" type="RC:LandTypeCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating current ownership status of the land on which the facility is located.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="LastName" type="RC:LastNameDataType">
		<xsd:annotation>
			<xsd:documentation>Last name of a person.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="MarketBurnerCode" type="RC:MarketBurnerCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating that the handler directs shipments of used oil to burners.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="MiddleInitial" type="RC:MiddleInitialDataType">
		<xsd:annotation>
			<xsd:documentation>Middle initial of a person.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="MixedWasteGeneratorCode" type="RC:MixedWasteGeneratorCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating that the handler is engaged in generating mixed waste (waste that is both hazardous and radioactive).</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="MeasurementTypeCode" type="RC:MeasurementTypeCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating the unit of measure of the associated design capacity.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="NAICSCode" type="RC:NAICSCodeDataType">
		<xsd:annotation>
			<xsd:documentation>The North American Industry Classification System Code that identifies the business activities of the facility.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="NAICSOwnerCode" type="RC:NAICSOwnerCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Indicates the agency that defines the NAICS Code.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="NAICSSequenceNumber" type="RC:NAICSSequenceNumberDataType">
		<xsd:annotation>
			<xsd:documentation>Sequence number for each NAICS code for the handler.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="OnsiteBurnerExemptionCode" type="RC:OnsiteBurnerExemptionCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating that the handler qualifies for the Small Quantity Onsite Burner Exemption.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="OrganizationFormalName" type="RC:OrganizationFormalNameDataType">
		<xsd:annotation>
			<xsd:documentation>The legal, formal name of an organization that is affiliated with the facility site.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="OwnerOperatorIndicator" type="RC:OwnerOperatorIndicatorDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating whether the data is associated with a current or previous owner or operator. The system will allow multiple current owners and operators.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="OwnerOperatorSequenceNumber" type="RC:OwnerOperatorSequenceNumberDataType">
		<xsd:annotation>
			<xsd:documentation>Sequential number used to order multiple occurrences of owners and operators.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="OwnerOperatorTypeCode" type="RC:OwnerOperatorTypeCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating the owner/operator type.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PhoneExtentionNumber" type="RC:PhoneExtentionNumberDataType">
		<xsd:annotation>
			<xsd:documentation>Telephone number extension associated with the contact.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ProcessorCode" type="RC:ProcessorCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating that the handler is engaged in processing used oil activities.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RecyclerActivityCode" type="RC:RecyclerActivityCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating that the handler is engaged in recycling hazardous waste.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ResponsibleAgencyCode" type="RC:ResponsibleAgencyCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating the agency responsible for conducting the evaluation.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ResponsiblePersonDataOwnerCode" type="RC:ResponsiblePersonDataOwnerCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Indicates the agency that defines the person identifier.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ResponsiblePersonID" type="RC:ResponsiblePersonIDDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating the person within the agency responsible for conducting the evaluation.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ResponsibleSuborganizationDataOwnerCode" type="RC:ResponsibleSuborganizationDataOwnerCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Indicates the agency that defines the suborganization identifier.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RefinerCode" type="RC:RefinerCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating that the handler is engaged in re-refining used oil activities.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SignedDate" type="RC:SignedDateDataType">
		<xsd:annotation>
			<xsd:documentation>Date on which the handler information was certified by the reporting site.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SpecificationMarketerCode" type="RC:SpecificationMarketerCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating that the handler is a marketer who first claims the used oil meets the specifications.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="StateActivityOwnerName" type="RC:StateActivityOwnerNameDataType">
		<xsd:annotation>
			<xsd:documentation>Indicates the agency that defines the state activity type.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="StateActivityTypeCode" type="RC:StateActivityTypeCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating the type of state activity.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="StateDistrictCode" type="RC:StateDistrictCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating the state-designated legislative district(s) in which the site is located.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="TelephoneNumberText" type="RC:TelephoneNumberTextDataType">
		<xsd:annotation>
			<xsd:documentation>Telephone Number data</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="TransactionCode" type="RC:TransactionCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Transaction code used to define the add, update, or delete.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="TransferFacilityCode" type="RC:TransferFacilityCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating that the handler owns or operates a used oil transfer facility.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="TransporterActivityCode" type="RC:TransporterActivityCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating that the handler is engaged in the transportation of hazardous waste.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="TransporterCode" type="RC:TransporterCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating that the handler is engaged in used oil transportation and/or transfer facility activities.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="TreatmentStorageDisposalActivityCode" type="RC:TreatmentStorageDisposalActivityCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating that the handler is engaged in the treatment, storage, or disposal of hazardous waste.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="UndergroundInjectionActivityCode" type="RC:UndergroundInjectionActivityCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating that the handler generates and or treats, stores, or disposes of hazardous waste and has an injection well located at the installation.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="UniversalWasteDestinationFacilityIndicator" type="RC:UniversalWasteDestinationFacilityIndicatorDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating that the handler treats, disposes of, or recycles hazardous waste on site.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="UniversalWasteOwnerName" type="RC:UniversalWasteOwnerNameDataType">
		<xsd:annotation>
			<xsd:documentation>Indicates the agency that defines the universal waste type.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="UniversalWasteTypeCode" type="RC:UniversalWasteTypeCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating the type of universal waste.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="WasteCode" type="RC:WasteCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code used to describe hazardous waste.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="WasteCodeOwnerName" type="RC:WasteCodeOwnerNameDataType">
		<xsd:annotation>
			<xsd:documentation>Indicates the agency that owns the data record.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="WasteGeneratorOwnerName" type="RC:WasteGeneratorOwnerNameDataType">
		<xsd:annotation>
			<xsd:documentation>Indicates the agency that defines the generator status type.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="WasteGeneratorStatusCode" type="RC:WasteGeneratorStatusCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating that the handler is engaged in the generation of hazardous waste.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- CME Elements-->
	<xsd:element name="Description" type="RC:DescriptionDataType">
		<xsd:annotation>
			<xsd:documentation>Description of the GM Form.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Name" type="RC:NameDataType">
		<xsd:annotation>
			<xsd:documentation>Name or number assigned by the implementing agency to identify a corrective action area.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ActualDate" type="RC:ActualDateDataType">
		<xsd:annotation>
			<xsd:documentation>Date on which actual completion of a corrective action event occurs.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EPAAttorneyDataOwnerCode" type="RC:EPAAttorneyDataOwnerCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Indicates the agency that defines the person identifier.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EPAAttorneyID" type="RC:EPAAttorneyIDDataType">
		<xsd:annotation>
			<xsd:documentation>Current attorney at the EPA level.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EPAResponsiblePersonDataOwnerCode" type="RC:EPAResponsiblePersonDataOwnerCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Indicates the agency that defines the person identifier.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EPAResponsiblePersonID" type="RC:EPAResponsiblePersonIDDataType">
		<xsd:annotation>
			<xsd:documentation>The person currently responsible for the permit at the EPA level.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="NewScheduledDate" type="RC:NewScheduledDateDataType">
		<xsd:annotation>
			<xsd:documentation>The new scheduled completion date for an event.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="OriginalScheduledDate" type="RC:OriginalScheduledDateDataType">
		<xsd:annotation>
			<xsd:documentation>The original scheduled completion date for an event.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ResponsibleSuborganizationID" type="RC:ResponsibleSuborganizationIDDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating the section of the organization accountable for overseeing the activities imposed by the authority.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="StateResponsiblePersonDataOwnerCode" type="RC:StateResponsiblePersonDataOwnerCodeDataType">
		<xsd:annotation>
			<xsd:documentation>Indicates the agency that defines the person identifier.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="StateResponsiblePersonID" type="RC:StateResponsiblePersonIDDataType">
		<xsd:annotation>
			<xsd:documentation>The state person currently responsible for overseeing the corrective action area.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ActualPaidAmount" type="RC:ActualPaidAmountDataType">
		<xsd:annotation>
			<xsd:documentation>The dollar amount of any cost recovery required to be paid pursuant to a Final Order.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ActualPaymentDate" type="RC:ActualPaymentDateDataType"/>
	<xsd:element name="ReturnComplianceActualDate" type="RC:ReturnComplianceActualDateDataType">
		<xsd:annotation>
			<xsd:documentation>The calendar date, determined by the Responsible Authority, on which the regulated entity actually returned to compliance with respect to the legal obligation that is the subject of the Violation Determined Date.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AgencyWhichDeterminedViolation" type="RC:AgencyWhichDeterminedViolationDataType"/>
	<xsd:element name="AppealResolutionDate" type="RC:AppealResolutionDateDataType"/>
	<xsd:element name="AppealInitiatedDate" type="RC:AppealInitiatedDateDataType"/>
	<xsd:element name="CitationName" type="RC:CitationNameDataType">
		<xsd:annotation>
			<xsd:documentation>The citation(s) of the violations alleged.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CitationNameOwner" type="RC:CitationNameOwnerDataType">
		<xsd:annotation>
			<xsd:documentation>State postal code</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CitationNameSequenceNumber" type="RC:CitationNameSequenceNumberDataType"/>
	<xsd:element name="CitationNameType" type="RC:CitationNameTypeDataType">
		<xsd:annotation>
			<xsd:documentation>Existing nationally defined values: FR, FS, OC, PC,SR,SS,V3</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CitizenComplaintIndicator" type="RC:CitizenComplaintIndicatorDataType">
		<xsd:annotation>
			<xsd:documentation>The inspection or record review was initiated because of a tip/complaint</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CommitmentActivityLocation" type="RC:CommitmentActivityLocationDataType"/>
	<xsd:element name="CommitmentEndDate" type="RC:CommitmentEndDateDataType"/>
	<xsd:element name="CommitmentLead" type="RC:CommitmentLeadDataType"/>
	<xsd:element name="CommitmentSequenceNumber" type="RC:CommitmentSequenceNumberDataType"/>
	<xsd:element name="CommitmentStartDate" type="RC:CommitmentStartDateDataType"/>
	<xsd:element name="CommitmentType" type="RC:CommitmentTypeDataType"/>
	<xsd:element name="CommitmentTypeOwner" type="RC:CommitmentTypeOwnerDataType"/>
	<xsd:element name="ConsentAgreementFinalOrderSequenceNumber" type="RC:ConsentAgreementFinalOrderSequenceNumberDataType"/>
	<xsd:element name="CorrectiveActionComponent" type="RC:CorrectiveActionComponentDataType"/>
	<xsd:element name="EnforcementActionDate" type="RC:EnforcementActionDateDataType">
		<xsd:annotation>
			<xsd:documentation>The calendar date the enforcement action was issued or filed.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DateOfRequest" type="RC:DateOfRequestDataType"/>
	<xsd:element name="DateResponseReceived" type="RC:DateResponseReceivedDataType"/>
	<xsd:element name="ViolationDeterminedDate" type="RC:ViolationDeterminedDateDataType">
		<xsd:annotation>
			<xsd:documentation>The calendar date the Responsible Authority determines that a regulated entity is in violation of a legally enforceable obligation.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DayZero" type="RC:DayZeroDataType">
		<xsd:annotation>
			<xsd:documentation>Date fo the Last Non-Followup Evaluation (Applies to SNY/SNN Evaluations Only)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DeterminedByAgency" type="RC:DeterminedByAgencyDataType"/>
	<xsd:element name="DispositionStatus" type="RC:DispositionStatusDataType"/>
	<xsd:element name="DispositionStatusDate" type="RC:DispositionStatusDateDataType"/>
	<xsd:element name="DispositionStatusOwner" type="RC:DispositionStatusOwnerDataType"/>
	<xsd:element name="EPAHandlerID" type="RC:EPAHandlerIDDataType">
		<xsd:annotation>
			<xsd:documentation>Number that uniquely identifies the EPA handler.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EnforcementAgencyLocationName" type="RC:EnforcementAgencyLocationNameDataType">
		<xsd:annotation>
			<xsd:documentation>The U.S.Postal Service alphabetic code that represents the U.S.State or territory in which a state or local government enforcement agency operates.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EnforcementAttorney" type="RC:EnforcementAttorneyDataType">
		<xsd:annotation>
			<xsd:documentation>Identifies the attorney within the agency responsible for issuing the enforcement action.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EnforcementDocketNumber" type="RC:EnforcementDocketNumberDataType">
		<xsd:annotation>
			<xsd:documentation>Notes the relevant docket number which enforcement staff have assigned for tracking of enforcement actions.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EnforcementHandlerID" type="RC:EnforcementHandlerIDDataType"/>
	<xsd:element name="EnforcementActionIdentifier" type="RC:EnforcementActionIdentifierDataType">
		<xsd:annotation>
			<xsd:documentation>The unique alphanumeric identifier used in the applicable database to identify a specific enforcement action pertaining to a regulated entity or facility.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EnforcementOwner" type="RC:EnforcementOwnerDataType">
		<xsd:annotation>
			<xsd:documentation>State Postal Code</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EnforcementAgencyName" type="RC:EnforcementAgencyNameDataType">
		<xsd:annotation>
			<xsd:documentation>The full name of the agency, department, or organization that submitted the enforcement action data to EPA.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EnforcementResponsiblePersonIdentifier" type="RC:ReponsiblePersonDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating the person within the agency responsible for conducting the enforcement.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EnforcementResponsiblePersonOwner" type="RC:EnforcementResponsiblePersonOwnerDataType">
		<xsd:annotation>
			<xsd:documentation>Indicates the agency that defines the person identifier.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EnforcementResponsibleSuborganization" type="RC:EnforcementResponsibleSuborganizationDataType"/>
	<xsd:element name="EnforcementResponsibleSuborganizationOwner" type="RC:EnforcementResponsibleSuborganizationOwnerDataType"/>
	<xsd:element name="EnforcementType" type="RC:EnforcementTypeDataType">
		<xsd:annotation>
			<xsd:documentation>A code that identifies the type of action being taken against a handler.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EvaluationActivityLocation" type="RC:EvaluationActivityLocationDataType">
		<xsd:annotation>
			<xsd:documentation>Indicates the location of the agency regulating the EPA handler.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EvaluationHandlerID" type="RC:EvaluationHandlerIDDataType"/>
	<xsd:element name="EvaluationIdentifier" type="RC:EvaluationIdentifierDataType">
		<xsd:annotation>
			<xsd:documentation>Name or number assigned by the implementing agency to identify an evaluation.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EvaluationResponsibleAgency" type="RC:EvaluationResponsibleAgencyDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating the agency responsible for conducting the evaluation.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EvaluationResponsiblePersonIdentifier" type="RC:EvaluationResponsiblePersonIdentifierDataType">
		<xsd:annotation>
			<xsd:documentation>Code indicating the person within the agency responsible for conducting the evaluation.</xsd:documentation>
