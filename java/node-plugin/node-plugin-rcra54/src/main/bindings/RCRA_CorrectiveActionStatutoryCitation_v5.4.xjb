<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<jaxb:bindings version="2.1" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:hj="http://hyperjaxb3.jvnet.org/ejb/schemas/customizations" xmlns:orm="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:inheritance="http://jaxb2-commons.dev.java.net/basic/inheritance" xmlns:annox="http://annox.dev.java.net"
    jaxb:extensionBindingPrefixes="hj orm inheritance" schemaLocation="../xsd/5/4/RCRA_CorrectiveActionStatutoryCitation_v5.4.xsd">

    <jaxb:bindings node="//xsd:complexType[@name='CorrectiveActionStatutoryCitationDataType']">
        <hj:entity>
            <orm:table name="RCRA_CA_STATUTORY_CITATION"/>
        </hj:entity>
        <hj:generated-id name="Dbid">
            <orm:column name="CA_STATUTORY_CITATION_ID"/>
        </hj:generated-id>
    </jaxb:bindings>

    <jaxb:bindings node="//xsd:element[@ref='RC:StatutoryCitationDataOwnerCode']">
        <hj:basic>
            <orm:column name="STATUTORY_CITTION_DTA_OWNR_CDE"/>
        </hj:basic>
    </jaxb:bindings>
    
    <jaxb:bindings node="//xsd:element[@ref='RC:StatutoryCitationDescription']">
        <hj:ignored/>
    </jaxb:bindings>
    
</jaxb:bindings>

