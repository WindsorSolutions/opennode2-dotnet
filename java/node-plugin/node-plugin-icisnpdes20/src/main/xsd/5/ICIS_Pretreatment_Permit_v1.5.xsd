<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema 
targetNamespace="http://www.exchangenetwork.net/schema/icis/1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1" xmlns:icis="http://www.exchangenetwork.net/schema/icis/1" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<!-- INCLUDE STATEMENTS -->
	<xsd:include schemaLocation="ICIS_Key_Elements_v1.5.xsd"/>
	<xsd:include schemaLocation="ICIS_Contact_v1.5.xsd"/>
	<!-- SCHEMA HEADER -->
	<xsd:annotation>
		<xsd:documentation>Schema Name: ICIS_Pretreatment_Permit_v1.5.xsd</xsd:documentation>
		<xsd:documentation>Current Version Available: http://www.exchangenetwork.net/schema/draft/icis/SchemasVersion1.5</xsd:documentation>
		<xsd:documentation>Description: ICIS-NPDES permit Pretreatment data input format</xsd:documentation>
		<xsd:documentation>Application: ICIS-NPDES</xsd:documentation>
		<xsd:documentation>Developed By: U.S. Environmental Protection Agency</xsd:documentation>
		<xsd:documentation>Point of Contact: EPA/Alison Kittle</xsd:documentation>
		<xsd:documentation>Data Written: July 2006</xsd:documentation>
		<xsd:documentation>Last Date Modified:3-7-2008</xsd:documentation>
	</xsd:annotation>
	<!-- GLOBAL ELEMENT DECLARATIONS -->
	<xsd:element name="ControlAuthorityNPDESIdentifier" type="icis:String9FixedType"/>
	<xsd:element name="ControlAuthorityRegionalAgencyCode" type="icis:StringMin1Max2Type"/>
	<xsd:element name="ControlAuthorityStateAgencyCode" type="icis:String2FixedType"/>
	<xsd:element name="PretreatmentContact" type="icis:PretreatmentContact"/>
	<xsd:element name="PretreatmentPermit" type="icis:PretreatmentPermit"/>
	<xsd:element name="PretreatmentProgramApprovedDate" type="icis:DateType"/>
	<xsd:element name="PretreatmentProgramRequiredIndicatorCode" type="icis:StringMin1Max3Type"/>
	<!-- COMPLEX TYPE DEFINITIONS -->
	<xsd:complexType name="PretreatmentPermitData">
		<xsd:sequence>
			<xsd:element ref="icis:TransactionHeader"/>
			<xsd:element ref="icis:PretreatmentPermit"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PretreatmentPermit">
		<xsd:complexContent>
			<xsd:extension base="icis:BasicPermitKeyElements">
				<xsd:sequence>
					<xsd:element ref="icis:PretreatmentProgramRequiredIndicatorCode" minOccurs="0"/>
					<xsd:element ref="icis:ControlAuthorityStateAgencyCode" minOccurs="0"/>
					<xsd:element ref="icis:ControlAuthorityRegionalAgencyCode" minOccurs="0"/>
					<xsd:element ref="icis:ControlAuthorityNPDESIdentifier" minOccurs="0"/>
					<xsd:element ref="icis:PretreatmentProgramApprovedDate" minOccurs="0"/>
					<xsd:element ref="icis:PretreatmentContact" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="PretreatmentContact">
		<xsd:sequence>
			<xsd:element ref="icis:Contact" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
