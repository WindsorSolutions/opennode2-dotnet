<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema 
targetNamespace="http://www.exchangenetwork.net/schema/icis/1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1" xmlns:icis="http://www.exchangenetwork.net/schema/icis/1" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<!-- INCLUDE STATEMENTS -->
	<xsd:include schemaLocation="ICIS_Key_Elements_v1.5.xsd"/>
	<xsd:include schemaLocation="ICIS_Permit_v1.5.xsd"/>
	<!-- SCHEMA HEADER -->
	<xsd:annotation>
		<xsd:documentation>Schema Name: ICIS_Master_General_Permit_v1.5.xsd</xsd:documentation>
		<xsd:documentation>Current Version Available: http://www.exchangenetwork.net/schema/draft/icis/SchemasVersion1.5</xsd:documentation>
		<xsd:documentation>Description: ICIS-NPDES master general permit data input format</xsd:documentation>
		<xsd:documentation>Application: ICIS-NPDES</xsd:documentation>
		<xsd:documentation>Developed By: U.S. Environmental Protection Agency</xsd:documentation>
		<xsd:documentation>Point of Contact: EPA/Alison Kittle</xsd:documentation>
		<xsd:documentation>Data Written: July 2006</xsd:documentation>
		<xsd:documentation>Last Date Modified:3-7-2008</xsd:documentation>
	</xsd:annotation>
	<!-- GLOBAL ELEMENT DECLARATIONS -->
	<xsd:element name="GeneralPermitIndustrialCategory" type="icis:StringMin1Max3Type"/>
	<xsd:element name="MasterGeneralPermit" type="icis:MasterGeneralPermit"/>
	<xsd:element name="MasterGeneralPermitContact" type="icis:MasterGeneralPermitContact"/>
	<xsd:element name="PermitComponentTypeCode" type="icis:StringMin1Max3Type"/>
	<xsd:element name="PermitName" type="icis:StringMin1Max120Type"/>
	<!-- COMPLEX TYPE DEFINITIONS -->
	<xsd:complexType name="MasterGeneralPermitData">
		<xsd:sequence>
			<xsd:element ref="icis:TransactionHeader"/>
			<xsd:element ref="icis:MasterGeneralPermit"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="MasterGeneralPermit">
		<xsd:complexContent>
			<xsd:extension base="icis:BasicPermitKeyElements">
				<xsd:sequence>
					<xsd:group ref="icis:PermitDataGroup" minOccurs="0"/>
					<xsd:element ref="icis:MasterGeneralPermitContact" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="icis:GeneralPermitIndustrialCategory" minOccurs="0"/>
					<xsd:element ref="icis:PermitName" minOccurs="0"/>
					<xsd:element ref="icis:PermitComponentTypeCode" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="MasterGeneralPermitContact">
		<xsd:sequence>
			<xsd:element ref="icis:Contact"/>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
