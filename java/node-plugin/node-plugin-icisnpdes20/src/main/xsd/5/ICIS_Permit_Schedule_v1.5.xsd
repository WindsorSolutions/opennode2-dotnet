<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema 
targetNamespace="http://www.exchangenetwork.net/schema/icis/1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1" xmlns:icis="http://www.exchangenetwork.net/schema/icis/1" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<!-- INCLUDE STATEMENTS -->
	<xsd:include schemaLocation="ICIS_Key_Elements_v1.5.xsd"/>
	<xsd:include schemaLocation="ICIS_Schedule_Event_v1.5.xsd"/>
	<!--xsd:include schemaLocation="ICIS_Stays_v1.5.xsd"/-->
	<!-- SCHEMA HEADER -->
	<xsd:annotation>
		<xsd:documentation>Schema Name: ICIS_Permit_Schedule_v1.5.xsd</xsd:documentation>
		<xsd:documentation>Current Version Available: http://www.exchangenetwork.net/schema/draft/icis/SchemasVersion1.5</xsd:documentation>
		<xsd:documentation>Description: ICIS-NPDES permit schedule data input format</xsd:documentation>
		<xsd:documentation>Application: ICIS-NPDES</xsd:documentation>
		<xsd:documentation>Developed By: U.S. Environmental Protection Agency</xsd:documentation>
		<xsd:documentation>Point of Contact: EPA/Alison Kittle</xsd:documentation>
		<xsd:documentation>Data Written: July 2006</xsd:documentation>
		<xsd:documentation>Last Date Modified:3-7-2008</xsd:documentation>
	</xsd:annotation>
	<!-- GLOBAL ELEMENT DECLARATIONS -->
	<xsd:element name="PermitSchedule" type="icis:PermitSchedule"/>
	<xsd:element name="PermitScheduleEvent" type="icis:PermitScheduleEvent"/>
	<xsd:element name="PermitScheduleComments" type="icis:StringMin1Max4000Type"/>
	<!-- COMPLEX TYPE DEFINITIONS -->
	<xsd:complexType name="PermitScheduleData">
		<xsd:sequence>
			<xsd:element ref="icis:TransactionHeader"/>
			<xsd:element ref="icis:PermitSchedule"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PermitSchedule">
		<xsd:complexContent>
			<xsd:extension base="icis:PermitScheduleKeyElements">
				<xsd:sequence>
					<xsd:element ref="icis:NarrativeConditionCode" minOccurs="0"/>
					<xsd:element ref="icis:PermitScheduleComments" minOccurs="0"/>
					<xsd:element ref="icis:PermitScheduleEvent" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="PermitScheduleEvent">
		<xsd:complexContent>
			<xsd:extension base="icis:PermitScheduleEventKeyElements">
				<xsd:sequence>
					<xsd:group ref="icis:ScheduleEventGroup" minOccurs="0"/>
					<!--xsd:group ref="icis:StayDataGroup"/-->
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
</xsd:schema>
