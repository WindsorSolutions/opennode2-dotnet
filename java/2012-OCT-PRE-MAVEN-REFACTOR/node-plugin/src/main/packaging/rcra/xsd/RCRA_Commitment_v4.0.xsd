<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:RC="http://www.exchangenetwork.net/schema/RCRA/4" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.exchangenetwork.net/schema/RCRA/4" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="4.0">
	<xsd:include schemaLocation="./RCRA_Shared_v4.0.xsd"/>
	<xsd:annotation>
		<xsd:documentation>
		Schema Name : 	RCRA_Commitment_v4.0
		Description : 	Compliance Monitoring and Enforcement Commitment/Initiative Data.
		Developed by : 	 Windsor Solutions, Inc.
	</xsd:documentation>
	</xsd:annotation>
	<xsd:element name="Commitment" type="RC:CommitmentDataType">
		<xsd:annotation>
			<xsd:documentation>Compliance Monitoring and Enforcement Commitment/Initiative Data.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="CommitmentDataType">
		<xsd:sequence>
			<xsd:element ref="RC:TransactionCode" minOccurs="0"/>
			<xsd:element ref="RC:CommitmentLead"/>
			<xsd:element ref="RC:CommitmentSequenceNumber"/>
			<xsd:element ref="RC:EPAHandlerID"/>
			<xsd:element ref="RC:CommitmentTypeOwner" minOccurs="0"/>
			<xsd:element ref="RC:CommitmentType" minOccurs="0"/>
			<xsd:element ref="RC:FederalFiscalYear" minOccurs="0"/>
			<xsd:element ref="RC:CommitmentStartDate" minOccurs="0"/>
			<xsd:element ref="RC:CommitmentEndDate" minOccurs="0"/>
			<xsd:element ref="RC:CommitmentActivityLocation" minOccurs="0"/>
			<xsd:element ref="RC:Notes" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
