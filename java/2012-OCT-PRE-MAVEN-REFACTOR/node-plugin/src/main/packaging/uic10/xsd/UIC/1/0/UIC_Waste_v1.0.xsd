<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:uic="http://www.exchangenetwork.net/schema/uic/1" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.exchangenetwork.net/schema/uic/1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<xsd:include schemaLocation="UIC_SimpleContent_v1.0.xsd"/>
	<xsd:annotation>
		<xsd:documentation>
        Schema Name  : UIC_Waste_v1.0.xsd
        Description   : the Resource Conservation and Recovery Act (RCRA) waste code and generalized waste source.
        Developed by  : 	 Lockheed Martin
        Point of Contact :   Lockheed Martin
</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="WasteDetailType">
		<xsd:annotation>
			<xsd:documentation>Defines the Waste information structure.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="uic:WasteIdentifier"/>
			<xsd:element ref="uic:WasteCode" minOccurs="0"/>
			<xsd:element ref="uic:WasteStreamClassificationCode" minOccurs="0"/>
			<xsd:element ref="uic:WasteWellIdentifier"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="WasteDetail" type="uic:WasteDetailType"/>
</xsd:schema>
