<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
                http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<bean name="/schedule.htm"
		class="com.windsor.node.admin.schedule.ScheduleController">
		<property name="scheduleService" ref="scheduleService" />
	</bean>

	<bean name="/schedule-edit.htm"
		class="com.windsor.node.admin.schedule.EditScheduleController">
		<property name="flowService" ref="flowService" />
		<property name="scheduleService" ref="scheduleService" />
		<property name="partnerService" ref="partnerService" />
		<property name="accountService" ref="accountService" />
		<property name="validator">
			<bean
				class="com.windsor.node.admin.valid.ScheduleValidator" />
		</property>
		<property name="successView" value="schedule.htm?bi=0" />
	</bean>
    
    <bean name="/service-list.htm"
       class="com.windsor.node.admin.schedule.ServicesByFlowController">
       <property name="flowService" ref="flowService" />
    </bean>

</beans>